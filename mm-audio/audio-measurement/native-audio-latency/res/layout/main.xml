<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<LinearLayout
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    >
    <EditText
         android:layout_width="80px"
         android:layout_height="50px"
         android:layout_alignParentLeft="true"
         android:padding="5dip"
         android:id="@+id/et1"
         />
    <TextView
         android:layout_width = "wrap_content"
         android:layout_height = "70px"
         android:text="@string/DSPLatency"
         />
</LinearLayout>
<Button
    android:id="@+id/cold_output"
    android:text="@string/cold_output"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    />
<Button
    android:id="@+id/warm_output"
    android:text="@string/warm_output"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    />
<Button
    android:id="@+id/continuous_output"
    android:text="@string/continuous_output"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    />
<Button
    android:id="@+id/cold_input"
    android:text="@string/cold_input"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    />
<Button
    android:id="@+id/continuous_input"
    android:text="@string/continuous_input"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    />
<LinearLayout
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    >
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ShowLatencies"
        android:id="@+id/show"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ClearLatencies"
        android:id="@+id/clear"
        android:layout_alignBottom="@id/show"
        android:layout_marginLeft ="80px"
        />
</LinearLayout>
<TextView
    android:id="@+id/tv1"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:hint="Cold Output Latency"
    android:textSize="16dip"
    />
<TextView
    android:id="@+id/tv2"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:hint="Warm Output Latency"
    android:textSize="16dip"
    />
<TextView
    android:id="@+id/tv3"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:hint="Continuous Output Latency"
    android:textSize="16dip"
    />
<TextView
    android:id="@+id/tv4"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:hint="Cold Input Latency"
    android:textSize="16dip"
    />
<TextView
    android:id="@+id/tv5"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:hint="Continuous Input Latency"
    android:textSize="16dip"
    />
</LinearLayout>
