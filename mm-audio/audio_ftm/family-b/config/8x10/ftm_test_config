tc 1
#Handset Capture
!Capture
Txdevice:0

enable
MultiMedia1 Mixer PRI_MI2S_TX:1
DEC1 MUX:ADC1

disable
MultiMedia1 Mixer PRI_MI2S_TX:0
DEC1 MUX:ZERO

tc 2
#Ear Piece
!Playback
Rxdevice:0

enable
RX1 MIX1 INP1:RX1
RX1 Digital Volume:67
DAC1 Switch:1
SEC_MI2S_RX Audio Mixer MultiMedia1:1

disable
SEC_MI2S_RX Audio Mixer MultiMedia1:0
RX1 MIX1 INP1:ZERO
DAC1 Switch:0

