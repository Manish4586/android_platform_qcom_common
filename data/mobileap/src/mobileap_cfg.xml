<?xml version="1.0" encoding="UTF-8"?>
<system xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="mobileap_cfg.xsd">
	<MobileAPCfg>
		<MobileAPNatCfg>
			<IoEMode>1</IoEMode>
			<TetheringOnly>0</TetheringOnly>
			<NATType>SYM</NATType>
			<FirewallEnabled>1</FirewallEnabled>
			<FirewallPktsAllowed>0</FirewallPktsAllowed>
			<Firewall>/etc/mobileap_firewall.xml</Firewall>
			<PortFwding>
				<PortFwdingPrivateIP>192.168.225.8</PortFwdingPrivateIP>
				<PortFwdingPrivatePort>1500</PortFwdingPrivatePort>
				<PortFwdingGlobalPort>1500</PortFwdingGlobalPort>
				<PortFwdingProtocol>17</PortFwdingProtocol>
			</PortFwding>
			<NatEntryTimeout>200</NatEntryTimeout>
			<DmzIP>0.0.0.0</DmzIP>
			<EnableIPSECVpnPassthrough>1</EnableIPSECVpnPassthrough>
			<EnablePPTPVpnPassthrough>1</EnablePPTPVpnPassthrough>
			<EnableL2TPVpnPassthrough>1</EnableL2TPVpnPassthrough>
		</MobileAPNatCfg>
		<MobileAPLanCfg>
			<Module>ar6000</Module>
			<DevMode>AP</DevMode>
			<HostAPDCfg>/etc/hostapd.conf</HostAPDCfg>
			<APIPAddr>192.168.225.1</APIPAddr>
			<SubNetMask>255.255.255.0</SubNetMask>
			<AccessProfile>FULL</AccessProfile> 
			<EnableDHCPServer>1</EnableDHCPServer>
			<DHCPCfg>
				<StartIP>192.168.225.20</StartIP>
				<EndIP>192.168.225.40</EndIP>
				<LeaseTime>12h</LeaseTime>
			</DHCPCfg> 
			<SSID2Cfg>
				<Enable>0</Enable>
				<DevMode>AP</DevMode>
				<HostAPDCfg>/etc/hostapd-eth1.conf</HostAPDCfg>
				<APIPAddr>192.168.2.1</APIPAddr>
				<SubNetMask>255.255.255.0</SubNetMask>
				<AccessProfile>INTERNETONLY</AccessProfile>
				<EnableDHCPServer>1</EnableDHCPServer>
				<DHCPCfg>
					<StartIP>192.168.2.20</StartIP>
					<EndIP>192.168.2.40</EndIP>
					<LeaseTime>12h</LeaseTime>
				</DHCPCfg> 
				<EnableSupplicant>0</EnableSupplicant>
				<SupplicantCfg>NA</SupplicantCfg>
				<ExternalAPSSID>NA</ExternalAPSSID>
				<STAModeConnType>NA</STAModeConnType>
				<StaticIPAddr>NA</StaticIPAddr>
				<StaticConfigDNSAddr>NA</StaticConfigDNSAddr>
				<StaticConfigGWAddr>NA</StaticConfigGWAddr>
				<StaticConfigNetMask>NA</StaticConfigNetMask>
			</SSID2Cfg> 
			<SSID3Cfg>
				<Enable>0</Enable>
				<DevMode>STA</DevMode>
				<HostAPDCfg>NA</HostAPDCfg>
				<APIPAddr>NA</APIPAddr>
				<SubNetMask>NA</SubNetMask>
				<AccessProfile>NA</AccessProfile>
				<EnableDHCPServer>0</EnableDHCPServer>
				<DHCPCfg>
					<StartIP>NA</StartIP>
					<EndIP>NA</EndIP>
					<LeaseTime>NA</LeaseTime>
				</DHCPCfg> 
				<EnableSupplicant>1</EnableSupplicant>
				<SupplicantCfg>/etc/wpa_supplicant.conf</SupplicantCfg>
				<ExternalAPSSID>dlink-ap</ExternalAPSSID>
				<STAModeConnType>0</STAModeConnType>
				<StaticIPAddr>0</StaticIPAddr>
				<StaticConfigDNSAddr>0</StaticConfigDNSAddr>
				<StaticConfigGWAddr>0</StaticConfigGWAddr>
				<StaticConfigNetMask>0</StaticConfigNetMask>
			</SSID3Cfg>
		</MobileAPLanCfg>
		<MobileAPWanCfg>
			<AutoConnect>0</AutoConnect>
			<Roaming>0</Roaming>
			<EriConfig>/etc/mobileap_eri_config.bin</EriConfig>
			<TECH>ANY</TECH>
			<V4_UMTS_PROFILE_INDEX>1</V4_UMTS_PROFILE_INDEX>
			<V4_CDMA_PROFILE_INDEX>0</V4_CDMA_PROFILE_INDEX>
			<V6_UMTS_PROFILE_INDEX>0</V6_UMTS_PROFILE_INDEX>
			<V6_CDMA_PROFILE_INDEX>0</V6_CDMA_PROFILE_INDEX>
			<IPFamily>IPv4</IPFamily>
		</MobileAPWanCfg>
	</MobileAPCfg>
</system>	
