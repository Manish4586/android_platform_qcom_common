<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#FAFAFA" />

            <stroke android:width="1dp" android:color="#A4A4A4" />

            <corners android:radius="3dp" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape></item>
    <item android:state_enabled="false"><shape>
            <gradient android:angle="270" android:endColor="#848484" android:startColor="#D8D8D8" />

            <stroke android:width="1dp" android:color="#848484" />

            <corners android:radius="4dp" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape></item>
    <item android:state_enabled="true"><shape>
            <gradient android:angle="270" android:endColor="#A4A4A4" android:startColor="#FAFAFA" />

            <stroke android:width="1dp" android:color="#A4A4A4" />

            <corners android:radius="4dp" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape></item>

</selector>
