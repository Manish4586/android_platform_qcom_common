<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The default Auto silence value -->
    <string name="default_auto_silence_value">1</string>

    <!-- The default snooze duration value -->
    <string name="default_snooze_duration_value">10</string>

    <!-- Entries listed in the ListPreference when invoking the auto silence
         preference. -->
    <string-array name="auto_silence_entries">
      <item>"1 мин."</item>
      <item>"5 мин."</item>
      <item>"10 мин."</item>
      <item>"15 мин."</item>
      <item>"20 мин."</item>
      <item>"25 мин."</item>
      <item>"30 мин."</item>
      <item>"Никогда"</item>
      </string-array>

    <!-- Values that are retrieved from the ListPreference. These must match
         the auto_silence_entries above. -->
    <string-array name="auto_silence_values" translatable="false">
      <item>1</item>
      <item>5</item>
      <item>10</item>
      <item>15</item>
      <item>20</item>
      <item>25</item>
      <item>30</item>
      <item>-1</item><!-- Off -->
      </string-array>

</resources>
