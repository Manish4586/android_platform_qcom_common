    <string-array name="switch_set">
        <item><xliff:g id="SWITCH">%1$s</xliff:g> dans moins d\'une minute.</item>
        <item><xliff:g id="SWITCH">%1$s</xliff:g> dans <xliff:g id="DAYS" example="2 days">%2$s</xliff:g>.</item>
        <item><xliff:g id="SWITCH">%1$s</xliff:g> dans <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g>.</item>
