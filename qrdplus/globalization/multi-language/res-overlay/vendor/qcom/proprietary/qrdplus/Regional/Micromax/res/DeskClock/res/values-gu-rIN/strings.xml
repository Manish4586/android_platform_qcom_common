      <item>1 મિનિટ</item>
      <item>5 મિનિટ</item>
      <item>10 મિનિટ</item>
      <item>15 મિનિટ</item>
      <item>20 મિનિટ</item>
      <item>25 મિનિટ</item>
      <item>30 મિનિટ</item>
      <item>ક્યારેય નહીં</item>
      </string-array>

    <!-- Values that are retrieved from the ListPreference. These must match
         the auto_silence_entries above. -->
    <string-array name="auto_silence_values" translatable="false">
      <item>1</item>
      <item>5</item>
      <item>10</item>
      <item>15</item>
      <item>20</item>
      <item>25</item>
      <item>30</item>
