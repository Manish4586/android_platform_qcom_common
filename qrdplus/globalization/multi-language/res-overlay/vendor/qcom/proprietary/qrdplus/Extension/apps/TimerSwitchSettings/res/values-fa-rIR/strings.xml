    <string-array name="switch_set">
        <item>‫<xliff:g id="SWITCH">%1$s</xliff:g> برای کمتر از ۱ دقیقه دیگر تنظیم شد.‬</item>
        <item>‫<xliff:g id="SWITCH">%1$s</xliff:g> برای <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> دیگر تنظیم شد.‬</item>
        <item>‫<xliff:g id="SWITCH">%1$s</xliff:g> برای <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> دیگر تنظیم شد.‬</item>
        <item>‫<xliff:g id="SWITCH">%1$s</xliff:g> برای <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> و <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> دیگر تنظیم شد.‬</item>
        <item>‫<xliff:g id="SWITCH">%1$s</xliff:g> برای <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> دیگر تنظیم شد.‬</item>
        <item>‫<xliff:g id="SWITCH">%1$s</xliff:g> برای <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> و <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> دیگر تنظیم شد.‬</item>
        <item>‫<xliff:g id="SWITCH">%1$s</xliff:g> برای <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> و <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> دیگر تنظیم شد.‬</item>
        <item>‫<xliff:g id="SWITCH">%1$s</xliff:g> برای <xliff:g id="DAYS" example="2 days">%2$s</xliff:g>،‫ <xliff:g id="HOURS" example="2 hours">%3$s</xliff:g> و <xliff:g id="MINUTES" example="2 minutes">%4$s</xliff:g> دیگر تنظیم شد.‬</item>
