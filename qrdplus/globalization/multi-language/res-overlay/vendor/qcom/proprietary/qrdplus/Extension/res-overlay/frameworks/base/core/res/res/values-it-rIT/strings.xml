<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="permlab_accessWifiState">Visualizza connessioni WLAN</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessWifiState">Consente all\'app di visualizzare informazioni
sulle reti WLAN, ad esempio se il WLAN è attivato e il nome dei
dispositivi WLAN connessi.</string>

    <string name="permlab_changeWifiState">Connessione e disconnessione dalla WLAN</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changeWifiState">Consente all\'app di connettersi/disconnettersi da
punti di accesso WLAN e apportare modifiche alla configurazione del dispositivo
per le reti WLAN.</string>
    <string name="permlab_changeWifiMulticastState">Consenti ricezione multicast WLAN</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changeWifiMulticastState" product="tablet">Consente all\'app di ricevere
pacchetti inviati a tutti i dispositivi su una rete WLAN utilizzando indirizzi multicast,
non solo il tablet. Viene consumata più energia rispetto alla modalità non multicast.</string>
    <string name="permdesc_changeWifiMulticastState" product="default">Consente all\'app di ricevere
pacchetti inviati a tutti i dispositivi su una rete WLAN utilizzando indirizzi multicast,
non solo il telefono. Viene consumata più energia rispetto alla modalità non multicast.</string>

    <string name="wifi_available_sign_in">Accedi a rete WLAN</string>
    <string name="wifi_watchdog_network_disabled">Impossibile connettersi alla rete WLAN</string>
    <string name="wifi_p2p_dialog_title">WLAN Direct</string>
    <string name="wifi_p2p_turnon_message">Avvia WLAN Direct. Verrà disattivato il client/hotspot WLAN.</string>
    <string name="wifi_p2p_failed_message">Avvio di WLAN Direct non riuscito.</string>
    <string name="wifi_p2p_enabled_notification_title">WLAN Direct è attivo</string>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">Il tablet verrà momentaneamente disconnesso dalla rete WLAN durante la connessione a <xliff:g id="device_name">%1$s</xliff:g></string>
    <string name="wifi_p2p_frequency_conflict_message" product="default">Il telefono verrà momentaneamente disconnesso dalla rete WLAN durante la connessione a <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Notification title when WLAN data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
    <string name="data_usage_wifi_limit_title">Dati WLAN disattivati</string>
    <!-- Notification title when WLAN data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <string name="data_usage_wifi_limit_snoozed_title">Limite dati WLAN superato</string>

    <string name="permlab_configureWifiDisplay">Configura schermi WLAN</string>
    <string name="permdesc_configureWifiDisplay">Consente all\'app di configurare schermi WLAN e connettersi ad essi.</string>
    <string name="permlab_controlWifiDisplay">Controlla schermi WLAN</string>
    <string name="permdesc_controlWifiDisplay">Consente all\'app di controllare le funzioni di basso livello degli schermi WLAN.</string>

