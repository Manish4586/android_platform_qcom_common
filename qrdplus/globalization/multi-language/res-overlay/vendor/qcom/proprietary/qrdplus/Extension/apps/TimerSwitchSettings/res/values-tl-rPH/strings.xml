    <string-array name="switch_set">
        <item><xliff:g id="SWITCH">%1$s</xliff:g> nakatakda ng wala pang 1 minuto mula ngayon.</item>
        <item><xliff:g id="SWITCH">%1$s</xliff:g> nakatakda ng <xliff:g id="DAYS" example="2 days">%2$s</xliff:g> mula ngayon.</item>
