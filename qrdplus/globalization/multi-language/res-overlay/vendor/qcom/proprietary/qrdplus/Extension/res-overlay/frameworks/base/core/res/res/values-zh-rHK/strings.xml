    <!-- Notification title when WLAN data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <string name="data_usage_wifi_limit_snoozed_title">已超出 WLAN 數據限制</string>

    <string name="permlab_configureWifiDisplay">設定 WLAN 顯示</string>
    <string name="permdesc_configureWifiDisplay">允許應用程式設定和連接 WLAN 顯示。</string>
    <string name="permlab_controlWifiDisplay">控制 WLAN 顯示</string>
    <string name="permdesc_controlWifiDisplay">允許應用程式控制 WLAN 顯示的低級功能。</string>

