<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- alert to the user that USB storage must be available before using FM recording  [CHAR LIMIT=NONE] -->
   <string name="no_storage">રેકોર્ડિંગ પ્રારંભ કરતા પહેલા SD કાર્ડ માઉન્ટ કરો.</string>
   <!-- alert to the user that the USB storage is being disk-checked [CHAR LIMIT=30] -->
   <string name="preparing_sd">SD કાર્ડ તૈયાર કરી રહ્યું છે</string>
   <!-- alert to the user that the FM fails to read or write the USB storage. [CHAR LIMIT=NONE] -->
   <string name="access_sd_fail">SD કાર્ડને ઍક્સેસ કરી શકાયું નથી.</string>
   <!-- Low-memory dialog message [CHAR LIMT=NONE] -->
   <string name="spaceIsLow_content">તમારા SD કાર્ડ સંગ્રહમાં જગ્યા નથી. ગુણવત્તા સેટિંગ્સ બદલો અથવા કેટલીક છબીઓ અથવા અન્ય ફાઇલો કાઢી નાખો.</string>
   <!-- The messsage shown when FM record reaches size limit. -->
   <string name="FMRecording_reach_size_limit">કદ સીમા પર પહોંચ્યા.</string>
    <!-- the name under which recordings will be visible in the media database is formatted like this -->
    <string name="audio_db_title_format"><xliff:g id="format">વવવવ-મમ-દિદિ કક:મિમિ:સેસે</xliff:g></string>
    <!-- all recordings will show up in the media database with this \'artist\' name -->
    <string name="audio_db_artist_name">મારી FM રેકોર્ડિંગ્સ</string>
    <!-- all recordings will show up in the media database with this \'album\' name -->
    <string name="audio_db_album_name">FM રેકોર્ડિંગ્સ</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <string name="audio_db_playlist_name">FM રેકોર્ડિંગ્સ</string>
    <string name="save_record_file">FM રેકોર્ડ કરેલી ફાઇલને "<xliff:g id="record_file">%1$s</xliff:g>" પર સાચવી</string>
    <string name="fm_record_progress">FM રેકોર્ડિંગ્સ પ્રક્રિયામાં છે</string>
    <string name="unable_to_store">રેકોર્ડ કરેલો ઑડિયો સાચવવામાં અસમર્થ</string>
</resources>
