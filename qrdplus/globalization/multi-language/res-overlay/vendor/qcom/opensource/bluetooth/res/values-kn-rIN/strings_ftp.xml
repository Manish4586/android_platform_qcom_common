<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="ftp_acceptance_dialog_title">%1$s ನಿಮ್ಮ ಫೋನ್ ಫೋಲ್ಡರ್‌ಗಳನ್ನು ಪ್ರವೇಶಿಸಲು ಬಯಸುತ್ತದೆ. %2$s ಗೆ ಪ್ರವೇಶ ನೀಡುವುದೇ?</string>
    <string name="ftp_acceptance_dialog_header">ಫೋನ್ ಫೋಲ್ಡರ್ ಪ್ರವೇಶ</string>
    <string name="ftp_session_key_dialog_title">%1$s ಗಾಗಿ ಸೆಷನ್ ಕೀ ಟೈಪ್ ಮಾಡಿ</string>
    <string name="ftp_session_key_dialog_header">ಬ್ಲೂಟೂತ್ ಸೆಷನ್ ಕೀ ಅಗತ್ಯವಿದೆ</string>
    <string name="ftp_acceptance_timeout_message">%1$s ರೊಂದಿಗೆ ಸಂಪರ್ಕ ಒಪ್ಪಲು ಸಮಯ ಮೀರಿದೆ</string>
    <string name="ftp_authentication_timeout_message">%1$s ರೊಂದಿಗೆ ಸೆಷನ್ ಕೀ ಇನ್‌ಪುಟ್ ಮಾಡಲು ಸಮಯ ಮೀರಿದೆ</string>
    <string name="ftp_notif_ticker">ಬ್ಲೂಟೂತ್ ಸಂಪರ್ಕ ವಿನಂತಿ</string>
    <!-- Notification title when a Bluetooth device wants to pair with us -->
    <string name="ftp_notif_title">FTP ಗಾಗಿ ಸೆಷನ್ ಕೀ ವಿನಂತಿ</string>
    <!-- Notification message when a Bluetooth device wants to pair with us -->
    <string name="ftp_notif_message">"\u0022<xliff:g id="device_name">%1$s</xliff:g>\u0022 ಗೆ ಸಂಪರ್ಕಿಸಲು ಸ್ಪರ್ಶಿಸಿ."</string>
    <!-- Activity label of BluetoothFtpPermissionActivity, also used as Strings in the permission dialog [CHAR LIMIT=none] -->
    <!--Notification Title for ACTIVE SESSION -->
    <string name="ftp_notif_active_session">Bluetooth FTP ಸಕ್ರಿಯ ಸೆಷನ್ </string>
    <!--Notification message for ACTIVE SESSION -->
    <string name="ftp_notif_connected">\u0022<xliff:g id="device_name">%1$s</xliff:g>\u0022ಗೆ "Bluetooth ಫೈಲ್ ವರ್ಗಾವಣೆ ಪ್ರವೇಶ."</string>

    <string name="bluetooth_ftp_request">"Ftp ವಿನಂತಿ"</string>

    <!-- Bluetooth FTP permission Alert Activity text [CHAR LIMIT=none] -->
    <string name="bluetooth_ftp_acceptance_dialog_text">%1$s ನಿಮ್ಮ ಫೈಲ್‌ಗಳು ಮತ್ತು ಫೋಲ್ಡರ್‌ಗಳನ್ನು ಪ್ರವೇಶಿಸಲು ಬಯಸುತ್ತದೆ. %2$s ಗೆ ಪ್ರವೇಶ ನೀಡುವುದೇ?</string>

