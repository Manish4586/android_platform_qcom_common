    <string name="test_account_3_short_description">Спілкуйтеся з користувачами мережі Chat</string>

    <!-- DO NOT TRANSLATE. Hardcoded number used for restricted incoming phone numbers. -->
    <string name="handle_restricted">ОБМЕЖЕНО</string>

    <string name="headset_plugin_view_text">Ввімкнено гарнітуру.</string>
    <string name="headset_plugin_view_title">Гарнітура</string>
