    <string name="no_vm_number">হারানো ভয়েসমেইল নম্বর দিয়ে</string>
    <!-- Body text of the "Missing voicemail number" dialog -->
    <string name="no_vm_number_msg">SIM কার্ডে কোন ভয়েসমেইল নম্বর সংরক্ষিত নেই।</string>
    <!-- Button label on the "Missing voicemail number" dialog -->
    <string name="add_vm_number_str">নম্বর যোগ করুন</string>

