    <string name="test_account_0_short_description">Q Mobile সহ অ্যাকাউন্ট</string>
    <!-- DO NOT TRANSLATE. Short description for test Subscription 1. -->
    <string name="test_account_1_short_description">Market Wireless সহ অ্যাকাউন্ট</string>
    <!-- DO NOT TRANSLATE. Short description for test Subscription 2. -->
    <string name="test_account_2_short_description">আপনার সার্কেলগুলিতে থাকা প্রত্যেকের সাথে কথা বলুন!</string>
