    <string name="call_notif_conditionalCF">सशर्त कॉल फ़ारवर्डिंग सक्रिय</string>
    <!-- Toast text for a forwarded MO call  -->
    <string name="call_notif_MOcall_forwarding">जावक कॉल अग्रेषित की गई</string>
    <!-- Toast text for MO call, when the caller & callee both belng to a CUG group -->
    <string name="call_notif_cugcall">बंद उपयोगकर्ता समूह में कॉल करें</string>
    <!-- In-call screen: status label when MO calls are barred -->
    <string name="call_notif_outgoing_barred">जावक कॉल्स पर रोक लगाई गई है</string>
    <!-- Toast text for MO calls, when incoming calls are barred at remote party -->
    <string name="call_notif_incoming_barred">दूरस्थ पार्टी पर आवक कॉल्स पर रोक लगाई गई है</string>
    <!-- Toast text for MO calls when CLIR Suppression is rejected -->
    <string name="call_notif_clir_suppression_rejected">स्वयं के नंबर का अवरोधन नेटवर्क के द्वारा अस्वीकार कर दिया गया है</string>
    <!-- In-call screen: status label when MO call gets deflected to another party -->
    <string name="call_notif_call_deflected">दूरस्थ पार्टी के द्वारा कॉल अग्रेषित किया गया है</string>
    <!-- Toast text for incoming call being forwarded to other remote party  -->
    <string name="call_notif_MTcall_forwarding">आवक कॉल अग्रेषित किया गया है</string>
    <!-- Toast text for incoming call which is part of MultiParty Call -->
    <string name="call_notif_multipartycall">बहु-पक्षीय कॉल</string>
    <!-- Toast text for incoming call which is answered,put on hold & then released -->
    <string name="call_notif_callonhold_released">कॉल दूरस्थ रुप से जारी किया गया है</string>
    <!-- Toast text for incoming call forward check received -->
    <string name="call_notif_forwardcheckreceived">कॉल अग्रेषित किया गया था</string>
    <!-- Toast text for incoming call connecting through ECT -->
    <string name="call_notif_callconnectingect">स्पष्ट कॉल स्थानांतरण के माध्यम से कॉल कनेक्ट करना</string>
    <!-- Toast text for incoming call connected through ECT -->
    <string name="call_notif_callconnectedect">स्पष्ट कॉल स्थानांतरण के माध्यम से कॉल कनेक्ट किया गया</string>
</resources>
