    <!-- Advanced Settings -->
    <string name="advanced_setting">ಸುಧಾರಿತ ಸೆಟ್ಟಿಂಗ್‌ಗಳು</string>
    <string name="show_ssn_label">ಈವೆಂಟ್ ಅಧಿಸೂಚನೆ</string>
    <string name="show_ssn_summary">ಕರೆ ನಿರ್ವಹಣೆಗೆ ಸಂಬಂಧಿಸಿದಂತೆ ಹಲವಾರು ನೆಟ್‌ವರ್ಕ್ ಈವೆಂಟ್‌ಗಳಿಗೆ ಅಧಿಸೂಚನೆಯನ್ನು ತೋರಿಸು</string>

    <!-- Blacklist preferences -->
    <string name="blacklist_title">ಬ್ಲಾಕ್‌ಲಿಸ್ಟ್</string>
    <string name="blacklist_summary_disabled">"ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಲಾಗಿದೆ"</string>
    <string name="blacklist_summary">ಬ್ಲಾಕ್‌ಲಿಸ್ಟ್‌ನಲ್ಲಿರುವ ಫೋನ್ ಸಂಖ್ಯೆಗಳಿಂದ ಒಳಬರುವ ಕರೆಗಳನ್ನು ನಿರ್ಬಂಧಿಸಲಾಗುತ್ತದೆ</string>

    <!-- Number lookup -->
    <string name="number_lookup">ಫೋನ್ ಸಂಖ್ಯೆ ಲುಕಪ್</string>
