<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title for the Calendar Storage process. -->
    <string name="calendar_storage">"ಕ್ಯಾಲೆಂಡರ್ ಸಂಗ್ರಹಣೆ"</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading calendar. -->
    <string name="upgrade_msg">"ಕ್ಯಾಲೆಂಡರ್ ಡೇಟಾಬೇಸ್ ಅಪ್‌ಗ್ರೇಡ್‌ ಮಾಡಲಾಗುತ್ತಿದೆ."</string>

    <!-- Temporary Calendar name to use before we have the calendar name from the server -->
    <string name="calendar_default_name">"ಡೀಫಾಲ್ಟ್"</string>

    <!-- Caption on secret calendar info -->
    <string name="calendar_info">"ಕ್ಯಾಲೆಂಡರ್ ಮಾಹಿತಿ"</string>

    <!-- Error message for secret calendar info -->
    <string name="calendar_info_error">"ದೋಷ"</string>

    <!-- No calendars message for secret calendar info -->
    <string name="calendar_info_no_calendars">"ಯಾವುದೇ ಕ್ಯಾಲೆಂಡರ್‌ಗಳಿಲ್ಲ"</string>

    <!-- Event count message for secret calendar info -->
    <string name="calendar_info_events">"ಈವೆಂಟ್‌ಗಳು: <xliff:g id="EVENTS">%1$d</xliff:g>"</string>

    <!-- Event and dirty event count message for secret calendar info -->
    <string name="calendar_info_events_dirty">"ಈವೆಂಟ್‌ಗಳು: <xliff:g id="EVENTS_0">%1$d</xliff:g>,ಉಳಿಸಲಾಗದವು: <xliff:g id="DIRTY_EVENTS">%2$d</xliff:g>"</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">"ಕ್ಯಾಲೆಂಡರ್‌"</string>

    <!-- Debug tool - dialog - delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_tool_delete_button">"ಈಗ ಅಳಿಸು"</string>

    <!-- Debug tool - dialog - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_tool_start_button">"ಪ್ರಾರಂಭ"</string>
    
    <!-- Debug tool - Main message shown to the user when starting up the debug tool [CHAR LIMIT=NONE] -->
    <string name="debug_tool_message">"ನೀವು 1) ನಿಮ್ಮ ಕ್ಯಾಲೆಂಡರ್ ಡೇಟಾಬೇಸ್‌ ಅನ್ನು ಯಾವುದೇ ಅಪ್ಲಿಕೇಶನ್‌ಗಳು ಓದಬಲ್ಲ SD ಕಾರ್ಡ್/USB ಸಂಗ್ರಹಣೆಗೆ ನಕಲು ಮಾಡಲಿರುವಿರಿ, ಮತ್ತು 2) ಅದನ್ನು ಇಮೇಲ್ ಮಾಡಲಿರುವಿರಿ. ನೀವು ಅದನ್ನು ಯಶಸ್ವಿಯಾಗಿ ನಕಲು ಮಾಡಿದ ನಂತರ ಅಥವಾ ಇಮೇಲ್ ಸ್ವೀಕರಿಸಿದ ನಂತರ ಸಾಧನದಲ್ಲಿರುವ ಪ್ರತಿಯನ್ನು ತಪ್ಪದೇ ಅಳಿಸಿ."</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_sender_picker">"ನಿಮ್ಮ ಫೈಲ್ ಕಳುಹಿಸಲು ಪ್ರೋಗ್ರಾಂ ಒಂದನ್ನು ಆಯ್ಕೆ ಮಾಡಿ"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_subject">"ಕ್ಯಾಲೆಂಡರ್ Db ಲಗತ್ತಿಸಲಾಗಿದೆ"</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_body">"ನನ್ನ ಎಲ್ಲ ಅಪಾಯಿಂಟ್‌ಮೆಂಟ್‌ಗಳು ಮತ್ತು ವೈಯಕ್ತಿಕ ಮಾಹಿತಿಗಳನ್ನು ಒಳಗೊಂಡ ನನ್ನ ಕ್ಯಾಲೆಂಡರ್ ಡೇಟಾಬೇಸ್ ಲಗತ್ತಿಸಲಾಗಿದೆ. ಎಚ್ಚರಿಕೆಯಿಂದ ನಿಭಾಯಿಸಿ."</string>
</resources>
