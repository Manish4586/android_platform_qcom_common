    <!-- Title for the Calendar Storage process. -->
    <string name="calendar_storage">"Archiviazione calendario"</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading calendar. -->
    <string name="upgrade_msg">"Upgrade del database di Calendario."</string>

    <!-- Temporary Calendar name to use before we have the calendar name from the server -->
    <string name="calendar_default_name">"Predefinito"</string>

    <!-- Caption on secret calendar info -->
    <string name="calendar_info">"Info calendario"</string>

    <!-- Error message for secret calendar info -->
    <string name="calendar_info_error">"Errore"</string>

    <!-- No calendars message for secret calendar info -->
    <string name="calendar_info_no_calendars">"Nessun calendario"</string>

    <!-- Event count message for secret calendar info -->
    <string name="calendar_info_events">"Eventi: <xliff:g id="EVENTS">%1$d</xliff:g>"</string>

    <!-- Event and dirty event count message for secret calendar info -->
    <string name="calendar_info_events_dirty">"Eventi: <xliff:g id="EVENTS_0">%1$d</xliff:g>, Non salvati: <xliff:g id="DIRTY_EVENTS">%2$d</xliff:g>"</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
