<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title for the Calendar Storage process. -->
    <string name="calendar_storage">"ឧបករណ៍​ផ្ទុក​ប្រតិទិន"</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading calendar. -->
    <string name="upgrade_msg">"ធ្វើ​បច្ចុប្បន្នភាព​មូលដ្ឋាន​ទិន្នន័យ​ប្រតិទិន។"</string>

    <!-- Temporary Calendar name to use before we have the calendar name from the server -->
    <string name="calendar_default_name">"លំនាំដើម"</string>

    <!-- Caption on secret calendar info -->
    <string name="calendar_info">"ព័ត៌មាន​ប្រតិទិន"</string>

    <!-- Error message for secret calendar info -->
    <string name="calendar_info_error">"កំហុស"</string>

    <!-- No calendars message for secret calendar info -->
    <string name="calendar_info_no_calendars">"មិន​មាន​ប្រតិទិន"</string>

    <!-- Event count message for secret calendar info -->
    <string name="calendar_info_events">"ព្រឹត្តិការណ៍៖ <xliff:g id="EVENTS">%1$d</xliff:g>"</string>

    <!-- Event and dirty event count message for secret calendar info -->
    <string name="calendar_info_events_dirty">"ព្រឹត្តិការណ៍៖ <xliff:g id="EVENTS_0">%1$d</xliff:g>, មិន​បាន​រក្សាទុក៖ <xliff:g id="DIRTY_EVENTS">%2$d</xliff:g>"</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">"ប្រតិទិន"</string>

    <!-- Debug tool - dialog - delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_tool_delete_button">"លុប​ឥឡូវ"</string>

    <!-- Debug tool - dialog - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_tool_start_button">"ចាប់ផ្ដើម"</string>
    
    <!-- Debug tool - Main message shown to the user when starting up the debug tool [CHAR LIMIT=NONE] -->
    <string name="debug_tool_message">"អ្នក​ហៀបនឹង ១) ចម្លង​មូលដ្ឋាន​ទិន្នន័យ​ប្រតិទិន​របស់​អ្នក​ទៅកាន់​កាត​អេសឌី/ឧបករណ៍​ផ្ទុក​យូអេសប៊ី​ដែល​អាច​អាន​បាន​ដោយ​កម្មវិធី​ណាមួយ ២) ផ្ញើ​អ៊ីមែល​វា។ ចងចាំ​ថា​អ្នក​ត្រូវ​លុប​ច្បាប់​ចម្លង​ភ្លាមៗ បន្ទាប់ពី​បាន​ចម្លង​ចេញពី​ឧបករណ៍ ឬ​បាន​ទទួល​អ៊ីមែល។"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_sender_picker">"ជ្រើស​កម្មវិធី​ដើម្បី​ផ្ញើ​ឯកសារ​របស់​អ្នក"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_subject">"បាន​ភ្ជាប់​មូលដ្ឋាន​ទិន្នន័យ​ប្រតិទិន"</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_body">"ឯកសារ​ភ្ជាប់​គឺជា​មូលដ្ឋាន​ទិន្នន័យ​ប្រតិទិន​ជាមួយ​ការ​ណាត់​ជួប​ទាំងអស់ និង​ព័ត៌មាន​ផ្ទាល់ខ្លួន​របស់​ខ្ញុំ។ ប្រើ​ដោយ​ប្រុងប្រយ័ត្ន។"</string>
</resources>
