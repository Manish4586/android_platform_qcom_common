<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title for the Calendar Storage process. -->
    <string name="calendar_storage">કૅલેન્ડર સંગ્રહ</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading calendar. -->
    <string name="upgrade_msg">કૅલેન્ડર ડેટાબેસ અપગ્રેડ કરી રહ્યાં છે.</string>

    <!-- Temporary Calendar name to use before we have the calendar name from the server -->
    <string name="calendar_default_name">ડિફૉલ્ટ</string>

    <!-- Caption on secret calendar info -->
    <string name="calendar_info">કૅલેન્ડર માહિતી</string>

    <!-- Error message for secret calendar info -->
    <string name="calendar_info_error">ભૂલ</string>

    <!-- No calendars message for secret calendar info -->
    <string name="calendar_info_no_calendars">કોઈ કૅલેન્ડર્સ નથી</string>

    <!-- Event count message for secret calendar info -->
    <string name="calendar_info_events">ઇવેન્ટ્સ: <xliff:g id="events">%1$d</xliff:g></string>

    <!-- Event and dirty event count message for secret calendar info -->
    <string name="calendar_info_events_dirty">ઇવેન્ટ્સ: <xliff:g id="events">%1$d</xliff:g>, વણસાચવેલ: <xliff:g id="dirty events">%2$d</xliff:g></string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">કૅલેન્ડર</string>

    <!-- Debug tool - dialog - delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_tool_delete_button">હવે કાઢી નાખો</string>

    <!-- Debug tool - dialog - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_tool_start_button">પ્રારંભ કરો</string>
    
    <!-- Debug tool - Main message shown to the user when starting up the debug tool [CHAR LIMIT=NONE] -->
    <string name="debug_tool_message">તમે 1) SD કાર્ડ/USB સંગ્રહ પર તમારા કૅલેન્ડર ડેટાબેસની એક કૉપિ બનાવવામાં છો, જે કોઈપણ એપ્લિકેશન દ્વારા વાંચી શકાશે અને 2) તેને ઈમેલ કરવામાં છો. તમે ઉપકરણમાંથી તેની સફળતાપૂર્વક કૉપિ બનાવી લો અથવા ઈમેલ પ્રાપ્ત કરી લો પછી કાઢી નાખવાનું યાદ રાખો.</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_sender_picker">તમારી ફાઇલ મોકલવા માટે એક પ્રોગ્રામ પસંદ કરો</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_subject">કૅલેન્ડર ડેટાબેસ જોડ્યો</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_body">મારી બધી ઍપોઇન્ટમેન્ટ્સ અને વ્યક્તિગત માહિતી મારા કૅલેન્ડર ડેટાબેઝ સાથે જોડેલી છે. કૃપયા ધ્યાનપૂર્વક વાપરો.</string>
</resources>
