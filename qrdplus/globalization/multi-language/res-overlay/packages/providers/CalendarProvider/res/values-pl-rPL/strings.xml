    <!-- Title for the Calendar Storage process. -->
    <string name="calendar_storage">"Pamięć kalendarza"</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading calendar. -->
    <string name="upgrade_msg">"Uaktualnianie bazy danych Kalendarza."</string>

    <!-- Temporary Calendar name to use before we have the calendar name from the server -->
    <string name="calendar_default_name">"Domyślny"</string>

    <!-- Caption on secret calendar info -->
