<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title for the Calendar Storage process. -->
    <string name="calendar_storage">ক্যালেন্ডার স্টোরেজ</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading calendar. -->
    <string name="upgrade_msg">আপগ্রেড করা ক্যালেন্ডার ডাটাবেস।</string>

    <!-- Temporary Calendar name to use before we have the calendar name from the server -->
    <string name="calendar_default_name">ডিফল্ট</string>

    <!-- Caption on secret calendar info -->
    <string name="calendar_info">ক্যালেন্ডার তথ্য</string>

    <!-- Error message for secret calendar info -->
    <string name="calendar_info_error">ত্রুটি</string>

    <!-- No calendars message for secret calendar info -->
    <string name="calendar_info_no_calendars">ক্যালেন্ডার নয়</string>

    <!-- Event count message for secret calendar info -->
    <string name="calendar_info_events">অনুষ্ঠানগুলি: <xliff:g id="events">%1$d</xliff:g></string>

    <!-- Event and dirty event count message for secret calendar info -->
    <string name="calendar_info_events_dirty">অনুষ্ঠানগুলি: <xliff:g id="events">%1$d</xliff:g>, অসংরক্ষিত: <xliff:g id="dirty events">%2$d</xliff:g></string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">ক্যালেন্ডার</string>

    <!-- Debug tool - dialog - delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_tool_delete_button">এখন মুছুন</string>

    <!-- Debug tool - dialog - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_tool_start_button">চালু করুন</string>
    
    <!-- Debug tool - Main message shown to the user when starting up the debug tool [CHAR LIMIT=NONE] -->
    <string name="debug_tool_message">আপনি এগুলি করতে চলেছেন 1) SD কার্ড/USB সঞ্চয়স্থানে আপনার ক্যালেন্ডারের একটি অনুলিপি গ্রহণ যা যে কোনও অ্যাপ্লিকেশনে পঠনযোগ্য, এবং 2) এটি ইমেল করা। আপনি প্রতিলিপিটি ডিভাইস থেকে অনুলিপি করে নেওয়া বা ইমেলটি প্রাপ্ত করার পরে মনে করে মুছে দিন।</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_sender_picker">আপনার ফাইলে পাঠানোর জন্য একটি প্রোগ্রাম নির্বাচন করুন</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_subject">ক্যালেন্ডার Db সংযুক্ত</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_tool_email_body">আমার সমস্ত পরিচিতি তথ্য সহ আমার ক্যালেন্ডার ডেটাবেসটি সংযুক্ত রয়েছে। যত্নের সাথে ব্যবহার করুন।</string>
</resources>
