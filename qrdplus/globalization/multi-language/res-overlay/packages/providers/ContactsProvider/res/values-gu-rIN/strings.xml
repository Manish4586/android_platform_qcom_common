<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for this app\'s shared user ID (and thus for all other
         apps sharing the uid). -->
    <string name="sharedUserLabel">એન્ડ્રોઇડ કોર એપ્લિકેશન્સ</string>

    <!-- This is the label for the application that stores contacts data -->
    <string name="app_label">સંપર્કો સંગ્રહ</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">સંપર્કો</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading contacts. -->
    <string name="upgrade_msg">સંપર્કો ડેટાબેસને અપગ્રેડ કરી રહ્યું છે.</string>

    <!-- Ticker for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_ticker">સંપર્કો અપગ્રેડ્સને વધુ મેમરીની જરૂર છે.</string>

    <!-- Title for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_title">સંપર્કો માટે સંગ્રહ અપગ્રેડ કરી રહ્યું છે</string>

    <!-- Text for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_text">અપગ્રેડ પૂર્ણ કરવા માટે ટચ કરો.</string>

    <!-- The name of the default contact directory -->
    <string name="default_directory">સંપર્કો</string>

    <!-- The name of the invisible local contact directory -->
    <string name="local_invisible_directory">અન્ય</string>

    <!-- The prefix string before the number used for the display name for the voicemail table.
         Because of the way this is combined in SQL we can\'t allow a generic format string.
         The resulting string will be this string with the number appended to the end.
         Note that the trailing space is important, and that to achieve it we have to wrap the
         string in double quotes. -->
    <string name="voicemail_from_column">"આના તરફથી વૉઇસમેલ "</string>

    <!-- Debug tool - title of the dialog which copies the contact database into the external storage. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_title">સંપર્કો ડેટાબેસને કૉપિ કરો</string>

    <!-- Debug tool - message shown to the user on the dialog which sends a copy of  the contact database via email or other apps. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_database_message">તમે 1) તમારા ડેટાબેસની એક કૉપિ બનાવવામાં છો, જેમાં આંતરિક સંગ્રહ પરની બધી સંપર્કો સંબંધિત માહિતી અને બધા કૉલ લૉગનો સમાવેશ છે અને 2) તેને ઈમેલ કરવામાં છો. તમે ઉપકરણમાંથી તેની સફળતાપૂર્વક કૉપિ બનાવી લો અથવા ઈમેલ પ્રાપ્ત કરી લો પછી કાઢી નાખવાનું યાદ રાખો.</string>

    <!-- Debug tool - dialog button- delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_dump_delete_button">હવે કાઢી નાખો</string>

    <!-- Debug tool - dialog button - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_dump_start_button">પ્રારંભ કરો</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_sender_picker">તમારી ફાઇલ મોકલવા માટે એક પ્રોગ્રામ પસંદ કરો</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_subject">સંપર્કો Db જોડેલ છે</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_body">મારી બધી સંપર્ક માહિતી સાથે મારો સંપર્કો ડેટાબેસ જોડેલ છે. સાવધાની વર્તો.</string>

    <string name="group_title_co_workers">"સહકાર્યકરો"</string>
    <string name="group_title_family">"પરિવાર"</string>
    <string name="group_title_friends">"મિત્રો"</string>
</resources>
