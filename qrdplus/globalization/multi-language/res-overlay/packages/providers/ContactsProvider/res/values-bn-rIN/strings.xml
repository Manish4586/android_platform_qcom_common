<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for this app\'s shared user ID (and thus for all other
         apps sharing the uid). -->
    <string name="sharedUserLabel">Android কোর অ্যাপসমূহ</string>

    <!-- This is the label for the application that stores contacts data -->
    <string name="app_label">যোগাযোগসমূহের স্টোরেজ</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">যোগাযোগসমূহ</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading contacts. -->
    <string name="upgrade_msg">পরিচিতির ডেটাবেস আপগ্রেড করছি।</string>

    <!-- Ticker for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_ticker">পরিচিতির আপগ্রেডের আরো স্মৃতির প্রয়োজন।</string>

    <!-- Title for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_title">পরিচিতিগুলির জন্য সঞ্চয়স্থান আপগ্রেড করা হচ্ছে</string>

    <!-- Text for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_text">আপগ্রেড সম্পূর্ণ করতে স্পর্শ করুন।</string>

    <!-- The name of the default contact directory -->
    <string name="default_directory">যোগাযোগসমূহ</string>

    <!-- The name of the invisible local contact directory -->
    <string name="local_invisible_directory">অন্যান্য</string>

    <!-- The prefix string before the number used for the display name for the voicemail table.
         Because of the way this is combined in SQL we can\'t allow a generic format string.
         The resulting string will be this string with the number appended to the end.
         Note that the trailing space is important, and that to achieve it we have to wrap the
         string in double quotes. -->
    <string name="voicemail_from_column">"এর থেকে ভয়েসমেইল "</string>

    <!-- Debug tool - title of the dialog which copies the contact database into the external storage. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_title">পরিচিতির ডাটাবেস অনুলিপি করুন</string>

    <!-- Debug tool - message shown to the user on the dialog which sends a copy of  the contact database via email or other apps. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_database_message">আপনি এগুলি করতে চলেছেন 1) আপনার ডেটাবেসের একটি অনুলিপি করা যার মধ্যে সমস্ত পরিচিতি সম্পর্কিত তথ্য এবং অভ্যন্তরীণ সঞ্চয়স্থানে সমস্ত কল লগ অন্তর্ভুক্ত, এবং 2) এটি ইমেল করা। আপনি প্রতিলিপিটি ডিভাইস থেকে অনুলিপি করে নেওয়া বা ইমেলটি প্রাপ্ত করার পরে মনে করে মুছে দিন।</string>

    <!-- Debug tool - dialog button- delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_dump_delete_button">এখন মুছুন</string>

    <!-- Debug tool - dialog button - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_dump_start_button">চালু করুন</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_sender_picker">আপনার ফাইলে পাঠানোর জন্য একটি প্রোগ্রাম নির্বাচন করুন</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_subject">পরিচিতি Db সংযুক্ত</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_body">আমার সমস্ত পরিচিতি তথ্য সহ আমার পরিচিতিগুলির ডেটাবেসটি সংযুক্ত রয়েছে। যত্নের সাথে ব্যবহার করুন।</string>

    <string name="group_title_co_workers">"সহকর্মীগণ"</string>
    <string name="group_title_family">"পরিবার"</string>
    <string name="group_title_friends">"বন্ধুগণ"</string>
</resources>
