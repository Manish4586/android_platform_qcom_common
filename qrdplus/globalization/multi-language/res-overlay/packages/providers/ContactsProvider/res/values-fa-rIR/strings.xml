<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for this app\'s shared user ID (and thus for all other
         apps sharing the uid). -->
    <string name="sharedUserLabel">"Android Core Apps"</string>

    <!-- This is the label for the application that stores contacts data -->
    <string name="app_label">"حافظه مخاطبین"</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">"مخاطبین"</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading contacts. -->
    <string name="upgrade_msg">"ارتقا پایگاه داده مخاطبین."</string>

    <!-- Ticker for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_ticker">"برای ارتقای مخاطبین به حافظه بیشتری نیاز است."</string>

    <!-- Title for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_title">"ارتقا حافظه برای مخاطبین"</string>

    <!-- Text for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_text">"برای تکمیل ارتقا لمس کنید."</string>

    <!-- The name of the default contact directory -->
    <string name="default_directory">"مخاطبین"</string>

    <!-- The name of the invisible local contact directory -->
    <string name="local_invisible_directory">"سایر موارد"</string>

    <!-- The prefix string before the number used for the display name for the voicemail table.
         Because of the way this is combined in SQL we can\'t allow a generic format string.
         The resulting string will be this string with the number appended to the end.
         Note that the trailing space is important, and that to achieve it we have to wrap the
         string in double quotes. -->
    <string name="voicemail_from_column">"پست صوتی از "</string>

    <!-- Debug tool - title of the dialog which copies the contact database into the external storage. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_title">"کپی پایگاه داده مخاطبین"</string>

    <!-- Debug tool - message shown to the user on the dialog which sends a copy of  the contact database via email or other apps. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_database_message">"شما در شرف ۱) ایجاد یک کپی از پایگاه داده‌ در حافظه داخلی هستید، این کپی حاوی همه اطلاعات مربوط به مخاطبین و همه گزارشات تماس است و همچنین می‌خواهید ۲) آنرا ایمیل کنید. به خاطر داشته باشید که به محض کپی کردن این نسخه در دستگاه یا دریافت ایمیل، آنرا حذف کنید."</string>

    <!-- Debug tool - dialog button- delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_dump_delete_button">"اکنون حذف شود"</string>

    <!-- Debug tool - dialog button - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_dump_start_button">"شروع"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_sender_picker">"یک برنامه را برای ارسال فایل خود انتخاب کنید"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_subject">"پایگاه داده مخاطبین پیوست شد"</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_body">"پایگاه داده مخاطبین من همراه با تمام اطلاعات مخاطبین پیوست شده است. با دقت از آن استفاده شود."</string>

    <string name="group_title_co_workers">‎"‎همکاران‎"‎</string>
    <string name="group_title_family">‎"‎خانواده‎"‎</string>
    <string name="group_title_friends">‎"‎دوستان‎"‎</string>
</resources>
