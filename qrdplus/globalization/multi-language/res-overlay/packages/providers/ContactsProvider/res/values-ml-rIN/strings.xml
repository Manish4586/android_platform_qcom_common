    <!-- Label for this app\'s shared user ID (and thus for all other
         apps sharing the uid). -->
    <string name="sharedUserLabel">"Android കോർ അപ്ലിക്കേഷനുകൾ"</string>

    <!-- This is the label for the application that stores contacts data -->
    <string name="app_label">"കോൺടാക്റ്റുകളുടെ സംഭരണം"</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">കോൺടാക്റ്റുകൾ</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading contacts. -->
    <string name="upgrade_msg">"കോൺ‌ടാക്റ്റുകളുടെ ഡാറ്റാബേസ് അപ്‌ഗ്രേഡുചെയ്യുന്നു."</string>

    <!-- Ticker for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_ticker">"കോൺ‌ടാക്റ്റുകൾ അപ്‌ഗ്രേഡുചെയ്യാൻ കൂടുതൽ മെമ്മറി ആവശ്യമാണ്."</string>

    <!-- Title for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_title">"കോൺ‌ടാക്റ്റുകൾക്കായുള്ള സംഭരണം അപ്‌ഗ്രേഡുചെയ്യുന്നു"</string>

    <!-- Text for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_text">"അപ്‌ഗ്രേഡ് പൂർത്തിയാക്കാൻ സ്‌പർശിക്കുക."</string>

    <!-- The name of the default contact directory -->
    <string name="default_directory">കോൺടാക്റ്റുകൾ</string>

    <!-- The name of the invisible local contact directory -->
    <string name="local_invisible_directory">"മറ്റുള്ളവ"</string>

    <!-- The prefix string before the number used for the display name for the voicemail table.
         Because of the way this is combined in SQL we can\'t allow a generic format string.
         The resulting string will be this string with the number appended to the end.
         Note that the trailing space is important, and that to achieve it we have to wrap the
         string in double quotes. -->
    <string name="voicemail_from_column">"ഈ നമ്പറിൽ നിന്നുള്ള വോയ്‌സ്‌മെയിൽ "</string>

    <!-- Debug tool - title of the dialog which copies the contact database into the external storage. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_title">"കോൺടാക്റ്റുകളുടെ ഡാറ്റാബേസ് പകർത്തുക"</string>

    <!-- Debug tool - message shown to the user on the dialog which sends a copy of  the contact database via email or other apps. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_database_message">"നിങ്ങൾ 1) എല്ലാ കോൺടാക്റ്റുകളുമായും ബന്ധപ്പെട്ട വിവരങ്ങളും എല്ലാ കോൾ ലോഗും ഉൾപ്പെടുന്ന നിങ്ങളുടെ ഡാറ്റാബേസിന്റെ ഒരു പകർപ്പ് ആന്തരിക സംഭരണത്തിൽ സൃഷ്‌ടിക്കാനും 2) അത് ഇമെയിൽ ചെയ്യാനും പോകുന്നു. ഉപകരണത്തിൽ നിന്ന് പകർപ്പ് നിങ്ങൾ പകർത്തിക്കഴിഞ്ഞാലോ ഇമെയിൽ ലഭിച്ചുകഴിഞ്ഞാലോ ഉടൻ തന്നെ അത് ഇല്ലാതാക്കാൻ ശ്രദ്ധിക്കുക."</string>

    <!-- Debug tool - dialog button- delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_dump_delete_button">"ഇപ്പോൾ ഇല്ലാതാക്കുക"</string>

    <!-- Debug tool - dialog button - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_dump_start_button">"ആരംഭിക്കുക"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_sender_picker">"നിങ്ങളുടെ ഫയൽ അയയ്‌ക്കുന്നതിന് ഒരു പ്രോഗ്രാം തിരഞ്ഞെടുക്കുക"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_subject">"കോൺടാക്റ്റുകളുടെ Db അറ്റാച്ചുചെയ്‌തു"</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_body">"അറ്റാച്ചുചെയ്‌തിരിക്കുന്നത് എന്റെ എല്ലാ കോൺടാക്റ്റുകളുടെയും വിവരമടങ്ങിയ എന്റെ കോൺടാക്റ്റുകളുടെ ഡാറ്റാബേസാണ്. ശ്രദ്ധാപൂർവ്വം കൈകാര്യം ചെയ്യുക."</string>

    <string name="group_title_co_workers">"സഹപ്രവർത്തകർ"</string>
    <string name="group_title_family">"കുടുംബം"</string>
    <string name="group_title_friends">"സുഹൃത്തുക്കൾ"</string>
