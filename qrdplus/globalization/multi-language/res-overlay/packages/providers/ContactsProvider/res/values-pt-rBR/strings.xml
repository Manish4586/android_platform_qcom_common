<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Label for this app\'s shared user ID (and thus for all other
         apps sharing the uid). -->
    <string name="sharedUserLabel">"Principais apps do Android"</string>

    <!-- This is the label for the application that stores contacts data -->
    <string name="app_label">"Armazenamento de contatos"</string>

    <!-- What to show in messaging that refers to this provider, e.g. AccountSyncSettings -->
    <string name="provider_label">"Contatos"</string>

    <!-- [CHAR LIMIT=NONE] Boot message while upgrading contacts. -->
    <string name="upgrade_msg">"Atualizando o banco de dados de contatos."</string>

    <!-- Ticker for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_ticker">"A atualização de contatos precisa de mais memória."</string>

    <!-- Title for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_title">"Atualização do armazenamento para contatos"</string>

    <!-- Text for the notification shown when updating contacts fails because of memory shortage -->
    <string name="upgrade_out_of_memory_notification_text">"Toque para concluir a atualização."</string>

    <!-- The name of the default contact directory -->
    <string name="default_directory">"Contatos"</string>

    <!-- The name of the invisible local contact directory -->
    <string name="local_invisible_directory">"Outros"</string>

    <!-- The prefix string before the number used for the display name for the voicemail table.
         Because of the way this is combined in SQL we can\'t allow a generic format string.
         The resulting string will be this string with the number appended to the end.
         Note that the trailing space is important, and that to achieve it we have to wrap the
         string in double quotes. -->
    <string name="voicemail_from_column">"Correio de voz de "</string>

    <!-- Debug tool - title of the dialog which copies the contact database into the external storage. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_title">"Copiar banco de dados de contatos"</string>

    <!-- Debug tool - message shown to the user on the dialog which sends a copy of  the contact database via email or other apps. [CHAR LIMIT=NONE] -->
    <string name="debug_dump_database_message">"Você está prestes a 1) fazer uma cópia de seu banco de dados no armazenamento interno, com todas as informações relacionadas aos contatos e todo o histórico de chamadas e 2) enviar essa cópia por e-mail. Lembre-se de excluir a cópia, logo que você a tiver copiado do dispositivo ou que o e-mail for recebido."</string>

    <!-- Debug tool - dialog button- delete file now [CHAR LIMIT=NONE] -->
    <string name="debug_dump_delete_button">"Excluir agora"</string>

    <!-- Debug tool - dialog button - start copying [CHAR LIMIT=NONE] -->
    <string name="debug_dump_start_button">"Iniciar"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_sender_picker">"Escolha um programa para enviar o arquivo"</string>

    <!-- Debug tool - email subject [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_subject">"BD de contatos anexado"</string>

    <!-- Debug tool - email body [CHAR LIMIT=NONE] -->
    <string name="debug_dump_email_body">"Meu banco de dados de contatos está anexado, Lá estão todas as informações de meus contatos. Use-o com cuidado."</string>

    <string name="group_title_co_workers">"Colegas de trabalho"</string>
    <string name="group_title_family">"Família"</string>
    <string name="group_title_friends">"Amigos"</string>
</resources>
