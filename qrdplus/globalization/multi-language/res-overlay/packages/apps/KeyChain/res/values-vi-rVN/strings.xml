<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Chuỗi chính"</string>

    <!-- Dialog title when no certificates were found -->
    <string name="title_no_certs">"Không tìm thấy chứng chỉ nào"</string>

    <!-- Dialog title when at least one certificate was found -->
    <string name="title_select_cert">"Chọn chứng chỉ"</string>

    <!-- Used at top of dialog to identify requesting application -->
    <string name="requesting_application">"Ứng dụng %s đã yêu cầu chứng chỉ. Việc chọn chứng chỉ sẽ cho phép ứng dụng sử dụng danh tính này với máy chủ tại lúc này và trong tương lai."</string>

    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->
    <string name="requesting_server">"Ứng dụng đã xác định máy chủ yêu cầu là %s nhưng bạn chỉ nên cho phép ứng dụng truy cập vào chứng chỉ nếu bạn tin tưởng ứng dụng."</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->
    <string name="install_new_cert_message">"Bạn có thể cài đặt chứng chỉ từ tệp PKCS#12 với tiện ích mở rộng %1$s hoặc %2$s nằm trong bộ nhớ ngoài."</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate -->
    <string name="install_new_cert_button_label">"Cài đặt"</string>

    <!-- Label of button to send the allow the application certificate request -->
    <string name="allow_button">"Cho phép"</string>

    <!-- Label of button to send the deny the application certificate request -->
    <string name="deny_button">"Từ chối"</string>

</resources>
