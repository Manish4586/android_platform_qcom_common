<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Msururu Muhimu"</string>

    <!-- Dialog title when no certificates were found -->
    <string name="title_no_certs">"Hakuna vyeti vinavyopatikana"</string>

    <!-- Dialog title when at least one certificate was found -->
    <string name="title_select_cert">"Chagua cheti"</string>

    <!-- Used at top of dialog to identify requesting application -->
    <string name="requesting_application">Programu %s imeomba cheti. Kuchagua cheti kutairuhusu programu kutumia utambulisho huu na seva wakati wa sasa na siku zijazo.</string>

    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->
    <string name="requesting_server">Programu imetambua seva inayotuma maombi kama %s, lakini unafaa kuiruhusu programu kufikia cheti ikiwa unaomani peke yake.</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->
    <string name="install_new_cert_message">"Unaweza kusakinisha vyeti kutoka katika faili PKCS#12 kwa kirefusho %1$s au %2$s kilicho katika hifadhi ya nje."</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate -->
    <string name="install_new_cert_button_label">"Sakinisha"</string>

    <!-- Label of button to send the allow the application certificate request -->
    <string name="allow_button">"Ruhusu"</string>

    <!-- Label of button to send the deny the application certificate request -->
    <string name="deny_button">"Kana"</string>

</resources>
