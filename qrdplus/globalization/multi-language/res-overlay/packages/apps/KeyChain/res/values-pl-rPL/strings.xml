<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Pęk kluczy"</string>

    <!-- Dialog title when no certificates were found -->
    <string name="title_no_certs">"Nie znaleziono certyfikatów"</string>

    <!-- Dialog title when at least one certificate was found -->
    <string name="title_select_cert">"Wybierz certyfikat"</string>

    <!-- Used at top of dialog to identify requesting application -->
    <string name="requesting_application">"Aplikacja %s prosi o certyfikat. Wybranie certyfikatu umożliwi aplikacji używanie tej tożsamości na serwerach zarówno teraz, jak i w przyszłości."</string>

    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->
    <string name="requesting_server">"Aplikacja zidentyfikowała serwer żądający jako %s, ale dostęp do certyfikatu należy przyznać tylko zaufanej aplikacji."</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->
    <string name="install_new_cert_message">"Możesz zainstalować certyfikaty z pliku PKCS#12 z rozszerzeniem %1$s lub %2$s, który znajduje się w pamięci zewnętrznej."</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate -->
    <string name="install_new_cert_button_label">"Zainstaluj"</string>

    <!-- Label of button to send the allow the application certificate request -->
    <string name="allow_button">"Zezwól"</string>

    <!-- Label of button to send the deny the application certificate request -->
    <string name="deny_button">"Odmów"</string>

</resources>
