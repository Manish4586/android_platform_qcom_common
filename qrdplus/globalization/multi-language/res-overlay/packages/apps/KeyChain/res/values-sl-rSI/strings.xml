<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Shramba poverilnic"</string>

    <!-- Dialog title when no certificates were found -->
    <string name="title_no_certs">"Potrdil ni mogoče najti"</string>

    <!-- Dialog title when at least one certificate was found -->
    <string name="title_select_cert">"Izberite potrdilo"</string>

    <!-- Used at top of dialog to identify requesting application -->
    <string name="requesting_application">"Aplikacija %s je zahtevala potrdilo. Če izberete potrdilo, boste aplikaciji omogočili uporabo te identitete s strežniki zdaj in v prihodnosti."</string>

    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->
    <string name="requesting_server">"Aplikacija je zahtevajoči strežnik prepoznala kot %s, vendar ji dostop do potrdila dovolite le, če ji zaupate."</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->
    <string name="install_new_cert_message">"Namestite lahko potrdila iz datoteke PKCS#12 z razširitvijo %1$s ali %2$s, ki je v zunanjem pomnilniku."</string>

    <!-- Label of button to send the user to the CertInstaller to install a certificate -->
    <string name="install_new_cert_button_label">"Namesti"</string>

    <!-- Label of button to send the allow the application certificate request -->
    <string name="allow_button">"Dovoli"</string>

    <!-- Label of button to send the deny the application certificate request -->
    <string name="deny_button">"Zavrni"</string>

</resources>
