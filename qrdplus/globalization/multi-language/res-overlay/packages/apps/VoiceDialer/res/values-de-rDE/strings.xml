<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Sprachwahl"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Wussten Sie schon..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Durch Drücken und Halten der grünen Anruftaste wird die Sprachwahl geöffnet."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Beispiele: "<b>" \n\"Max Muster anrufen\" \n\"Max Muster zu Hause anrufen\" \n\"Mailbox anrufen\" \n\"(040) 123 4567 wählen\" \n\"110, 112, … wählen\" \n\"+44 7333 444 555 wählen\" \n\"Wahlwiederholung\" \n\"Kalender öffnen\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Sprachwahl"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Bluetooth-Voice-Dialer"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Startvorgang..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Jetzt sprechen..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Bitte versuchen Sie es erneut."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Initialisierung nicht möglich"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Starten Sie Ihr Telefon neu und versuchen Sie es dann erneut."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Keine Headset-Verbindung"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" Privat"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" Mobil"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" Geschäftlich"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" Sonstige"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"Die Sprachwahl-Protokollierung ist aktiviert."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"Die Sprachwahl-Protokollierung ist deaktiviert."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Jetzt sprechen..."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Keine Ergebnisse, versuchen Sie es erneut."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Ungültige Auswahl"</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Bis bald."</string>

</resources>
