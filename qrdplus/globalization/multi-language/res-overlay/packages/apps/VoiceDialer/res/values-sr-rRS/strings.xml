<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Бирач гласом"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Да ли сте знали..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Притиском и држањем зеленог дугмета за позив отвара се Бирач гласом."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Примери: "<b>" \n„Позови Петра Петровића“ \n„Позови Петра Петровића код куће, …“ \n„Позови говорну пошту“ \n„Позови(866) 555 0123“ \n„Позови 911, 811, …“ \n„Позови +44 7333 444 555“ \n„Поново позови“ \n„Oтвори календар“ "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Бирач гласом"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Bluetooth бирач гласом"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Покретање..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Слушање..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Покушајте поново."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Покретање није могуће"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Поново покрените телефон, а затим покушајте опет."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Прекинута је веза са слушалицама."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" код куће"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" на број мобилног телефона"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" на послу"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" на други"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"Пријављивање Бирача гласом је укључено."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"Пријављивање Бирача гласом је искључено."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Говорите сада."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Нема резултата, покушајте поново."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Неважећи избор."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Довиђења"</string>

</resources>
