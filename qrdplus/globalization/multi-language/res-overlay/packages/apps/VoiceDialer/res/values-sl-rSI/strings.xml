<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Glasovni klicalnik"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Ste vedeli ..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Glasovni klicalnik odprete tako, da pritisnete in držite zeleni gumb »Pokliči«."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Primeri: "<b>" \n»Pokliči Janeza Kovača« \n»Pokliči Janeza Kovača domov …« \n»Pokliči tajnico« \n»Pokliči (866) 555 0123« \n»Pokliči 911, 811 …« \n»Pokliči +44 7333 444 555« \n»Pokliči isto številko« \n»Odpri koledar« "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Glasovni klicalnik"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Bluetoothov glasovni klicalnik"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Zagon ..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Poslušanje ..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Poskusite znova."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Ni mogoče začeti"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Znova zaženite telefon in poskusite znova."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Povezava s slušalkami z mikrofonom je prekinjena."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" doma"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" na mobilnem telefonu"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" v službi"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" drugje"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"Prijava z glasovnim klicalnikom je vklopljena."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"Prijava z glasovnim klicalnikom je izklopljena."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Začnite govoriti."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Ni rezultatov, poskusite znova."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Neveljavna izbira."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Nasvidenje."</string>

</resources>
