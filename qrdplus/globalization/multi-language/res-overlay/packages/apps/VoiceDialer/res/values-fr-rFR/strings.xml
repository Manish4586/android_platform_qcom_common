<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Reconnaissance vocale"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Saviez-vous que..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Appuyez sur le bouton vert Appel et maintenez la touche enfoncée pour ouvrir l\'application Reconnaissance vocale."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Exemples : "<b>" \n\"Appeler Thomas Durand\" \n\"Appeler Thomas Durand à la maison, …\" \n\"Appeler la messagerie vocale\" \n\"Composer le 01 23 45 67 89\" \n\"Composer le 15, 112, …\" \n\"Composer le +33 6 12 34 56 78\" \n\"Recomposer\" \n\"Ouvrir Google Agenda\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Reconnaissance vocale"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Appels par reconnaissance vocale via le Bluetooth"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Démarrage…"</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Écoute en cours…"</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Veuillez réessayer."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Impossible de procéder à l\'initialisation."</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Rallumez votre téléphone, puis réessayez."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Connexion au casque perdue"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" à la maison"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" sur le téléphone portable"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" au bureau"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" autre"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"La connexion à l\'application Reconnaissance vocale est activée."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"La connexion à l\'application Reconnaissance vocale est désactivée."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Parlez maintenant."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Aucun résultat, réessayez."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Choix incorrect"</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Au revoir"</string>

</resources>
