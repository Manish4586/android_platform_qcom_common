<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">వాయిస్ డయలర్</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">గ్రీన్ కాల్ బటన్‌ను\u2026</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">
        నొక్కి మరియు అలాగే పట్టుకోవడం ద్వారా వాయిస్ డయలర్‌ను తెరుస్తుందని
        మీకు తెలుసా.
    </string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">
        ఉదాహరణలు:
        <b>
            \n\"జాన్ డోకు కాల్ చేయి\"
            \n\"ఇంటి వద్ద జాన్ డోకు కాల్ చేయి, \u2026\"
            \n\"వాయిస్‌మెయిల్‌కు కాల్ చేయి\"
            \n\"(866) 555 0123కు డయల్ చేయి\"
            \n\"911, 811, కు డయల్ చేయి\u2026\"
            \n\"+44 7333 444 555\కు డయల్ చేయి"
            \n\"మళ్ళీ డయల్ చేయి\"
            \n\"క్యాలెండర్‌ను తెరువు\"
        </b>
    </string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">వాయిస్ డయలర్</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">Bluetooth వాయిస్ డయలర్</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">ప్రారంభిస్తుంది\u2026</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">వింటున్నారు\u2026</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">మళ్లీ ప్రయత్నించండి.</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">ప్రారంభించలేము</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">
         మీ ఫోన్ పునఃప్రారంభించి, మళ్లీ ప్రయత్నించండి.
    </string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">
      హెడ్‌సెట్‌కు అనుసంధానాన్ని కోల్పోయింది.
    </string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" ఇంటి వద్ద"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" మొబైల్‌లో"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" కార్యాలయంలో"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" ఇతరుల వద్ద"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">వాయిస్ డయలర్ లాగింగ్ ప్రారంభించబడింది.</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">వాయిస్ డయలర్ లాగింగ్ ఆపివేయబడింది.</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">ఇప్పుడు మాట్లాడండి.</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">ఫలితాలు లేవు, మళ్ళీ ప్రయత్నించండి.</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">చెల్లని ఎంపిక.</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">వెళ్ళొస్తాను.</string>

</resources>
