<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Discador por voz"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Você sabia..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Pressionar e manter pressionado o botão verde de chamada abre o Discador por voz."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Exemplos: "<b>" \n\"Chamar José da Silva\" \n\"Chamar José da Silva em casa, …\" \n\"Chamar correio de voz\" \n\"Discar (866) 555 0123\" \n\"Discar 911, 811, …\" \n\"Discar +44 7333 444 555\" \n\"Rediscar\" \n\"Abrir Agenda\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Discador por voz"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Discagem por voz Bluetooth"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Iniciando..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Ouvindo..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">Tente novamente.</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Não foi possível inicializar"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Reinicie o telefone e tente novamente."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Conexão com o fone de ouvido perdida."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" em casa"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" no celular"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" no trabalho"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" em outro número"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"O registro de discagem por voz está ativado."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"O registro de discagem por voz está desligado."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Fale agora."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Sem resultados. Tente novamente."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Opção inválida."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Adeus."</string>

</resources>
