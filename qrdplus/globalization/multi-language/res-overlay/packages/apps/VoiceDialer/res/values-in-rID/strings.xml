<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Aplikasi Telepon via Suara"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Mungkin Anda tahu..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Menekan &amp; menahan tombol Panggil warna hijau akan membuka Aplikasi Telepon via Suara."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Contoh: "<b>" \n\"Panggil Budi Setiawan\" \n\"Panggil John Doe di rumah, …\" \n\"Panggil kotak pesan\" \n\"Hubungi (866) 555 0123\" \n\"Hubungi 911, 811, …\" \n\"Hubungi +44 7333 444 555\" \n\"Hubungi Kembali\" \n\"Buka Kalender\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Aplikasi Telepon via Suara"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Aplikasi Telepon via Suara Bluetooth"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Memulai..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Mendengarkan…"</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Coba lagi."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Tidak dapat memulai"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Mulai ulang ponsel Anda lalu coba lagi."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Sambungan ke headset terputus."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" di rumah"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" pada nomor seluler"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" di kantor"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" di lainnya"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"Penyimpanan log diaktifkan untuk Aplikasi Telepon via Suara."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"Penyimpanan log dinonaktifkan untuk Aplikasi Telepon via Suara."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Bicara sekarang."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Tidak ada hasil, coba lagi."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Pilihan tidak valid."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Sampai Jumpa."</string>

</resources>
