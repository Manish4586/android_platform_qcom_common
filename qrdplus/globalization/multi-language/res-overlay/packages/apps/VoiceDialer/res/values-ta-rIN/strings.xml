<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">குரல் டயலர்</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">உங்களுக்குத் தெரியுமா\u2026</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">
        பச்சை நிற &amp;அழைப்பு பொத்தானை அழுத்திப் பிடிப்பதால்
        குரல் டயலர் திறக்கும்.
    </string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">
        எடுத்துக்காட்டுகள்:
        <b>
            \n\"ஜான் டோவை அழை\"
            \n\"வீட்டில் ஜான் டோவை அழை, \u2026\"
            \n\"குரல்அஞ்சல் அழை\"
            \n\"டயல் செய் (866) 555 0123\"
            \n\"டயல் செய் 911, 811, \u2026\"
            \n\"டயல் செய் +44 7333 444 555\"
            \n\"மீண்டும் டயல்செய்\"
            \n\"நாள்காட்டி திற\"
        </b>
    </string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">குரல் டயலர்</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">Bluetooth குரல் டயலர்</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">தொடங்குகிறது\u2026</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">கவனிக்கிறது\u2026</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">மீண்டும் முயற்சி செய்க.</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">தொடங்க முடியவில்லை</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">
        உங்கள் தொலைபேசியை மறுதொடக்கம் செய்து, பின் மீண்டும் முயற்சிசெய்க.
    </string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">
      தலையணிக்கான இணைப்பு இழக்கப்பட்டது.
    </string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" வீட்டில்"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" மொபைலில்"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" பணியில்"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" மற்றவையில்"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">குரல் டயலர் பதிவுசெய்வது இயக்கப்பட்டுள்ளது.</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">குரல் டயலர் பதிவுசெய்வது அணைக்கப்பட்டுள்ளது.</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">இப்போது பேசுங்கள்.</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">முடிவுகள் இல்லை, திரும்ப முயற்சிக்கவும்.</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">செல்லுபடியாகாத தேர்வு.</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">குட் பை.</string>

</resources>
