<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">आवाज डायलर</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">आपणम जाणता काय\u2026</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">
        हिरवे आणि कॉल बटण दाबूबन आणि धरुन ठेवल्याने आवाज डायलर
        उघडतो.
    </string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">
        उदाहरणे:
        <b>
            \n\"जॉन डोस कॉल करा\"
            \n\"जॉन डोस घरी कॉल करा, \u2026\"
            \n\"व्हॉइसमेल कॉल करा\"
            \n\"(866) 555 0123\ डायल करा"
            \n\"911, 811, \u2026 डायल करा\"
            \n\"+44 7333 444 555\ डायल करा"
            \n\"रीडायल करा\"
            \n\"दिनदर्शिका उघडा\"
        </b>
    </string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">आवाज डायलर</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">Bluetooth आवाज डायलर</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">सुरू करीत आहे\u2026</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">ऐकत आहे\u2026</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">पुन्हा प्रयत्न करा.</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">प्रारंभ करू शकलो नाही</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">
        फोन पुन्हा सुरू करा व नंतर प्रयत्न करा.
    </string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">
      हेडसेटचे कनेक्शन गमावले.
    </string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" घरी"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" मोबाइलवर"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" कार्यस्थानी"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" इतरत्र"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">व्हॉइस डायलर लॉगिंग चालू केले आहे.</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">व्हॉइस डायलर लॉगिंग बंद केले आहे.</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">आत्ता बोला.</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">परिणाम नाहीत, पुन्हा प्रयत्न करा.</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">अवैध पसंतनिवड अनुपलब्ध.</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">गुडबाय.</string>

</resources>
