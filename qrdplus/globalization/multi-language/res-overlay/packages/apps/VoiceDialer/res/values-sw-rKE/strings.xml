<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Piga kwa Kutamka"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Ulijua kuwa..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Kubonyeza &amp; kushikilia kitufe cha kijani cha Kupiga simu hufungua Kipigaji simu ya Sauti."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Mifano: "<b>" \n\"Pigia simu John Doe\" \n\"Pigia John Doe simu akiwa nyumbani, …\" \n\"Piga simu ya barua ya sauti\" \n\"Piga (866) 555 0123\" \n\"Piga 911, 811, …\" \n\"Piga +44 7333 444 555\" \n\"Piga tena\" \n\"Fungua Kalenda\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Kipiga simu ya Sauti"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Kipigaji cha Sauti cha Blurtooth"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Inaanza..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Inasikiliza..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Jaribu tena."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Haikuweza kuanzilisha"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Washa upya simu yako kisha  ujaribu tena."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Muunganisho wa kipaza sauti cha kichwa umepotea."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" nyumbani"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" kwenye simu ya mkononi"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" kazini"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" kwa nyingine"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"Kuweka kumbukumbu ya Kupiga kwa Kutamka kumewashwa."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"Kuweka kumbukumbu ya Kupiga kwa Kutamka kumezimwa."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Zungumza sasa."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Hakuna matokeo, jaribu tena."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Chaguo batili."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Kwaheri."</string>

</resources>
