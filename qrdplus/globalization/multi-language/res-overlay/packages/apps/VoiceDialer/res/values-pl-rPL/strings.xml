<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Wybieranie głosowe"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Czy wiesz, że..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Naciśnięcie i przytrzymanie zielonego przycisku otwiera aplikację Wybieranie głosowe."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Przykłady: "<b>" \n„Połącz z Janem Kowalskim” \n„Połącz z Janem Kowalskim w domu, …” \n„Połącz z pocztą głosową” \n„Wybierz (866) 555 0123” \n„Wybierz 911, 811, …” \n„Wybierz +44 7333 444 555” \n„Wybierz ponownie” \n„Otwórz Kalendarz” "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Wybieranie głosowe"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Wybieranie głosowe Bluetooth"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Uruchamianie..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Słuchanie..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Spróbuj ponownie."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Nie udało się zainicjować."</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Uruchom ponownie telefon i powtórz próbę."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Utracono połączenie z zestawem słuchawkowym."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" w domu"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" w telefonie komórkowym"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" w pracy"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" w innym miejscu"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"Rejestrowanie w Wybieraniu głosowym jest włączone."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"Rejestrowanie w Wybieraniu głosowym jest wyłączone."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Mów teraz."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Brak wyników. Spróbuj ponownie."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Nieprawidłowy wybór."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Do widzenia."</string>

</resources>
