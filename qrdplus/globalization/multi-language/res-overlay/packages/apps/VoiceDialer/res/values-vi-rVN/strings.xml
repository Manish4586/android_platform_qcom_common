<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Khẩu lệnh"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Bạn có biết…"</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Nhấn &amp; giữ nút Gọi màu xanh để mở Khẩu lệnh."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Ví dụ: "<b>" \n\"Gọi John Doe\" \n\"Gọi John Doe theo số nhà riêng, …\" \n\"Gọi thư thoại\" \n\"Quay số (866) 555 0123\" \n\"Quay số 911, 811, …\" \n\"Quay số +44 7333 444 555\" \n\"Quay số lại\" \n\"Mở Lịch\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Khẩu lệnh"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Khẩu lệnh qua Bluetooth"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Đang khởi động…"</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"Đang nghe…"</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Thử lại."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Không thể khởi chạy"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Hãy khởi động lại điện thoại của bạn, sau đó thử lại."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Mất kết nối đến tai nghe."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" theo số nhà riêng"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" theo số điện thoại di động"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" theo số cơ quan"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" theo số khác"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"Đã bật tính năng ghi nhật ký Khẩu lệnh."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"Đã tắt tính năng ghi nhật ký Khẩu lệnh."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Nói ngay."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Không có kết quả nào, hãy thử lại."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Lựa chọn không hợp lệ."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Tạm biệt."</string>

</resources>
