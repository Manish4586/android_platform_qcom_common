<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">المتصل الصوتي</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"هل تعلم..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"يؤدي الضغط مع الاستمرار على زر الاتصال الأخضر إلى فتح Voice Dialer."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" أمثلة: "<b>" \n\"الاتصال بعمار ياسر\" \n\"الاتصال بعمار ياسر بالمنزل، …\" \n\"الاتصال بالبريد الصوتي\" \n\"طلب 0123 555 ‏(866)‏\"\n\"طلب 911، 811، …\" \n\"طلب 555 444 7333 44+\" \n\"إعادة الطلب\" \n\"فتح التقويم\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">المتصل الصوتي</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Voice Dialer للبلوتوث"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"جارٍ التشغيل..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"جارٍ الاستماع..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"أعد المحاولة."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"تتعذر التهيئة"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"أعد تشغيل الهاتف، ثم أعد المحاولة."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"تم فقد الاتصال بسماعة الرأس."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" بالمنزل"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" بالجوال"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" بالعمل"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" برقم آخر"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"تم تشغيل تسجيل Voice Dialer."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"تم إيقاف تسجيل Voice Dialer."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"تحدث الآن."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"لا نتائج، حاول مرة أخرى."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"اختيار غير صالح"</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"وداعًا."</string>

</resources>
