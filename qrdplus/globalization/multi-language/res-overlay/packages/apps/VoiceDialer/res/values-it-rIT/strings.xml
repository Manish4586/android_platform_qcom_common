<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"Compositore vocale"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"Informazioni utili"</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"Premi e tieni premuto il tasto verde Chiama per aprire il compositore vocale."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" Esempi: "<b>" \n\"Chiama Mario Rossi\" \n\"Chiama Mario Rossi a casa, …\" \n\"Messaggio in segreteria\" \n\"Componi (866) 555 0123\" \n\"Componi 911, 811, …\" \n\"Componi +44 7333 444 555\" \n\"Ricomponi\" \n\"Apri Calendario\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"Compositore vocale"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"Compositore vocale Bluetooth"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"Avvio..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"In ascolto..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"Riprova."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"Inizializzazione non riuscita"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"Riavvia il telefono e riprova."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"Conness. ad auricolare persa."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" a casa"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" al cellulare"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" al lavoro"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" ad altro numero"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"La registrazione con Compositore vocale è attiva."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"La registrazione con Compositore vocale non è attiva."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"Parla ora."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"Nessun risultato, riprova."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"Scelta non valida."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"Arrivederci."</string>

</resources>
