<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">ಧ್ವನಿ ಡಯಲರ್</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">ನಿಮಗೆ ಗೊತ್ತೇ\u2026</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">
        ಹಸಿರು ಬಣ್ಣದ ಕರೆ ಬಟನ್ ಅನ್ನು ಒತ್ತಿ ಹಿಡಿದರೆ ಧ್ವನಿ ಡಯಲರ್ ತೆರೆದುಕೊಳ್ಳುತ್ತದೆ
.
    </string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">
        ಉದಾಹರಣೆಗಳು:
<b>
\n\"ಜಾನ್ ಡೋಗೆ ಕರೆಮಾಡಿ\"
\n\"ಮನೆಯಲ್ಲಿ ಜಾನ್ ಡೋಗೆ ಕರೆಮಾಡಿ, \u2026\"
\n\"ಕರೆ ಧ್ವನಿಮೇಲ್\"
\n\"(866) 555 0123\"
ಗೆ ಡಯಲ್ ಮಾಡಿ \n\" 911, 811, \u2026\"
ಗೆ ಡಯಲ್ ಮಾಡಿ \n\"+44 7333 444 555\"
ಗೆ ಡಯಲ್ ಮಾಡಿ \n\"ಮರುಡಯಲ್ ಮಾಡಿ\"
\n\"ಕ್ಯಾಲೆಂಡರ್ ತೆರೆಯಿರಿ\"
</b>
    </string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">ಧ್ವನಿ ಡಯಲರ್</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">ಬ್ಲೂಟೂತ್ ಧ್ವನಿ ಡಯಲರ್</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">ಪ್ರಾರಂಭಿಸಲಾಗುತ್ತಿದೆ\u2026</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">ಕೇಳಲಾಗುತ್ತಿದೆ\u2026</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">ಮತ್ತೊಮ್ಮೆ ಪ್ರಯತ್ನಿಸಿ.</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">ಪ್ರಾರಂಭಿಸಲು ಸಾಧ್ಯವಾಗುತ್ತಿಲ್ಲ</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">
        ನಿಮ್ಮ ಫೋನ್ ಮರುಪ್ರಾರಂಭಿಸಿ ನಂತರ ಮತ್ತೊಮ್ಮೆ ಪ್ರಯತ್ನಿಸಿ.
    </string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">
      ಹೆಡ್‌ಸೆಟ್ ಸಂಪರ್ಕ ಕಡಿತಗೊಂಡಿದೆ.
    </string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" ಮನೆಯಲ್ಲಿ"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" ಮೊಬೈಲ್‌ನಲ್ಲಿ"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" ಕೆಲಸದಲ್ಲಿ"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" ಇತರೆಡೆ"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">ಧ್ವನಿ ಡಯಲರ್ ಲಾಗಿಂಗ್ ಆನ್ ಆಗಿದೆ.</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">ಧ್ವನಿ ಡಯಲರ್ ಲಾಗಿಂಗ್ ಆಫ್ ಆಗಿದೆ.</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">ಈಗ ಮಾತನಾಡಿ.</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">ಯಾವುದೇ ಫಲಿತಾಂಶಗಳಿಲ್ಲ, ಮತ್ತೊಮ್ಮೆ ಪ್ರಯತ್ನಿಸಿ.</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">ಅಮಾನ್ಯ ಆಯ್ಕೆ</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">ಗುಡ್‌ಬೈ.</string>

</resources>
