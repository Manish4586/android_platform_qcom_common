<resources>
    <!-- The name of the Voice Dialer application. -->
    <string name="voiceDialer">"شماره‌گیر صوتی"</string>

    <!-- title to the Tool Tip window.  open when VoiceDialer not launched
        by green Call button -->
    <string name="tool_tip_title">"آیا می‌دانستید که..."</string>

    <!-- message in the Tool Tip window.  purpose is to notify user of green
        Call button launch -->
    <string name="tool_tip_message">"با فشردن و نگهداشتن کلید تماس سبز رنگ، شماره‌گیر صوتی باز می‌شود."</string>

    <!-- present on the VoiceDialer main screen to give examples of valid
            speech input -->
    <string name="examples">" مثال‌ها: "<b>" \n\"با جان دو تماس بگیر\" \n\"با خانه جان دو تماس بگیر، …\" \n\"با پست صوتی تماس بگیر\" \n\"شماره 0123 555 (866) را بگیر\" \n\"با شماره 911، 811، … تماس بگیر\" \n\"با شماره 555 444 7333 44+ تماس بگیر\" \n\"دوباره شماره بگیر\" \n\"تقویم را باز کن\" "</b></string>

    <!-- title to the VoiceDialer main screen -->
    <string name="title">"شماره‌گیر صوتی"</string>

    <!-- title to the VoiceDialer main screen -->
    <string name="bluetooth_title">"شماره‌گیر صوتی بلوتوث"</string>

    <!-- displayed on the VoiceDialer main screen while initializing -->
    <string name="initializing">"در حال راه‌اندازی..."</string>

    <!-- displayed on the VoiceDialer main screen while listening to speech -->
    <string name="listening">"در حال گوش کردن..."</string>

    <!-- displayed on the VoiceDialer main screen upon failure -->
    <string name="please_try_again">"دوباره امتحان کنید."</string>


    <!--displayed when there is a fatal error on initialization or
        recognition-->
    <string name="failure">"مقداردهی اولیه انجام نشد"</string>

    <!-- displayed on the VoiceDialer main screen after unrecoverable error.
            when present, this message follows the one above -->
    <string name="recognition_error">"تلفن خود را مجدداً راه‌اندازی کنید و دوباره امتحان کنید."</string>

    <!-- displayed when the connection tot he bluetooth headset is lost -->
    <string name="headset_connection_lost">"اتصال به هدست قطع شد."</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'home\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_home">" در خانه"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'mobile\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="on_mobile">" در تلفن همراه"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'work\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_work">" در محل کار"</string>

    <!-- appended to recognition result from VoiceDialer.grxml to
            indicate a \'other\' number.
            \'Call John Smith\' becomes \'Call John Smith at home\'  -->
    <string name="at_other">" در سایر مکان‌ها"</string>

    <!-- message in popup which indicates that VoiceDialer logging
            has been enabled -->
    <string name="logging_enabled">"گزارش‌گیری شماره‌گیر صوتی فعال شده است."</string>
    
    <!-- message in popup which indicates that VoiceDialer logging
            has been disabled -->
    <string name="logging_disabled">"گزارش‌گیری شماره‌گیر صوتی غیر فعال است."</string>

    <!-- message that is spoken to indicate that the user can begin speaking.
    -->
    <string name="speak_now_tts">"اکنون صحبت کنید."</string>

    <!-- message that is spoken when we get no results
    -->
    <string name="no_results_tts">"نتیجه‌ای یافت نشد، دوباره امتحان کنید."</string>

    <!-- message that is spoken when the user asks for a choice that
            is not available -->
    <string name="invalid_choice_tts">"انتخاب نامعتبر است."</string>

    <!-- message that is spoken to inform the user that the application
            is shutting down.-->
    <string name="goodbye_tts">"خداحافظ."</string>

</resources>
