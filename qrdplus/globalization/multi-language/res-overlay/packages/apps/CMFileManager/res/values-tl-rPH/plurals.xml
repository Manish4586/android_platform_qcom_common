<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <plurals name="n_folders">
        <item quantity="one"><xliff:g id="folders">%1$d</xliff:g> folder</item>
        <item quantity="other"><xliff:g id="folders">%1$d</xliff:g> (na) folder</item>
    </plurals>
    <plurals name="n_files">
        <item quantity="one"><xliff:g id="files">%1$d</xliff:g> file</item>
        <item quantity="other"><xliff:g id="files">%1$d</xliff:g> (na) file</item>
    </plurals>

    <!-- Search - Found items -->
    <plurals name="search_found_items">
        <item quantity="one"><xliff:g id="count">%1$d</xliff:g> item ang nakita</item>
        <item quantity="other"><xliff:g id="count">%d</xliff:g> (na) item ang nakita</item>
    </plurals>

    <!-- Statusbar text if there are selected files or folders -->
    <plurals name="selection_folders">
        <item quantity="one"><xliff:g id="folders">%1$d</xliff:g> folder ang pinili.</item>
        <item quantity="other"><xliff:g id="folders">%1$d</xliff:g> (na) folder ang pinili.</item>
    </plurals>
    <plurals name="selection_files">
        <item quantity="one"><xliff:g id="files">%1$d</xliff:g> file ang pinili.</item>
        <item quantity="other"><xliff:g id="files">%1$d</xliff:g> (na) file ang pinili.</item>
    </plurals>
</resources>
