<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <plurals name="n_folders">
        <item quantity="one"><xliff:g id="folders">%1$d</xliff:g> директоријум</item>
        <item quantity="other">Број директоријума: <xliff:g id="folders">%1$d</xliff:g></item>
    </plurals>
    <plurals name="n_files">
        <item quantity="one"><xliff:g id="files">%1$d</xliff:g> датотека</item>
        <item quantity="other">Број датотека: <xliff:g id="files">%1$d</xliff:g></item>
    </plurals>

    <!-- Search - Found items -->
    <plurals name="search_found_items">
        <item quantity="one"><xliff:g id="count">%1$d</xliff:g> ставка пронађена</item>
        <item quantity="other">Пронађено ставки: <xliff:g id="count">%d</xliff:g></item>
    </plurals>

    <!-- Statusbar text if there are selected files or folders -->
    <plurals name="selection_folders">
        <item quantity="one"><xliff:g id="folders">%1$d</xliff:g> директоријум изабран.</item>
        <item quantity="other">Изабрано директоријума: <xliff:g id="folders">%1$d</xliff:g></item>
    </plurals>
    <plurals name="selection_files">
        <item quantity="one"><xliff:g id="files">%1$d</xliff:g> датотека изабрана.</item>
        <item quantity="other">Изабрано датотека: <xliff:g id="files">%1$d</xliff:g></item>
    </plurals>
</resources>
