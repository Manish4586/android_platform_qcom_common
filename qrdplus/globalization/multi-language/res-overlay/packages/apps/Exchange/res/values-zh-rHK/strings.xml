    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_1day">"1 天"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_3days">"3 天"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_1week">"1 星期"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_2weeks">"兩星期"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_1month">"1 個月"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_all">"全部"</string>

    <string name="account_manager_type_exchange" translatable="false">com.android.exchange</string>
    <string name="authority_email_provider" translatable="false">com.android.email.provider</string>

    <!-- Title for notification warning about an authentication error [CHAR LIMIT=40]  -->
    <string name="auth_error_notification_title">"認證錯誤"</string>

    <!-- text for notification warning about an authentication error [CHAR LIMIT=60]  -->
    <string name="auth_error_notification_text">"輕觸即可編輯 <xliff:g id="ACCOUNT">%s</xliff:g> 的帳戶設定"</string>

    <!-- Message displayed when there is no application to handle the settings option. [CHAR LIMIT=NONE] -->
    <string name="email_settings_not_available">"重新啟用電子郵件應用程式，即可查看設定。"</string>

