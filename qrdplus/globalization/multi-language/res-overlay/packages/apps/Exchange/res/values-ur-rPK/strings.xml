    <!-- Message displayed when there is no application to handle the settings option. [CHAR LIMIT=NONE] -->
    <string name="email_settings_not_available">"ترتیبات دیکھنے کیلئے ای میل ایپلیکیشن کو دوبارہ فعال کریں۔"</string>

