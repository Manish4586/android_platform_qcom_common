<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Programma di installazione certificato"</string>

    <!-- Title of the file picker screen -->
    <string name="pick_file_title">"Scegli un certificato"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <string name="pkcs12_password_dialog_title">"Estrai certificato"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <string name="extracting_pkcs12">"Estrazione..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <string name="pkcs12_file_password_dialog_title">"Estrazione da %s"</string>
    <!-- Title of dialog to name a credential -->
    <string name="name_credential_dialog_title">"Specifica un nome per il certificato"</string>
    <!-- Description for the credential name input box -->
    <string name="credential_name">"Nome certificato:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <string name="credential_password">"Digita la password per l\'estrazione dei certificati."</string>
    <string name="credential_info">"Il pacchetto contiene:"</string>
    <string name="p12_description">"Certificati in keystore PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_userkey">"una chiave utente"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_usercrt">"un certificato utente"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_cacrt">"un certificato CA"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <string name="n_cacrts">"Certificati CA %d"</string>

    <string name="password_error">"Digita la password corretta."</string>
    <string name="password_empty_error">"Digita la password."</string>
    <string name="name_empty_error">"Digita un nome."</string>
    <string name="name_char_error">"Digita un nome contenente soltanto lettere e numeri."</string>
    <string name="storage_error">"Impossibile salvare il certificato. Tocca OK per riprovare."</string>
    <string name="unable_to_save_cert">"Impossibile salvare il certificato. L\'archivio credenziali non è attivo o non è stato inizializzato correttamente."</string>
    <string name="cert_not_saved">"Il certificato non è installato."</string>
    <string name="no_cert_to_saved">"Nessun certificato da installare."</string>
    <string name="invalid_cert">"Il certificato non è valido."</string>

    <!-- toast message -->
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> è installato."</string>
    <!-- toast message -->
    <string name="cert_too_large_error">"Installazione impossibile: dimensioni del certificato troppo grandi."</string>
    <!-- toast message -->
    <string name="cert_missing_error">"Installazione impossibile: file del certificato non trovato."</string>
    <!-- toast message -->
    <string name="cert_read_error">"Installazione impossibile: file del certificato non leggibile."</string>

    <!-- Shown when the certificate file can\'t be found.  [CHAR LIMIT=NONE] -->
    <string name="no_cert_file_found" product="nosdcard">"Nessun file di certificato trovato nell\'archivio USB."</string>
    <string name="no_cert_file_found" product="default">"Nessun file di certificato trovato sulla scheda SD."</string>
    <!-- Shown when USB storage can\'t be found to look for a certificate.  [CHAR LIMIT=30] -->
    <string name="sdcard_not_present" product="nosdcard">"Archivio USB non disponibile."</string>
    <string name="sdcard_not_present" product="default">"Scheda SD non presente."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <string name="only_primary_user_allowed">"Solo il proprietario di questo dispositivo può installare certificati."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <string name="credential_usage_label">"Utilizzo della credenziale:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <string-array name="credential_usage">
        <item>"VPN e app"</item>
        <item>"Wi-Fi"</item>
    </string-array>
</resources>
