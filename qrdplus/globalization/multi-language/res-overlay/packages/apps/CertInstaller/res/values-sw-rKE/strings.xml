<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Kisakinishaji Cheti"</string>

    <!-- Title of the file picker screen -->
    <string name="pick_file_title">"Chagua cheti"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <string name="pkcs12_password_dialog_title">"Chopoa cheti"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <string name="extracting_pkcs12">"Inadondoa..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <string name="pkcs12_file_password_dialog_title">"Chopoa kutoka kwa %s"</string>
    <!-- Title of dialog to name a credential -->
    <string name="name_credential_dialog_title">"Jina kwenye cheti"</string>
    <!-- Description for the credential name input box -->
    <string name="credential_name">"Jina la cheti:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <string name="credential_password">"Chapisha nenosiri ili kudondoa vyeti."</string>
    <string name="credential_info">"Furushi lina:"</string>
    <string name="p12_description">"Vyeti katika hifadhi-vitufe ya PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_userkey">"ufunguo wa mtumiaji mmoja"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_usercrt">"cheti cha mtumiaji mmoja"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_cacrt">"Cheti kimoja cha CA"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <string name="n_cacrts">"Vyeti vya %d CA"</string>

    <string name="password_error">"Chapa nenosiri sahihi."</string>
    <string name="password_empty_error">"Chapa nenosiri."</string>
    <string name="name_empty_error">"Chapa jina."</string>
    <string name="name_char_error">"Chapisha jina ambalo lina herufi na nambari tu."</string>
    <string name="storage_error">"Haikuweza kuhifadhi cheti. Gusa SAWA ili kujaribu tena."</string>
    <string name="unable_to_save_cert">"Haikuweza kuhifadhi cheti. Hifadhi ya hati haijawezeshwa au haijaanzilishwa vizuri."</string>
    <string name="cert_not_saved">"Cheti hakijasakinishwa."</string>
    <string name="no_cert_to_saved">"Hakuna cheti cha kusakinisha."</string>
    <string name="invalid_cert">"Cheti ni batili."</string>

    <!-- toast message -->
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> imesakinishwa."</string>
    <!-- toast message -->
    <string name="cert_too_large_error">"Haikuweza kusakinisha kwa sababu cheti ni kikubwa mno."</string>
    <!-- toast message -->
    <string name="cert_missing_error">"Haikuweza kusakinisha kwa sababu faili ya cheti haikupatikana."</string>
    <!-- toast message -->
    <string name="cert_read_error">"Haikuweza kusakinisha kwa sababu faili ya cheti haikuweza kusomwa."</string>

    <!-- Shown when the certificate file can\'t be found.  [CHAR LIMIT=NONE] -->
    <string name="no_cert_file_found" product="nosdcard">"Hakuna faili ya cheti iliyopatikana katika hifadhi ya USB."</string>
    <string name="no_cert_file_found" product="default">"Hakuni faili ya cheti iliyopatikana kwenye kadi ya SD."</string>
    <!-- Shown when USB storage can\'t be found to look for a certificate.  [CHAR LIMIT=30] -->
    <string name="sdcard_not_present" product="nosdcard">"Hifadhi ya USB haipatikani."</string>
    <string name="sdcard_not_present" product="default">"Kadi ya SD haipo."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <string name="only_primary_user_allowed">"Mmiliki wa kifaa hiki tu ndiye awezaye kusakinisha vyeti."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <string name="credential_usage_label">"Matumizi ya kitambulisho:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <string-array name="credential_usage">
        <item>"VPN na programu"</item>
        <item>"Wi-Fi"</item>
    </string-array>
</resources>
