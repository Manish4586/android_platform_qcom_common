<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Zertifikats-Installer"</string>

    <!-- Title of the file picker screen -->
    <string name="pick_file_title">"Zertifikat auswählen"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <string name="pkcs12_password_dialog_title">"Zertifikat extrahieren"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <string name="extracting_pkcs12">"Zertifikate werden extrahiert..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <string name="pkcs12_file_password_dialog_title">"Zertifikate aus %s extrahieren"</string>
    <!-- Title of dialog to name a credential -->
    <string name="name_credential_dialog_title">"Zertifikat benennen"</string>
    <!-- Description for the credential name input box -->
    <string name="credential_name">"Zertifikatname:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <string name="credential_password">"Geben Sie das Passwort zum Extrahieren der Zertifikate ein."</string>
    <string name="credential_info">"Das Paket enthält Folgendes:"</string>
    <string name="p12_description">"Zertifikate im PKCS12-Keystore"</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_userkey">"ein Nutzerschlüssel"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_usercrt">"ein Nutzerzertifikat"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_cacrt">"ein CA-Zertifikat"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <string name="n_cacrts">"%d CA-Zertifikate"</string>

    <string name="password_error">"Geben Sie das richtige Passwort ein."</string>
    <string name="password_empty_error">"Geben Sie das Passwort ein."</string>
    <string name="name_empty_error">"Geben Sie einen Namen ein."</string>
    <string name="name_char_error">"Geben Sie einen Namen ein, der ausschließlich aus Buchstaben und Zahlen besteht."</string>
    <string name="storage_error">"Zertifikat konnte nicht gespeichert werden. Zum Wiederholen auf \"OK\" tippen"</string>
    <string name="unable_to_save_cert">"Das Zertifikat konnte nicht gespeichert werden. Der Speicher für die Anmeldeinformationen wurde nicht aktiviert oder falsch initialisiert."</string>
    <string name="cert_not_saved">"Das Zertifikat ist nicht installiert."</string>
    <string name="no_cert_to_saved">"Kein zu installierendes Zertifikat"</string>
    <string name="invalid_cert">"Das Zertifikat ist ungültig."</string>

    <!-- toast message -->
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> ist installiert."</string>
    <!-- toast message -->
    <string name="cert_too_large_error">"Installation nicht möglich, weil das Zertifikat zu groß ist"</string>
    <!-- toast message -->
    <string name="cert_missing_error">"Installation nicht möglich, weil die Zertifikatdatei nicht gefunden wurde"</string>
    <!-- toast message -->
    <string name="cert_read_error">"Installation nicht möglich, weil die Zertifikatdatei nicht gelesen werden konnte"</string>

    <!-- Shown when the certificate file can\'t be found.  [CHAR LIMIT=NONE] -->
    <string name="no_cert_file_found" product="nosdcard">"Keine Zertifikatdatei auf USB-Speicher gefunden."</string>
    <string name="no_cert_file_found" product="default">"Auf der SD-Karte wurde keine Zertifikats-Datei gefunden."</string>
    <!-- Shown when USB storage can\'t be found to look for a certificate.  [CHAR LIMIT=30] -->
    <string name="sdcard_not_present" product="nosdcard">"Kein USB-Speicher verfügbar"</string>
    <string name="sdcard_not_present" product="default">"SD-Karte ist nicht vorhanden."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <string name="only_primary_user_allowed">"Nur der Eigentümer dieses Geräts kann Zertifikate installieren."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <string name="credential_usage_label">"Verwendung der Anmeldedaten:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <string-array name="credential_usage">
        <item>"VPN und Apps"</item>
        <item>"WLAN"</item>
    </string-array>
</resources>
