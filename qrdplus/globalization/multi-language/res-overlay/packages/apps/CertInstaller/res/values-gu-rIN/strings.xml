<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">પ્રમાણપત્ર ઇન્સ્ટોલર</string>

    <!-- Title of the file picker screen -->
    <string name="pick_file_title">પ્રમાણપત્ર પસંદ કરો</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <string name="pkcs12_password_dialog_title">પ્રમાણપત્ર એક્સટ્રેક્ટ કરો</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <string name="extracting_pkcs12">એક્સટ્રેક્ટ કરી રહ્યાં છે\u2026</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <string name="pkcs12_file_password_dialog_title">%s માંથી એક્સટ્રેક્ટ કરો</string>
    <!-- Title of dialog to name a credential -->
    <string name="name_credential_dialog_title">પ્રમાણપત્રને નામ આપો</string>
    <!-- Description for the credential name input box -->
    <string name="credential_name">પ્રમાણપત્રનું નામ:</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <string name="credential_password">પ્રમાણપત્રોને એક્સટ્રેક્ટ કરવા માટે પાસવર્ડ લખો.</string>
    <string name="credential_info">પેકેજમાં આનો સમાવેશ છે:</string>
    <string name="p12_description">PKCS12 કીસ્ટોરના પ્રમાણપત્રો.</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_userkey">એક ઉપયોગકર્તા કી</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_usercrt">એક ઉપયોગકર્તા પ્રમાણપત્ર</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_cacrt">એક CA પ્રમાણપત્ર</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <string name="n_cacrts">%d CA પ્રમાણપત્રો</string>

    <string name="password_error">સાચો પાસવર્ડ લખો.</string>
    <string name="password_empty_error">પાસવર્ડ લખો.</string>
    <string name="name_empty_error">એક નામ લખો.</string>
    <string name="name_char_error">પ્રમાણપત્ર નામમાં ફક્ત અક્ષરો અને સંખ્યાઓ જ સામેલ હોય છે.</string>
    <string name="storage_error">પ્રમાણપત્ર સાચવી શકાયું નથી. ફરી પ્રયાસ કરવા માટે ઓકે ટચ કરો.</string>
    <string name="unable_to_save_cert">પ્રમાણપત્ર સાચવી શકાયું નથી. ઓળખપત્ર સંગ્રહ સક્ષમ કરી શકાયો નથી અથવા યોગ્ય રીતે શરૂ કરાયેલ નથી.</string>
    <string name="cert_not_saved">પ્રમાણપત્ર ઇન્સ્ટોલ કરેલું નથી.</string>
    <string name="no_cert_to_saved">ઇન્સ્ટોલ કરવા માટે કોઈ પ્રમાણપત્ર નથી.</string>
    <string name="invalid_cert">પ્રમાણપત્ર અમાન્ય છે.</string>

    <!-- toast message -->
    <string name="cert_is_added"><xliff:g id="credential">%s</xliff:g> ઇન્સ્ટોલ કરેલું છે.</string>
    <!-- toast message -->
    <string name="cert_too_large_error">ઇન્સ્ટોલ કરી શકાયું નથી કારણ કે પ્રમાણપત્ર કદ ખૂબ મોટું છે.</string>
    <!-- toast message -->
    <string name="cert_missing_error">ઇન્સ્ટોલ કરી શકાયું નથી કારણ કે પ્રમાણપત્ર ફાઇલ શોધી શકાઇ નથી.</string>
    <!-- toast message -->
    <string name="cert_read_error">ઇન્સ્ટોલ કરી શકાયું નથી કારણ કે પ્રમાણપત્ર ફાઇલ વાંચી શકાઇ નથી.</string>

    <!-- Shown when the certificate file can\'t be found.  [CHAR LIMIT=NONE] -->
    <string name="no_cert_file_found" product="nosdcard">USB મેમરીમાં કોઈ પ્રમાણપત્ર ફાઇલ મળી નથી.</string>
    <string name="no_cert_file_found" product="default">SD કાર્ડમાં કોઈ પ્રમાણપત્ર ફાઇલ મળી નથી.</string>
    <!-- Shown when USB storage can\'t be found to look for a certificate.  [CHAR LIMIT=30] -->
    <string name="sdcard_not_present" product="nosdcard">USB સંગ્રહ ઉપલબ્ધ નથી.</string>
    <string name="sdcard_not_present" product="default">SD કાર્ડ ઉપલબ્ધ નથી.</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <string name="only_primary_user_allowed">ફક્ત આ ઉપકરણના માલિક જ પ્રમાણપત્રો ઇન્સ્ટોલ કરી શકશે.</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <string name="credential_usage_label">ઓળખપત્ર ઉપયોગમાં છે:</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <string-array name="credential_usage">
        <item>VPN અને એપ્લિકેશન્સ</item>
        <item>Wi-Fi</item>
    </string-array>
</resources>
