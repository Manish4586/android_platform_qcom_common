<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Instalador de certificado"</string>

    <!-- Title of the file picker screen -->
    <string name="pick_file_title">"Elegir un certificado"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <string name="pkcs12_password_dialog_title">"Extraer el certificado"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <string name="extracting_pkcs12">"Extrayendo…"</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <string name="pkcs12_file_password_dialog_title">"Extraer desde %s"</string>
    <!-- Title of dialog to name a credential -->
    <string name="name_credential_dialog_title">"Nombre del certificado"</string>
    <!-- Description for the credential name input box -->
    <string name="credential_name">"Nombre del certificado:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <string name="credential_password">"Escribe la contraseña para extraer los certificados."</string>
    <string name="credential_info">"El paquete contiene:"</string>
    <string name="p12_description">"Certificados en el keystore PKCS12."</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_userkey">"una clave de usuario"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_usercrt">"un certificado de usuario"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_cacrt">"un certificado de CA"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <string name="n_cacrts">"%d Certificados de CA"</string>

    <string name="password_error">"Escribe la contraseña correcta."</string>
    <string name="password_empty_error">"Escribe la contraseña."</string>
    <string name="name_empty_error">"Escribe un nombre."</string>
    <string name="name_char_error">"Escribe un nombre que solo contenga letras y números."</string>
    <string name="storage_error">"No fue posible guardar el certificado. Toca Aceptar para volver a intentarlo."</string>
    <string name="unable_to_save_cert">"No fue posible guardar el certificado. El almacenamiento de credenciales no se habilitó o inició correctamente."</string>
    <string name="cert_not_saved">"El certificado no se ha instalado."</string>
    <string name="no_cert_to_saved">"No ha certificado para instalar."</string>
    <string name="invalid_cert">"El certificado no es válido."</string>

    <!-- toast message -->
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> está instalado."</string>
    <!-- toast message -->
    <string name="cert_too_large_error">"No fue posible realizar la instalación porque el certificado es demasiado grande."</string>
    <!-- toast message -->
    <string name="cert_missing_error">"No fue posible realizar la instalación porque no se pudo ubicar el archivo del certificado."</string>
    <!-- toast message -->
    <string name="cert_read_error">"No fue posible realizar la instalación porque no se pudo leer el archivo del certificado."</string>

    <!-- Shown when the certificate file can\'t be found.  [CHAR LIMIT=NONE] -->
    <string name="no_cert_file_found" product="nosdcard">"No se ha encontrado un archivo de certificado en el almacenamiento USB."</string>
    <string name="no_cert_file_found" product="default">"No se ha encontrado un archivo de certificado en la tarjeta SD."</string>
    <!-- Shown when USB storage can\'t be found to look for a certificate.  [CHAR LIMIT=30] -->
    <string name="sdcard_not_present" product="nosdcard">"Alm. USB no disponible"</string>
    <string name="sdcard_not_present" product="default">"No se insertó la tarjeta SD."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <string name="only_primary_user_allowed">"Solo el propietario de este dispositivo puede instalar certificados."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <string name="credential_usage_label">"Uso de credencial:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <string-array name="credential_usage">
        <item>"VPN y aplicaciones"</item>
        <item>"Wi-Fi"</item>
    </string-array>
</resources>
