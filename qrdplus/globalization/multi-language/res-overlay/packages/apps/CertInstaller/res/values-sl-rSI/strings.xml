<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Namestitvena aplikacija za potrdilo"</string>

    <!-- Title of the file picker screen -->
    <string name="pick_file_title">"Izberite potrdilo"</string>

    <!-- Title of dialog to enter password for pkcs12 -->
    <string name="pkcs12_password_dialog_title">"Izvozi potrdilo"</string>
    <!-- message in progress bar when waiting for extracting certs from a pkcs12 package -->
    <string name="extracting_pkcs12">"Ekstrahiranje ..."</string>
    <!-- Title of dialog to enter password for pkcs12 file -->
    <string name="pkcs12_file_password_dialog_title">"Ekstrahiraj iz %s"</string>
    <!-- Title of dialog to name a credential -->
    <string name="name_credential_dialog_title">"Poimenovanje potrdila"</string>
    <!-- Description for the credential name input box -->
    <string name="credential_name">"Ime potrdila:"</string>
    <!-- Title of the credential info -->
    <!-- Description for the credential password input box -->
    <string name="credential_password">"Za ekstrahiranje potrdil vnesite geslo."</string>
    <string name="credential_info">"V tem paketu je:"</string>
    <string name="p12_description">"Potrdila v shrambi ključev PKCS12"</string>

    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_userkey">"en uporabniški ključ"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_usercrt">"potrdilo enega uporabnika"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE] -->
    <string name="one_cacrt">"eno overjeno potrdilo"</string>
    <!-- Item found in the PKCS12 keystore being investigated [CHAR LIMIT=NONE]-->
    <string name="n_cacrts">"%d overjenih potrdil"</string>

    <string name="password_error">"Vnesite pravilno geslo."</string>
    <string name="password_empty_error">"Vnesite geslo."</string>
    <string name="name_empty_error">"Vnesite ime."</string>
    <string name="name_char_error">"Vnesite ime, ki vsebuje samo črke in številke."</string>
    <string name="storage_error">"Potrdila ni bilo mogoče shraniti. Če želite poskusiti znova, se dotaknite možnosti »V redu«."</string>
    <string name="unable_to_save_cert">"Potrdila ni bilo mogoče shraniti. Shramba poverilnic ni omogočena ali pravilno inicializirana."</string>
    <string name="cert_not_saved">"Potrdilo ni nameščeno."</string>
    <string name="no_cert_to_saved">"Ni potrdila za namestitev."</string>
    <string name="invalid_cert">"Potrdilo ni veljavno."</string>

    <!-- toast message -->
    <string name="cert_is_added">"<xliff:g id="CREDENTIAL">%s</xliff:g> je nameščen."</string>
    <!-- toast message -->
    <string name="cert_too_large_error">"Namestitev ni uspela, ker je potrdilo preveliko."</string>
    <!-- toast message -->
    <string name="cert_missing_error">"Namestitev ni uspela, ker ni bilo mogoče najti datoteke s potrdilom."</string>
    <!-- toast message -->
    <string name="cert_read_error">"Namestitev ni uspela, ker ni bilo mogoče prebrati datoteke s potrdilom."</string>

    <!-- Shown when the certificate file can\'t be found.  [CHAR LIMIT=NONE] -->
    <string name="no_cert_file_found" product="nosdcard">"Na pomnilniku USB ni bilo mogoče najti datoteke s potrdilom."</string>
    <string name="no_cert_file_found" product="default">"Na kartici SD ni bilo mogoče najti datoteke s potrdilom."</string>
    <!-- Shown when USB storage can\'t be found to look for a certificate.  [CHAR LIMIT=30] -->
    <string name="sdcard_not_present" product="nosdcard">"Pomnilnik USB ni na voljo."</string>
    <string name="sdcard_not_present" product="default">"Ni kartice SD."</string>

    <!-- Message displayed when a user other than the owner on a multi-user system tries to
         install a certificate into the certificate store. [CHAR LIMIT=NONE] -->
    <string name="only_primary_user_allowed">"Samo lastnik sme namestiti potrdila."</string>

    <!-- Label for spinner that shows the possible usage for a credential. Shown when user is
         installing a credential [CHAR LIMIT=40] -->
    <string name="credential_usage_label">"Uporaba poverilnice:"</string>

    <!-- Usage type for a credential that the user is installing. The label will restrict the
         type of use for that credential. [CHAR LIMIT=40] -->
    <string-array name="credential_usage">
        <item>"Navidezno zasebno omrežje in aplikacije"</item>
        <item>"Wi-Fi"</item>
    </string-array>
</resources>
