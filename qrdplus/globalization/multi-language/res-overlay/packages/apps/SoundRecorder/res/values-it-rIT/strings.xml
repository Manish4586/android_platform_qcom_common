<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- application name and title of error dialogs-->
    <string name="app_name">"Registratore suoni"</string>
    <string name="folder_name">"Registrazione..."</string>
    <!-- Screen title before and during recording -->
    <string name="record_your_message">"Registra il messaggio"</string>
    <!-- Screen title after recording -->
    <string name="message_recorded">"Messaggio registrato"</string>
    <!-- Screen title while playing back a recording -->
    <string name="review_message">"Controlla messaggio"</string>
    <!-- Label shown during recording -->
    <string name="recording">"Registrazione..."</string>
    <!-- Label shown when recording is paused -->
    <string name="recording_paused">Pausa</string>
    <!-- Label shown when the recording is stopped for a reason other than the user stopping it (e.g. the sd card was removed) -->
    <string name="recording_stopped">"Registrazione interrotta"</string>
    <!-- label shown when there is not enough space to record something -->
    <string name="storage_is_full">"La memoria Ã¨ piena"</string>
    <!-- label shown when the recording has reached maximum allowed file size -->
    <string name="max_length_reached">"Lunghezza massima raggiunta"</string>
    <!-- label shown when there is no sd card available to record to [CHAR LIMIT=30] -->
    <string name="insert_sd_card" product="nosdcard">"Monta archivio USB"</string>
    <!-- label shown when there is no sd card available to record to -->
    <string name="insert_sd_card" product="default">"Inserisci una scheda SD"</string>
    <!-- label shown when there is more than 1 minute but less than 9 minutes of space left to record -->
    <string name="min_available">"<xliff:g id="MINUTES">%d</xliff:g> min a disposizione"</string>
    <!-- label shown when there is less than 1 minute of space left to record -->
    <string name="sec_available">"<xliff:g id="SECONDS">%d</xliff:g> s a disposizione"</string>

    <!-- button to accept the current recording and return it to the caller -->
    <string name="accept">"Fine"</string>
    <!-- button to discard the current recording and return to the caller -->
    <string name="discard">"Ignora"</string>
    <!-- acknowlegement button in a number of dialogs -->
    <string name="button_ok">"OK"</string>
    <!-- Do not translate. Format of the timer that shows how much has been recorded so far -->
    <string name="timer_format" translatable="false"><xliff:g id="format">%02d:%02d</xliff:g></string>
    <!-- label shown before the user has recorded anything -->
    <string name="press_record">Premere: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 9-AMR-WB A-AMR-WB-3GPP e premere Registra</string>
    <string name="press_record2">Premere Registra</string>
    <!-- label shown before the user has recorded anything -SSR support added-->
    <string name="press_record_ssr">Premere: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 7-AAC_SSR 8-LPCM_SSR 9-AMR-WB A-AMR-WB-3GPP e premere Registra</string>
   
    <!-- the name under which recordings will be visible in the media database is formatted like this --> 
    <string name="audio_db_title_format"><xliff:g id="format">aaaa-MM-gg HH:mm:ss</xliff:g></string>
    <!-- all recordings will show up in the media database with this \'artist\' name -->
    <string name="audio_db_artist_name">"Registrazioni"</string>
    <!-- all recordings will show up in the media database with this \'album\' name -->
    <string name="audio_db_album_name">"Registrazioni audio"</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <string name="audio_db_playlist_name">"Registrazioni personali"</string>
   
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card [CHAR LIMIT=NONE] -->
    <string name="error_sdcard_access" product="nosdcard">"Impossibile accedere all\'archivio USB."</string>
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card -->
    <string name="error_sdcard_access" product="default">"Impossibile accedere alla scheda SD."</string>
    <!-- shown as the message in a dialog when the app encountered an unspecified internal error -->
    <string name="error_app_internal">"Errore interno dell\'applicazione."</string>
    <!-- shown as the message in a dialog when the app encountered and unsupported format -->
    <string name="error_app_unsupported">Formato non supportato</string>
    <!-- shown as the message in a dialog when the recording could not be added to the media database -->
    <string name="error_mediadb_new_record">"Impossibile salvare l\'audio registrato."</string>
    <!-- shown as the message in a dialog when the recording cannot be started while in call -->
    <string name="error_mediadb_incall">Opzione supportata solo durante le chiamate</string>
    <!-- shown as the message in a dialog when voice call recording is started for AAC format -->
    <string name="error_mediadb_aacincall">Registrazione chiamata vocale AAC non supportata</string>
    <!-- Label shown during recording -->
    <string name="recording_now">Registrazione in corso...</string>
    <!-- shown as the message in a dialog when the recording file is saved successfully -->
    <string name="file_saved">File salvato</string>
    <!-- shown as the message in a dialog when the recording file is discarded -->
    <string name="file_discard">File ignorato</string>
    <!-- shown as the dialog title when user selects recording format -->
    <string name="format_setting">Tipo file</string>
    <string name="format_setting_aac_item">AAC</string>
    <string name="format_setting_amr_item">AMR</string>
    <string name="format_setting_3gpp_item">3GPP</string>
    <string name="format_setting_wav_item">WAV</string>
    <string name="storage_setting">Posizione archivio</string>
    <string name="storage_setting_sdcard_item">Scheda SD</string>
    <string name="storage_setting_local_item">Memoria telefono</string>
    <string name="no_phonestorage">Memoria telefono smontata</string>
    <string name="keyboard">Tastiera</string>
    <string name="file_deleted">File eliminato!</string>
    <string name="in_call_record_error">Errore di registrazione in fase di chiamata.</string>
</resources>
