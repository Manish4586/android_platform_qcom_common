<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- application name and title of error dialogs-->
    <string name="app_name">"Gravador de som"</string>
    <string name="folder_name">"Gravando"</string>
    <!-- Screen title before and during recording -->
    <string name="record_your_message">"Grave a sua mensagem"</string>
    <!-- Screen title after recording -->
    <string name="message_recorded">"Mensagem gravada"</string>
    <!-- Screen title while playing back a recording -->
    <string name="review_message">"Analisar mensagem"</string>
    <!-- Label shown during recording -->
    <string name="recording">"Gravando"</string>
    <!-- Label shown when recording is paused -->
    <string name="recording_paused">pausa</string>
    <!-- Label shown when the recording is stopped for a reason other than the user stopping it (e.g. the sd card was removed) -->
    <string name="recording_stopped">"Gravação interrompida"</string>
    <!-- label shown when there is not enough space to record something -->
    <string name="storage_is_full">"O armazenamento está cheio"</string>
    <!-- label shown when the recording has reached maximum allowed file size -->
    <string name="max_length_reached">"Tamanho máximo atingido"</string>
    <!-- label shown when there is no sd card available to record to [CHAR LIMIT=30] -->
    <string name="insert_sd_card" product="nosdcard">"Montar armazenamento USB"</string>
    <!-- label shown when there is no sd card available to record to -->
    <string name="insert_sd_card" product="default">"Inserir um cartão SD"</string>
    <!-- label shown when there is more than 1 minute but less than 9 minutes of space left to record -->
    <string name="min_available">"<xliff:g id="MINUTES">%d</xliff:g> minutos disponíveis"</string>
    <!-- label shown when there is less than 1 minute of space left to record -->
    <string name="sec_available">"<xliff:g id="SECONDS">%d</xliff:g> disponíveis"</string>

    <!-- button to accept the current recording and return it to the caller -->
    <string name="accept">"Concluído"</string>
    <!-- button to discard the current recording and return to the caller -->
    <string name="discard">"Descartar"</string>
    <!-- acknowlegement button in a number of dialogs -->
    <string name="button_ok">"OK"</string>
    <!-- Do not translate. Format of the timer that shows how much has been recorded so far -->
    <string name="timer_format" translatable="false"><xliff:g id="format">%02d:%02d</xliff:g></string>
    <!-- label shown before the user has recorded anything -->
    <string name="press_record">Pressione: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 9-AMR-WB A-AMR-WB-3GPP e pressione gravar</string>
    <string name="press_record2">Pressione gravar</string>
    <!-- label shown before the user has recorded anything -SSR support added-->
    <string name="press_record_ssr">Pressione: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 7-AAC_SSR 8-LPCM_SSR 9-AMR-WB A-AMR-WB-3GPP e pressione gravar</string>
   
    <!-- the name under which recordings will be visible in the media database is formatted like this --> 
    <string name="audio_db_title_format"><xliff:g id="format">HH:mm:ss dd/MM/yyyy</xliff:g></string>
    <!-- all recordings will show up in the media database with this \'artist\' name -->
    <string name="audio_db_artist_name">"Suas gravações"</string>
    <!-- all recordings will show up in the media database with this \'album\' name -->
    <string name="audio_db_album_name">"Gravações de áudio"</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <string name="audio_db_playlist_name">"Minhas gravações"</string>
   
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card [CHAR LIMIT=NONE] -->
    <string name="error_sdcard_access" product="nosdcard">"Não é possível acessar o armazenamento USB."</string>
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card -->
    <string name="error_sdcard_access" product="default">"Não é possível acessar o cartão SD."</string>
    <!-- shown as the message in a dialog when the app encountered an unspecified internal error -->
    <string name="error_app_internal">"Erro interno do app."</string>
    <!-- shown as the message in a dialog when the app encountered and unsupported format -->
    <string name="error_app_unsupported">"Formato incompatível"</string>
    <!-- shown as the message in a dialog when the recording could not be added to the media database -->
    <string name="error_mediadb_new_record">"Não foi possível salvar o áudio gravado."</string>
    <!-- shown as the message in a dialog when the recording cannot be started while in call -->
    <string name="error_mediadb_incall">"Esta opção é suportada somente quando está em chamada"</string>
    <!-- shown as the message in a dialog when voice call recording is started for AAC format -->
    <string name="error_mediadb_aacincall">"A gravação de chamada de voz AAC não é suportada"</string>
    <!-- Label shown during recording -->
    <string name="recording_now">Gravando ...</string>
    <!-- shown as the message in a dialog when the recording file is saved successfully -->
    <string name="file_saved">O arquivo foi salvo</string>
    <!-- shown as the message in a dialog when the recording file is discarded -->
    <string name="file_discard">O arquivo foi descartado</string>
    <!-- shown as the dialog title when user selects recording format -->
    <string name="format_setting">Tipo de arquivo</string>
    <string name="format_setting_aac_item">AAC</string>
    <string name="format_setting_amr_item">AMR</string>
    <string name="format_setting_3gpp_item">3GPP</string>
    <string name="format_setting_wav_item">WAV</string>
    <string name="storage_setting">Local armazenamento</string>
    <string name="storage_setting_sdcard_item">Cartão SD</string>
    <string name="storage_setting_local_item">Armazenamento do telefone</string>
    <string name="no_phonestorage">Armazenamento do telefone não instalado</string>
    <string name="keyboard">Teclado</string>
    <string name="file_deleted">O arquivo foi excluído.</string>
    <string name="in_call_record_error">Erro de gravação durante a ligação.</string>
</resources>
