<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- application name and title of error dialogs-->
    <string name="app_name">"ಧ್ವನಿ ರೆಕಾರ್ಡರ್"</string>
    <string name="folder_name">"ರೆಕಾರ್ಡ್ ಆಗುತ್ತಿದೆ"</string>
    <!-- Screen title before and during recording -->
    <string name="record_your_message">"ನಿಮ್ಮ ಸಂದೇಶವನ್ನು ರೆಕಾರ್ಡ್ ಮಾಡಿ"</string>
    <!-- Screen title after recording -->
    <string name="message_recorded">"ಸಂದೇಶ ರೆಕಾರ್ಡ್ ಆಗಿದೆ"</string>
    <!-- Screen title while playing back a recording -->
    <string name="review_message">"ಸಂದೇಶವನ್ನು ಪರಿಶೀಲಿಸಿ"</string>
    <!-- Label shown during recording -->
    <string name="recording">"ರೆಕಾರ್ಡ್ ಆಗುತ್ತಿದೆ"</string>
    <!-- Label shown when recording is paused -->
    <string name="recording_paused">ವಿರಾಮಗೊಳಿಸಿ</string>
    <!-- Label shown when the recording is stopped for a reason other than the user stopping it (e.g. the sd card was removed) -->
    <string name="recording_stopped">"ರೆಕಾರ್ಡಿಂಗ್ ನಿಂತಿದೆ"</string>
    <!-- label shown when there is not enough space to record something -->
    <string name="storage_is_full">"ಸಂಗ್ರಹಣೆ ತುಂಬಿದೆ"</string>
    <!-- label shown when the recording has reached maximum allowed file size -->
    <string name="max_length_reached">"ಗರಿಷ್ಠ ಮಿತಿ ತಲುಪಿದೆ"</string>
    <!-- label shown when there is no sd card available to record to [CHAR LIMIT=30] -->
    <string name="insert_sd_card" product="nosdcard">"USB ಸಂಗ್ರಹಣೆಯನ್ನು ಅಳವಡಿಸಿ"</string>
    <!-- label shown when there is no sd card available to record to -->
    <string name="insert_sd_card" product="default">"SD ಕಾರ್ಡ್ ಸೇರಿಸಿ"</string>
    <!-- label shown when there is more than 1 minute but less than 9 minutes of space left to record -->
    <string name="min_available">"<xliff:g id="MINUTES">%d</xliff:g> ನಿಮಿಷ ಲಭ್ಯ"</string>
    <!-- label shown when there is less than 1 minute of space left to record -->
    <string name="sec_available">"<xliff:g id="SECONDS">%d</xliff:g> ಸೆಕೆಂಡುಗಳು ಲಭ್ಯ"</string>

    <!-- button to accept the current recording and return it to the caller -->
    <string name="accept">"ಮುಗಿದಿದೆ"</string>
    <!-- button to discard the current recording and return to the caller -->
    <string name="discard">"ತಿರಸ್ಕರಿಸು"</string>
    <!-- acknowlegement button in a number of dialogs -->
    <string name="button_ok">"ಸರಿ"</string>
    <!-- Do not translate. Format of the timer that shows how much has been recorded so far -->
    <string name="timer_format" translatable="false"><xliff:g id="format">%02d:%02d</xliff:g></string>
    <!-- label shown before the user has recorded anything -->
    <string name="press_record">ಇದನ್ನು ಒತ್ತಿ: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 9-AMR-WB A-AMR-WB-3GPP ಮತ್ತು ರೆಕಾರ್ಡ್ ಒತ್ತಿ</string>
    <string name="press_record2">ರೆಕಾರ್ಡ್ ಒತ್ತಿ</string>
    <!-- label shown before the user has recorded anything -SSR support added-->
    <string name="press_record_ssr">ಇದನ್ನು ಒತ್ತಿ: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 7-AAC_SSR 8-LPCM_SSR 9-AMR-WB A-AMR-WB-3GPP ಮತ್ತು ರೆಕಾರ್ಡ್ ಒತ್ತಿ</string>
   
    <!-- the name under which recordings will be visible in the media database is formatted like this --> 
    <string name="audio_db_title_format"><xliff:g id="format">yyyy-MM-dd HH:mm:ss</xliff:g></string>
    <!-- all recordings will show up in the media database with this \'artist\' name -->
    <string name="audio_db_artist_name">"ನಿಮ್ಮ ರೆಕಾರ್ಡಿಂಗ್‌ಗಳು"</string>
    <!-- all recordings will show up in the media database with this \'album\' name -->
    <string name="audio_db_album_name">"ಆಡಿಯೊ ರೆಕಾರ್ಡಿಂಗ್‌ಗಳು"</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <string name="audio_db_playlist_name">"ನನ್ನ ರೆಕಾರ್ಡಿಂಗ್‌ಗಳು"</string>
   
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card [CHAR LIMIT=NONE] -->
    <string name="error_sdcard_access" product="nosdcard">"USB ಸಂಗ್ರಹಣೆಯನ್ನು ಪ್ರವೇಶಿಸಲು ಸಾಧ್ಯವಾಗುತ್ತಿಲ್ಲ."</string>
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card -->
    <string name="error_sdcard_access" product="default">"SD ಕಾರ್ಡ್ ಪ್ರವೇಶಿಸಲು ಸಾಧ್ಯವಾಗುತ್ತಿಲ್ಲ."</string>
    <!-- shown as the message in a dialog when the app encountered an unspecified internal error -->
    <string name="error_app_internal">"ಆಂತರಿಕ ಅಪ್ಲಿಕೇಶನ್ ದೋಷ."</string>
    <!-- shown as the message in a dialog when the app encountered and unsupported format -->
    <string name="error_app_unsupported">ಬೆಂಬಲಿಸದ ಸ್ವರೂಪ</string>
    <!-- shown as the message in a dialog when the recording could not be added to the media database -->
    <string name="error_mediadb_new_record">"ರೆಕಾರ್ಡ್ ಮಾಡಿರುವ ವೀಡಿಯೊವನ್ನು ಉಳಿಸಲು ಸಾಧ್ಯವಾಗುತ್ತಿಲ್ಲ."</string>
    <!-- shown as the message in a dialog when the recording cannot be started while in call -->
    <string name="error_mediadb_incall">ಕರೆಯಲ್ಲಿ ನಿರತರಾಗಿರುವಾಗ ಮಾತ್ರ ಈ ಆಯ್ಕೆ ಬೆಂಬಲಿಸುತ್ತದೆ</string>
    <!-- shown as the message in a dialog when voice call recording is started for AAC format -->
    <string name="error_mediadb_aacincall">AAC ಧ್ವನಿ ಕರೆ ರೆಕಾರ್ಡಿಂಗ್ ಬೆಂಬಲಿಸುವುದಿಲ್ಲ</string>
    <!-- Label shown during recording -->
    <string name="recording_now">ಇದೀಗ ರೆಕಾರ್ಡ್ ಆಗುತ್ತಿದೆ...</string>
    <!-- shown as the message in a dialog when the recording file is saved successfully -->
    <string name="file_saved">ಫೈಲ್ ಉಳಿಸಲಾಗಿದೆ</string>
    <!-- shown as the message in a dialog when the recording file is discarded -->
    <string name="file_discard">ಫೈಲ್ ತ್ಯಜಿಸಲಾಗಿದೆ</string>
    <!-- shown as the dialog title when user selects recording format -->
    <string name="format_setting">ಫೈಲ್‌ ಪ್ರಕಾರ</string>
    <string name="format_setting_aac_item">AAC</string>
    <string name="format_setting_amr_item">AMR</string>
    <string name="format_setting_3gpp_item">3GPP</string>
    <string name="format_setting_wav_item">WAV</string>
    <string name="storage_setting">ಸಂಗ್ರಹಣಾ ಸ್ಥಳ</string>
    <string name="storage_setting_sdcard_item">SD ಕಾರ್ಡ್</string>
    <string name="storage_setting_local_item">ಫೋನ್ ಸಂಗ್ರಹಣೆ</string>
    <string name="no_phonestorage">ಫೋನ್ ಸಂಗ್ರಹಣೆಯನ್ನು ಅನ್‌ಮೌಂಟ್ ಮಾಡಲಾಗಿದೆ</string>
    <string name="keyboard">ಕೀಬೋರ್ಡ್</string>
    <string name="file_deleted">ಫೈಲ್ ಅಳಿಸಲಾಗಿದೆ!</string>
    <string name="in_call_record_error">ಒಳ ಕರೆಯಲ್ಲಿ ರೆಕಾರ್ಡ್ ದೋಷ.</string>
</resources>
