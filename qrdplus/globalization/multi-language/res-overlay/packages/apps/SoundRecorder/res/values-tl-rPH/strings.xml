<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- application name and title of error dialogs-->
    <string name="app_name">"Recorder ng Tunog"</string>
    <string name="folder_name">"Nagre-record"</string>
    <!-- Screen title before and during recording -->
    <string name="record_your_message">"I-record ang iyong mensahe"</string>
    <!-- Screen title after recording -->
    <string name="message_recorded">"Na-record ang mensahe"</string>
    <!-- Screen title while playing back a recording -->
    <string name="review_message">"I-review ang mensahe"</string>
    <!-- Label shown during recording -->
    <string name="recording">"Nagre-record"</string>
    <!-- Label shown when recording is paused -->
    <string name="recording_paused">i-pause</string>
    <!-- Label shown when the recording is stopped for a reason other than the user stopping it (e.g. the sd card was removed) -->
    <string name="recording_stopped">"Itinigil ang pag-record"</string>
    <!-- label shown when there is not enough space to record something -->
    <string name="storage_is_full">"Puno na ang imbakan"</string>
    <!-- label shown when the recording has reached maximum allowed file size -->
    <string name="max_length_reached">"Naabot na ang maximum na haba"</string>
    <!-- label shown when there is no sd card available to record to [CHAR LIMIT=30] -->
    <string name="insert_sd_card" product="nosdcard">"I-mount ang imbakan ng USB"</string>
    <!-- label shown when there is no sd card available to record to -->
    <string name="insert_sd_card" product="default">"Magpasok ng SD card"</string>
    <!-- label shown when there is more than 1 minute but less than 9 minutes of space left to record -->
    <string name="min_available">"<xliff:g id="MINUTES">%d</xliff:g> min ang available"</string>
    <!-- label shown when there is less than 1 minute of space left to record -->
    <string name="sec_available">"<xliff:g id="SECONDS">%d</xliff:g>s ang available"</string>

    <!-- button to accept the current recording and return it to the caller -->
    <string name="accept">"Tapos na"</string>
    <!-- button to discard the current recording and return to the caller -->
    <string name="discard">"Itapon"</string>
    <!-- acknowlegement button in a number of dialogs -->
    <string name="button_ok">"OK"</string>
    <!-- Do not translate. Format of the timer that shows how much has been recorded so far -->
    <string name="timer_format" translatable="false"><xliff:g id="format">%02d:%02d</xliff:g></string>
    <!-- label shown before the user has recorded anything -->
    <string name="press_record">Pindutin ang: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 9-AMR-WB A-AMR-WB-3GPP at Pindutin ang i-record</string>
    <string name="press_record2">Pindutin ang i-record</string>
    <!-- label shown before the user has recorded anything -SSR support added-->
    <string name="press_record_ssr">Pindutin ang: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 7-AAC_SSR 8-LPCM_SSR 9-AMR-WB A-AMR-WB-3GPP at Pindutin ang i-record</string>
   
    <!-- the name under which recordings will be visible in the media database is formatted like this --> 
    <string name="audio_db_title_format"><xliff:g id="format">MM/dd/yyyy HH:mm:ss</xliff:g></string>
    <!-- all recordings will show up in the media database with this \'artist\' name -->
    <string name="audio_db_artist_name">"Iyong mga pag-record"</string>
    <!-- all recordings will show up in the media database with this \'album\' name -->
    <string name="audio_db_album_name">"Mga pag-record ng audio"</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <string name="audio_db_playlist_name">"Aking mga pag-record"</string>
   
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card [CHAR LIMIT=NONE] -->
    <string name="error_sdcard_access" product="nosdcard">"Hindi ma-access ang imbakan ng USB."</string>
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card -->
    <string name="error_sdcard_access" product="default">"Hindi ma-access ang SD card."</string>
    <!-- shown as the message in a dialog when the app encountered an unspecified internal error -->
    <string name="error_app_internal">"Panloob na error sa application."</string>
    <!-- shown as the message in a dialog when the app encountered and unsupported format -->
    <string name="error_app_unsupported">"Di-suportadong format"</string>
    <!-- shown as the message in a dialog when the recording could not be added to the media database -->
    <string name="error_mediadb_new_record">"Hindi mai-save na-record na audio."</string>
    <!-- shown as the message in a dialog when the recording cannot be started while in call -->
    <string name="error_mediadb_incall">"Ang opsyon na ito ay suportado, habang tumatawag lamang"</string>
    <!-- shown as the message in a dialog when voice call recording is started for AAC format -->
    <string name="error_mediadb_aacincall">"AAC voice call recording ay hindi suportado"</string>
    <!-- Label shown during recording -->
    <string name="recording_now">Nagre-record ngayon ...</string>
    <!-- shown as the message in a dialog when the recording file is saved successfully -->
    <string name="file_saved">File ay nai-save</string>
    <!-- shown as the message in a dialog when the recording file is discarded -->
    <string name="file_discard">Naitapon na ang file</string>
    <!-- shown as the dialog title when user selects recording format -->
    <string name="format_setting">Uri ng file</string>
    <string name="format_setting_aac_item">AAC</string>
    <string name="format_setting_amr_item">AMR</string>
    <string name="format_setting_3gpp_item">3GPP</string>
    <string name="format_setting_wav_item">WAV</string>
    <string name="storage_setting">Lokasyon ng storage</string>
    <string name="storage_setting_sdcard_item">SD card</string>
    <string name="storage_setting_local_item">Storage ng telepono</string>
    <string name="no_phonestorage">Naka-unmount ang storage ng telepono</string>
    <string name="keyboard">Keyboard</string>
    <string name="file_deleted">Natanggal na ang File!</string>
    <string name="in_call_record_error">Error sa pag-record sa tawag.</string>
</resources>
