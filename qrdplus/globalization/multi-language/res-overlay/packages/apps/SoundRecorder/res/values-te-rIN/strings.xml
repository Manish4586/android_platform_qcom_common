<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- application name and title of error dialogs-->
    <string name="app_name">"ధ్వని రికార్డర్"</string>
    <string name="folder_name">"రికార్డ్ చేస్తోంది"</string>
    <!-- Screen title before and during recording -->
    <string name="record_your_message">"మీ సందేశాన్ని రికార్డ్ చేయండి"</string>
    <!-- Screen title after recording -->
    <string name="message_recorded">"సందేశం రికార్డ్ చేయబడింది"</string>
    <!-- Screen title while playing back a recording -->
    <string name="review_message">"సందేశాన్ని సమీక్షించండి"</string>
    <!-- Label shown during recording -->
    <string name="recording">"రికార్డ్ చేస్తోంది"</string>
    <!-- Label shown when recording is paused -->
    <string name="recording_paused">ఫాజ్ చేయి</string>
    <!-- Label shown when the recording is stopped for a reason other than the user stopping it (e.g. the sd card was removed) -->
    <string name="recording_stopped">"రికార్డింగ్ ఆపివేయబడింది"</string>
    <!-- label shown when there is not enough space to record something -->
    <string name="storage_is_full">"నిల్వ నిండింది"</string>
    <!-- label shown when the recording has reached maximum allowed file size -->
    <string name="max_length_reached">"గరిష్ట పరిమితిని చేరుకుంది"</string>
    <!-- label shown when there is no sd card available to record to [CHAR LIMIT=30] -->
    <string name="insert_sd_card" product="nosdcard">"USB నిల్వను మౌంట్ చేయండి"</string>
    <!-- label shown when there is no sd card available to record to -->
    <string name="insert_sd_card" product="default">"SD కార్డును చొప్పించండి"</string>
    <!-- label shown when there is more than 1 minute but less than 9 minutes of space left to record -->
    <string name="min_available">"<xliff:g id="MINUTES">%d</xliff:g> నిమిషాల సమయం అందుబాటులో ఉంది"</string>
    <!-- label shown when there is less than 1 minute of space left to record -->
    <string name="sec_available">"<xliff:g id="SECONDS">%d</xliff:g>సెకన్ల సమయం అందుబాటులో ఉంది"</string>

    <!-- button to accept the current recording and return it to the caller -->
    <string name="accept">"పూర్తయింది"</string>
    <!-- button to discard the current recording and return to the caller -->
    <string name="discard">"విస్మరించు"</string>
    <!-- acknowlegement button in a number of dialogs -->
    <string name="button_ok">"సరే"</string>
    <!-- Do not translate. Format of the timer that shows how much has been recorded so far -->
    <string name="timer_format" translatable="false"><xliff:g id="format">%02d:%02d</xliff:g></string>
    <!-- label shown before the user has recorded anything -->
    <string name="press_record">నొక్కండి: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 9-AMR-WB A-AMR-WB-3GPP మరియు రికార్డ్ నొక్కండి</string>
    <string name="press_record2">రికార్డ్‌ను నొక్కండి</string>
    <!-- label shown before the user has recorded anything -SSR support added-->
    <string name="press_record_ssr">నొక్కండి: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP
6-AAC_MP4 7-AAC_SSR 8-LPCM_SSR 9-AMR-WB A-AMR-WB-3GPP మరియు రికార్డ్ నొక్కండి</string>
   
    <!-- the name under which recordings will be visible in the media database is formatted like this --> 
    <string name="audio_db_title_format"><xliff:g id="format">dd-MM-yyyy HH:mm:ss</xliff:g></string>
    <!-- all recordings will show up in the media database with this \'artist\' name -->
    <string name="audio_db_artist_name">"మీ రికార్డింగ్‌లు"</string>
    <!-- all recordings will show up in the media database with this \'album\' name -->
    <string name="audio_db_album_name">"ఆడియో రికార్డింగ్‌లు"</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <string name="audio_db_playlist_name">"నా రికార్డింగ్‌లు"</string>
   
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card [CHAR LIMIT=NONE] -->
    <string name="error_sdcard_access" product="nosdcard">"USB నిల్వను ప్రాప్యత చేయడం సాధ్యపడలేదు."</string>
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card -->
    <string name="error_sdcard_access" product="default">"SD కార్డుని ప్రాప్యత చేయడం సాధ్యపడలేదు."</string>
    <!-- shown as the message in a dialog when the app encountered an unspecified internal error -->
    <string name="error_app_internal">"అంతర్గత అనువర్తన లోపం."</string>
    <!-- shown as the message in a dialog when the app encountered and unsupported format -->
    <string name="error_app_unsupported">మద్దతివ్వని ఆకృతి</string>
    <!-- shown as the message in a dialog when the recording could not be added to the media database -->
    <string name="error_mediadb_new_record">"రికార్డ్ చేసిన ఆడియోను సేవ్ చేయడం సాధ్యపడలేదు."</string>
    <!-- shown as the message in a dialog when the recording cannot be started while in call -->
    <string name="error_mediadb_incall">కాల్‌లో ఉన్నప్పుడు మాత్రమే ఈ ఎంపిక మద్దతిస్తుంది</string>
    <!-- shown as the message in a dialog when voice call recording is started for AAC format -->
    <string name="error_mediadb_aacincall">AAC వాయిస్ కాల్ రికార్డింగ్ మద్దతివ్వడం లేదు</string>
    <!-- Label shown during recording -->
    <string name="recording_now">ఇప్పుడు రికార్డ్ చేస్తోంది ...</string>
    <!-- shown as the message in a dialog when the recording file is saved successfully -->
    <string name="file_saved">ఫైల్ సేవ్ చేయబడింది</string>
    <!-- shown as the message in a dialog when the recording file is discarded -->
    <string name="file_discard">ఫైల్ తీసివేయబడింది</string>
    <!-- shown as the dialog title when user selects recording format -->
    <string name="format_setting">ఫైల్ రకం</string>
    <string name="format_setting_aac_item">AAC</string>
    <string name="format_setting_amr_item">AMR</string>
    <string name="format_setting_3gpp_item">3GPP</string>
    <string name="format_setting_wav_item">WAV</string>
    <string name="storage_setting">నిల్వ స్థానం</string>
    <string name="storage_setting_sdcard_item">SD కార్డు</string>
    <string name="storage_setting_local_item">ఫోన్ నిల్వ</string>
    <string name="no_phonestorage">ఫోన్ నిల్వ అన్‌మౌంట్ చేయబడింది</string>
    <string name="keyboard">కీబోర్డ్</string>
    <string name="file_deleted">ఫైల్ తొలగించబడింది!</string>
    <string name="in_call_record_error">కాల్ సమయంలో రికార్డ్ దోషం.</string>
</resources>
