<resources>
    <!-- Default Application label used on home screen with an icon -->
    <string name="app_name">SIM ଟୁଲକିଟ୍</string>
    <!-- Application label used on home screen with an icon2 -->
    <string name="app_name2">SIM ଟୁଲକିଟ୍2</string>
    <!-- Application label used on home screen with an icon3 -->
    <string name="app_name3">SIM ଟୁଲକିଟ୍3</string>
    <!-- Menu button option for ending current SIM session -->
    <string name="menu_end_session">ଅଧିବେଶନ ଶେଷ କରନ୍ତୁ</string>
    <!-- Menu button option for requesting additional help information -->
    <string name="help">ସହାୟତା</string>
    <!-- Menu button option to navigate back to the previous screen -->
    <string name="menu_back">ବ୍ୟାକ୍</string>
    <!-- Settings entry which describes the current SIM Toolkit service on the device -->
    <string name="service_name">ସେବା ନାମ</string>
    <!-- Default description for service_name when no STK service is available -->
    <string name="stk_no_service">କୌଣସି ସେବା ଉପଲବ୍ଧ ନାହିଁ।</string>
    <!-- OK button used to confirm a message dialog -->
    <string name="button_ok">ଓକେ</string>
    <!-- Cancel button used to dismiss a message dialog and terminate the current SIM session-->
    <string name="button_cancel">ବାତିଲ୍</string>
    <!-- Yes button, used on a Yes/No confirmation screen -->
    <string name="button_yes">ହଁ</string>
    <!-- No button, used on a Yes/No confirmation screen -->
    <string name="button_no">ନା</string>
    <!-- Description for the type of text allowed in a form: alphanumeric or digits only -->
    <string name="alphabet">ବର୍ଣ୍ଣଗୁଡିକ</string>
    <!-- Type of alphabet text allowed in a form: 0-9, *, #, + characters only -->
    <string name="digits">ଅଙ୍କଗୁଡିକ (0-9, *, #, +)</string>
    <!-- Default transition message when launching a call -->
    <string name="default_call_setup_msg">କଲ୍ ପ୍ରଗତିରେ ଅଛି\u2026</string>
    <!-- Default message for SET_UP_CALL when there is no ALPHA tag in it -->
    <string name="default_setup_call_msg">ସେଟଅପ୍ କରାଯାଉଥିବା କଲ୍</string>

    <string name="stk_app_state">ଆପ୍ଲିକେସନ୍ ଷ୍ଟେଟ୍</string>
    <string name="enable_app">ସକ୍ଷମ ହୋଇଛି</string>
    <string name="disable_app">ଅକ୍ଷମ ହୋଇଛି</string>

    <!-- Title text used for system dialog from STK app -->
    <string name="stk_dialog_title">SIM ଟୁଲକିଟ୍</string>
    <!-- Default message when no alpha tag in Play Tone -->
    <string name="default_tone_dialog_msg">ଚାଳନ ହେଉଥିବା ଟୋନ୍</string>
    <!-- Default message when no alpha in OPEN_CHANNEL -->
    <string name="default_open_channel_msg">ଚ୍ୟାନେଲ୍ ଖୋଲନ୍ତୁ?</string>
    <!-- Default message when no alpha in SEND_DATA -->
    <string name="default_send_data_msg">ଡାଟା ପଠାଉଛି</string>
    <!-- Default message when no alpha in RECEIVE_DATA -->
    <string name="default_receive_data_msg">ଡାଟା ପ୍ରାପ୍ତ କରୁଛି</string>
    <!-- Default message when no alpha in CLOSE_CHANNEL-->
    <string name="default_close_channel_msg">ବନ୍ଦ ହୋଇଥିବା ଚ୍ୟାନେଲ୍</string>
    <!-- Button texts used in STK dialog -->
    <string name="stk_dialog_accept">ହଁ</string>
    <string name="stk_dialog_reject">ନା</string>

    <!-- Permission required for receiving intents from StkAppService-->
    <string name="stk_intent_permission">STK ଇନଟେଣ୍ଟସ୍ ଅନୁମତି ଦିଅନ୍ତୁ </string>
</resources>
