<resources>
    <!-- Default Application label used on home screen with an icon -->
    <string name="app_name">"Toolkit do SIM"</string>
    <!-- Application label used on home screen with an icon2 -->
    <string name="app_name2">Toolkit do SIM 2</string>
    <!-- Application label used on home screen with an icon3 -->
    <string name="app_name3">Toolkit do SIM 3</string>
    <!-- Menu button option for ending current SIM session -->
    <string name="menu_end_session">"Finalizar a sessão"</string>
    <!-- Menu button option for requesting additional help information -->
    <string name="help">"Ajuda"</string>
    <!-- Menu button option to navigate back to the previous screen -->
    <string name="menu_back">"Voltar"</string>
    <!-- Settings entry which describes the current SIM Toolkit service on the device -->
    <string name="service_name">"Nome do serviço"</string>
    <!-- Default description for service_name when no STK service is available -->
    <string name="stk_no_service">"Nenhum serviço disponível"</string>
    <!-- OK button used to confirm a message dialog -->
    <string name="button_ok">"OK"</string>
    <!-- Cancel button used to dismiss a message dialog and terminate the current SIM session-->
    <string name="button_cancel">Cancelar</string>
    <!-- Yes button, used on a Yes/No confirmation screen -->
    <string name="button_yes">"Sim"</string>
    <!-- No button, used on a Yes/No confirmation screen -->
    <string name="button_no">"Não"</string>
    <!-- Description for the type of text allowed in a form: alphanumeric or digits only -->
    <string name="alphabet">"Alfabeto"</string>
    <!-- Type of alphabet text allowed in a form: 0-9, *, #, + characters only -->
    <string name="digits">"Dígitos (0 a 9, *, #, +)"</string>
    <!-- Default transition message when launching a call -->
    <string name="default_call_setup_msg">"Chamada em andamento..."</string>
    <!-- Default message for SET_UP_CALL when there is no ALPHA tag in it -->
    <string name="default_setup_call_msg">A chamada está sendo definida</string>

    <string name="stk_app_state">"Estado do app"</string>
    <string name="enable_app">"Ativado"</string>
    <string name="disable_app">"Desativado"</string>

    <!-- Title text used for system dialog from STK app -->
    <string name="stk_dialog_title">"Toolkit do SIM"</string>
    <!-- Default message when no alpha tag in Play Tone -->
    <string name="default_tone_dialog_msg">Reprodução de um tom</string>
    <!-- Default message when no alpha in OPEN_CHANNEL -->
    <string name="default_open_channel_msg">"Abrir canal?"</string>
    <!-- Default message when no alpha in SEND_DATA -->
    <string name="default_send_data_msg">"Enviando dados"</string>
    <!-- Default message when no alpha in RECEIVE_DATA -->
    <string name="default_receive_data_msg">"Recebendo dados"</string>
    <!-- Default message when no alpha in CLOSE_CHANNEL-->
    <string name="default_close_channel_msg">"Canal fechado"</string>
    <!-- Button texts used in STK dialog -->
    <string name="stk_dialog_accept">"SIM"</string>
    <string name="stk_dialog_reject">"NÃO"</string>

    <!-- Permission required for receiving intents from StkAppService-->
    <string name="stk_intent_permission">Permitir tentativas STK </string>
</resources>
