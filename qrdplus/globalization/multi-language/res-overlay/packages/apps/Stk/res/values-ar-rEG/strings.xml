<resources>
    <!-- Default Application label used on home screen with an icon -->
    <string name="app_name">"‏مجموعة أدوات بطاقة SIM"</string>
    <!-- Application label used on home screen with an icon2 -->
    <string name="app_name2">مجموعة أدوات 2 لبطاقة SIM</string>
