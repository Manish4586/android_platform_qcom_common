<resources>
    <!-- Default Application label used on home screen with an icon -->
    <string name="app_name">"SIM пакет алатки"</string>
    <!-- Application label used on home screen with an icon2 -->
    <string name="app_name2">"SIM пакет алатки 2"</string>
    <!-- Application label used on home screen with an icon3 -->
    <string name="app_name3">"SIM пакет алатки 3"</string>
    <!-- Menu button option for ending current SIM session -->
    <string name="menu_end_session">"Заврши сесију"</string>
    <!-- Menu button option for requesting additional help information -->
    <string name="help">"Помоћ"</string>
    <!-- Menu button option to navigate back to the previous screen -->
    <string name="menu_back">"Назад"</string>
    <!-- Settings entry which describes the current SIM Toolkit service on the device -->
    <string name="service_name">"Назив услуге"</string>
    <!-- Default description for service_name when no STK service is available -->
    <string name="stk_no_service">"Ниједна услуга није доступна"</string>
    <!-- OK button used to confirm a message dialog -->
    <string name="button_ok">У реду</string>
    <!-- Cancel button used to dismiss a message dialog and terminate the current SIM session-->
    <string name="button_cancel">"Откажи"</string>
    <!-- Yes button, used on a Yes/No confirmation screen -->
    <string name="button_yes">"Да"</string>
    <!-- No button, used on a Yes/No confirmation screen -->
    <string name="button_no">"Не"</string>
    <!-- Description for the type of text allowed in a form: alphanumeric or digits only -->
    <string name="alphabet">"Абецеда"</string>
    <!-- Type of alphabet text allowed in a form: 0-9, *, #, + characters only -->
    <string name="digits">"Цифре (0-9, *, #, +)"</string>
    <!-- Default transition message when launching a call -->
    <string name="default_call_setup_msg">"Позивање је у току..."</string>
    <!-- Default message for SET_UP_CALL when there is no ALPHA tag in it -->
    <string name="default_setup_call_msg">У току је подешавање позива</string>

    <string name="stk_app_state">"Стање апликације"</string>
    <string name="enable_app">"Омогућено"</string>
    <string name="disable_app">"Онемогућено"</string>

    <!-- Title text used for system dialog from STK app -->
    <string name="stk_dialog_title">"SIM пакет алатки"</string>
    <!-- Default message when no alpha tag in Play Tone -->
    <string name="default_tone_dialog_msg">Репродуковање тона</string>
    <!-- Default message when no alpha in OPEN_CHANNEL -->
    <string name="default_open_channel_msg">"Желите ли да отворите канал?"</string>
    <!-- Default message when no alpha in SEND_DATA -->
    <string name="default_send_data_msg">"Слање података"</string>
    <!-- Default message when no alpha in RECEIVE_DATA -->
    <string name="default_receive_data_msg">"Примање података"</string>
    <!-- Default message when no alpha in CLOSE_CHANNEL-->
    <string name="default_close_channel_msg">"Канал је затворен"</string>
    <!-- Button texts used in STK dialog -->
    <string name="stk_dialog_accept">"ДА"</string>
    <string name="stk_dialog_reject">"НЕ"</string>

    <!-- Permission required for receiving intents from StkAppService-->
    <string name="stk_intent_permission">Дозволи STK намере </string>
</resources>
