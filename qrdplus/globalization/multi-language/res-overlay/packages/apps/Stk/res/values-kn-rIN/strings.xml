<resources>
    <!-- Default Application label used on home screen with an icon -->
    <string name="app_name">"SIM ಟೂಲ್‌ಕಿಟ್"</string>
    <!-- Application label used on home screen with an icon2 -->
    <string name="app_name2">"SIM ಟೂಲ್‌ಕಿಟ್ 2"</string>
    <!-- Application label used on home screen with an icon3 -->
    <string name="app_name3">"SIM ಟೂಲ್‌ಕಿಟ್ 3"</string>
    <!-- Menu button option for ending current SIM session -->
    <string name="menu_end_session">"ಸೆಷನ್ ಅಂತ್ಯಗೊಳಿಸು"</string>
    <!-- Menu button option for requesting additional help information -->
    <string name="help">"ಸಹಾಯ"</string>
    <!-- Menu button option to navigate back to the previous screen -->
    <string name="menu_back">"ಹಿಂದೆ"</string>
    <!-- Settings entry which describes the current SIM Toolkit service on the device -->
    <string name="service_name">"ಸೇವೆಯ ಹೆಸರು"</string>
    <!-- Default description for service_name when no STK service is available -->
    <string name="stk_no_service">"ಯಾವುದೇ ಸೇವೆ ಲಭ್ಯವಿಲ್ಲ"</string>
    <!-- OK button used to confirm a message dialog -->
    <string name="button_ok">"ಸರಿ"</string>
    <!-- Cancel button used to dismiss a message dialog and terminate the current SIM session-->
    <string name="button_cancel">"ರದ್ದುಮಾಡು"</string>
    <!-- Yes button, used on a Yes/No confirmation screen -->
    <string name="button_yes">"ಹೌದು"</string>
    <!-- No button, used on a Yes/No confirmation screen -->
    <string name="button_no">"ಇಲ್ಲ"</string>
    <!-- Description for the type of text allowed in a form: alphanumeric or digits only -->
    <string name="alphabet">"ವರ್ಣಮಾಲೆಗಳು"</string>
    <!-- Type of alphabet text allowed in a form: 0-9, *, #, + characters only -->
    <string name="digits">"ಅಂಕಿಗಳು (0-9, *, #, +)"</string>
    <!-- Default transition message when launching a call -->
    <string name="default_call_setup_msg">"ಕರೆಯು ಪ್ರಗತಿಯಲ್ಲಿದೆ…"</string>
    <!-- Default message for SET_UP_CALL when there is no ALPHA tag in it -->
    <string name="default_setup_call_msg">ಕರೆ ಹೊಂದಿಕೆಯಾಗುತ್ತಿದೆ</string>

    <string name="stk_app_state">"ಅಪ್ಲಿಕೇಶನ್ ಸ್ಥಿತಿ"</string>
    <string name="enable_app">"ಸಕ್ರಿಯಗೊಳಿಸಲಾಗಿದೆ"</string>
    <string name="disable_app">"ನಿಷ್ಕ್ರಿಯಗೊಳಿಸಲಾಗಿದೆ"</string>

    <!-- Title text used for system dialog from STK app -->
    <string name="stk_dialog_title">"SIM ಟೂಲ್‌ಕಿಟ್"</string>
    <!-- Default message when no alpha tag in Play Tone -->
    <string name="default_tone_dialog_msg">ಪ್ಲೇಯಿಂಗ್ ಟೋನ್</string>
    <!-- Default message when no alpha in OPEN_CHANNEL -->
    <string name="default_open_channel_msg">"ಚಾನಲ್ ತೆರೆಯುವುದೇ?"</string>
    <!-- Default message when no alpha in SEND_DATA -->
    <string name="default_send_data_msg">"ಡೇಟಾ ಕಳುಹಿಸಲಾಗುತ್ತಿದೆ"</string>
    <!-- Default message when no alpha in RECEIVE_DATA -->
    <string name="default_receive_data_msg">"ಡೇಟಾ ಸ್ವೀಕರಿಸಲಾಗುತ್ತಿದೆ"</string>
    <!-- Default message when no alpha in CLOSE_CHANNEL-->
    <string name="default_close_channel_msg">"ಚಾನಲ್ ಮುಚ್ಚಲಾಗಿದೆ"</string>
    <!-- Button texts used in STK dialog -->
    <string name="stk_dialog_accept">"ಹೌದು"</string>
    <string name="stk_dialog_reject">"ಇಲ್ಲ"</string>

    <!-- Permission required for receiving intents from StkAppService-->
    <string name="stk_intent_permission">STK ಉದ್ದೇಶಗಳನ್ನು ಅನುಮತಿಸಿ </string>
</resources>
