<resources>
    <!-- Default Application label used on home screen with an icon -->
    <string name="app_name">"Orodja za SIM"</string>
    <!-- Application label used on home screen with an icon2 -->
    <string name="app_name2">"Orodja za SIM 2"</string>
    <!-- Application label used on home screen with an icon3 -->
    <string name="app_name3">"Orodja za SIM 3"</string>
    <!-- Menu button option for ending current SIM session -->
    <string name="menu_end_session">"Končaj sejo"</string>
    <!-- Menu button option for requesting additional help information -->
    <string name="help">"Pomoč"</string>
    <!-- Menu button option to navigate back to the previous screen -->
    <string name="menu_back">"Nazaj"</string>
    <!-- Settings entry which describes the current SIM Toolkit service on the device -->
    <string name="service_name">"Ime storitve"</string>
    <!-- Default description for service_name when no STK service is available -->
    <string name="stk_no_service">"Storitev ni na voljo"</string>
    <!-- OK button used to confirm a message dialog -->
    <string name="button_ok">"V redu"</string>
    <!-- Cancel button used to dismiss a message dialog and terminate the current SIM session-->
    <string name="button_cancel">"Prekliči"</string>
    <!-- Yes button, used on a Yes/No confirmation screen -->
    <string name="button_yes">"Da"</string>
    <!-- No button, used on a Yes/No confirmation screen -->
    <string name="button_no">"Ne"</string>
    <!-- Description for the type of text allowed in a form: alphanumeric or digits only -->
    <string name="alphabet">"Pisave"</string>
    <!-- Type of alphabet text allowed in a form: 0-9, *, #, + characters only -->
    <string name="digits">"Številke (0–9, *, #, +)"</string>
    <!-- Default transition message when launching a call -->
    <string name="default_call_setup_msg">"Klic poteka …"</string>
    <!-- Default message for SET_UP_CALL when there is no ALPHA tag in it -->
    <string name="default_setup_call_msg">Vzpostavljanje klica</string>

    <string name="stk_app_state">"Stanje aplikacije"</string>
    <string name="enable_app">"Omogočeno"</string>
    <string name="disable_app">"Onemogočeno"</string>

    <!-- Title text used for system dialog from STK app -->
    <string name="stk_dialog_title">"Orodja za SIM"</string>
    <!-- Default message when no alpha tag in Play Tone -->
    <string name="default_tone_dialog_msg">Predvajanje zvoka</string>
    <!-- Default message when no alpha in OPEN_CHANNEL -->
    <string name="default_open_channel_msg">"Želite odpreti kanal?"</string>
    <!-- Default message when no alpha in SEND_DATA -->
    <string name="default_send_data_msg">"Pošiljanje podatkov"</string>
    <!-- Default message when no alpha in RECEIVE_DATA -->
    <string name="default_receive_data_msg">"Sprejemanje podatkov"</string>
    <!-- Default message when no alpha in CLOSE_CHANNEL-->
    <string name="default_close_channel_msg">"Kanal je zaprt"</string>
    <!-- Button texts used in STK dialog -->
    <string name="stk_dialog_accept">"DA"</string>
    <string name="stk_dialog_reject">"NE"</string>

    <!-- Permission required for receiving intents from StkAppService-->
    <string name="stk_intent_permission">Dovoli namene STK </string>
</resources>
