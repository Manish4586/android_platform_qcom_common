<resources>
    <!-- Default Application label used on home screen with an icon -->
    <string name="app_name">SIM တူးလ္ကစ္</string>
    <!-- Application label used on home screen with an icon2 -->
    <string name="app_name2">"SIM တူးလ်ကစ် 2"</string>
    <!-- Application label used on home screen with an icon3 -->
    <string name="app_name3">"SIM တူးလ်ကစ် 3"</string>
    <!-- Menu button option for ending current SIM session -->
    <string name="menu_end_session">သတ္မွတ္က႑ကိုၿပီးဆံုးမည္</string>
    <!-- Menu button option for requesting additional help information -->
    <string name="help">အကူအညီ</string>
    <!-- Menu button option to navigate back to the previous screen -->
    <string name="menu_back">ေနာက္ျပန္</string>
    <!-- Settings entry which describes the current SIM Toolkit service on the device -->
    <string name="service_name">ဝန္ေဆာင္မႈအမည္</string>
    <!-- Default description for service_name when no STK service is available -->
    <string name="stk_no_service">ဆက္သြယ္ခ်ိတ္ဆက္ရန္ ဝန္ေဆာင္မႈ တစ္ခုမွမရွိပါ</string>
    <!-- OK button used to confirm a message dialog -->
    <string name="button_ok">ေကာင္းၿပီ</string>
    <!-- Cancel button used to dismiss a message dialog and terminate the current SIM session-->
    <string name="button_cancel">ပယ္ဖ်က္ပါ</string>
    <!-- Yes button, used on a Yes/No confirmation screen -->
    <string name="button_yes">မွန္ပါသည္</string>
    <!-- No button, used on a Yes/No confirmation screen -->
    <string name="button_no">မဟုတ္ပါ</string>
    <!-- Description for the type of text allowed in a form: alphanumeric or digits only -->
    <string name="alphabet">အကၡရာမ်ား</string>
    <!-- Type of alphabet text allowed in a form: 0-9, *, #, + characters only -->
    <string name="digits">ဂဏန္းမ်ား (0-9, *, #, +)</string>
    <!-- Default transition message when launching a call -->
    <string name="default_call_setup_msg">ေခၚဆိုေနပါသည္</string>
    <!-- Default message for SET_UP_CALL when there is no ALPHA tag in it -->
    <string name="default_setup_call_msg">ဖုန်းခေါ်မှု ပြင်ဆင်နေဆဲ</string>

    <string name="stk_app_state">အပလီေကးရွင္းအေနအထား</string>
    <string name="enable_app">ဖြင့္ထားသည္</string>
    <string name="disable_app">သံုးမရေအာင္ ပိတ္ထားျခင္း</string>

    <!-- Title text used for system dialog from STK app -->
    <string name="stk_dialog_title">SIM တူးလ္ကစ္</string>
    <!-- Default message when no alpha tag in Play Tone -->
    <string name="default_tone_dialog_msg">ကစားစဉ် မြည်သံ</string>
    <!-- Default message when no alpha in OPEN_CHANNEL -->
    <string name="default_open_channel_msg">လွိုင္း ဖြင့္မည္လား?</string>
    <!-- Default message when no alpha in SEND_DATA -->
    <string name="default_send_data_msg">ေဒတာပို႔ေနၿပီ</string>
    <!-- Default message when no alpha in RECEIVE_DATA -->
    <string name="default_receive_data_msg">ေဒတာ ရယူေနသည္</string>
    <!-- Default message when no alpha in CLOSE_CHANNEL-->
    <string name="default_close_channel_msg">လွိုင္းပိတ္ထားသည္</string>
    <!-- Button texts used in STK dialog -->
    <string name="stk_dialog_accept">"YES"</string>
    <string name="stk_dialog_reject">"NO"</string>

    <!-- Permission required for receiving intents from StkAppService-->
    <string name="stk_intent_permission">STK Intents ကုိ ခြင့္ျပဳပါ </string>
</resources>
