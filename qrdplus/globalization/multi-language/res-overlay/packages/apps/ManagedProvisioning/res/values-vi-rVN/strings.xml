  <string name="device_owner_error_package_not_installed">"Ứng dụng quản trị chưa được cài đặt trên thiết bị của bạn"</string>
  <!-- Title of the cancel profile owner provisioning dialog. [CHAR LIMIT=45] -->
  <string name="profile_owner_cancel_title">"Dừng thiết lập?"</string>
