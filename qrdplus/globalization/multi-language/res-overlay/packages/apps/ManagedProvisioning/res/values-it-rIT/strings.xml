<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
  <!-- TODO: Review all strings before launch -->

  <!-- General provisioning strings used for both device owner and work profile provisioning -->

  <!-- Name of the application [CHAR LIMIT=NONE] -->
  <string name="app_name">"Provisioner dispositivo"</string>
  <!-- Title of the provisioning error dialog. [CHAR LIMIT=45] -->
  <string name="provisioning_error_title">"Ops!"</string>

  <!-- Work profile provisioning flow UI. -->

  <!-- User consent -->
  <!-- Title of all work profile setup screens. [CHAR LIMIT=NONE] -->
  <string name="setup_work_space">"Configura il tuo profilo"</string>
  <!-- The text that asks for user consent to create a work profile [CHAR LIMIT=NONE] -->
  <string name="company_controls_workspace">"La tua organizzazione controlla questo profilo e lo tiene protetto, mentre tu controlli tutto il resto sul dispositivo."</string>
  <!-- String shown above the icon of the app that will control the work profile after it is set up. [CHAR LIMIT=NONE]-->
  <string name="the_following_is_your_mdm">"La seguente app dovrà accedere a questo profilo:"</string>
  <!-- String for positive button on user consent dialog for creating a profile [CHAR LIMIT=NONE] -->
  <string name="set_up">"Configura"</string>
  <!-- Progress text shown when setting up work profile [CHAR LIMIT=NONE] -->
  <string name="setting_up_workspace">"Impostazione di un profilo di lavoro..."</string>
  <!-- Text in a pop-up that asks the user to confirm that they allow the mobile device management app to take control of the profile they are creating. [CHAR LIMIT=NONE]-->
  <string name="admin_has_ability_to_monitor_profile">"L\'amministratore ha la possibilità di monitorare e gestire impostazioni, accesso aziendale, app e dati associati al profilo, incluse l\'attività di rete e le informazioni sulla posizione del dispositivo."</string>
  <!-- Text in a pop-up that asks the user to confirm that they allow the mobile device management app to take control of the device. [CHAR LIMIT=NONE]-->
  <string name="admin_has_ability_to_monitor_device">"L\'amministratore ha la possibilità di monitorare e gestire impostazioni, accesso aziendale, app e dati associati al dispositivo, incluse l\'attività di rete e le informazioni sulla posizione del dispositivo."</string>
  <!-- String telling the user how to get more information about their work profile-->
  <string name="contact_your_admin_for_more_info">"Per ulteriori informazioni, contatta il tuo amministratore e consulta le norme sulla privacy della tua organizzazione."</string>
  <!-- Text on the link that will forward the user to a website with more information about work profiles [CHAR LIMIT=30]-->
  <string name="learn_more_link">"Ulteriori informazioni"</string>
  <!-- Text on negative button in a popup shown to the user to confirm that they want to start setting up a work profile [CHAR LIMIT=15]-->
  <string name="cancel_setup">"Annulla"</string>
  <!-- Text on positive button in a popup shown to the user to confirm that they want to start setting up a work profile [CHAR LIMIT=15]-->
  <string name="ok_setup">"OK"</string>

  <!-- Default profile name used for the creation of work profiles. [CHAR LIMIT=NONE] -->
  <string name="default_managed_profile_name">"Profilo di lavoro"</string>

  <!-- String shown when there is already a work profile for this user. CHAR LIMIT=NONE] -->
  <string name="managed_profile_already_present">"Hai già un profilo di lavoro sul dispositivo. Vuoi rimuoverlo?"</string>
  <!-- String on the dialog that prompts the user to confirm that they really want to delete their existing work profile-->
  <string name="sure_you_want_to_delete_profile">"Vuoi rimuovere questo profilo di lavoro? Tutti i dati associati verranno eliminati."</string>
  <!-- String on the button that triggers the deletion of a work profile.-->
  <string name="delete_profile">"Rimuovi"</string>
  <!-- String on the button that cancels out of the deletion of a work profile.-->
  <string name="cancel_delete_profile">"Annulla"</string>

  <!-- String shown on prompt for user to encrypt and reboot the device [CHAR LIMIT=NONE]-->
  <string name="encrypt_device_text">"Per continuare la configurazione del profilo di lavoro, dovrai crittografare il dispositivo. L\'operazione potrebbe richiedere del tempo."</string>
  <!-- String shown on button for user to cancel setup when asked to encrypt device. [CHAR LIMIT=20]-->
  <string name="encrypt_device_cancel">"Annulla"</string>
  <!-- String shown on button for user to continue to settings to encrypt the device. [CHAR LIMIT=20]-->
  <string name="encrypt_device_launch_settings">"Crittografa"</string>

  <!-- Title for reminder notification to resume provisioning after encryption [CHAR LIMIT=30] -->
  <string name="continue_provisioning_notify_title">"Crittografia completa"</string>
  <!-- Body for reminder notification to resume provisioning after encryption [CHAR LIMIT=NONE] -->
  <string name="continue_provisioning_notify_text">"Tocca per continuare a configurare il profilo di lavoro"</string>

  <!-- Explains the failure and what to do to next. [CHAR LIMIT=NONE] -->
  <string name="managed_provisioning_error_text">"Impossibile impostare il tuo profilo di lavoro. Contatta il tuo reparto IT oppure riprova in un secondo momento."</string>
  <!-- Error string displayed if this device doesn\'t support work profiles. -->
  <string name="managed_provisioning_not_supported">"Il tuo dispositivo non supporta profili di lavoro"</string>
  <!-- Error string displayed if the user that initiated the provisioning is not the user owner. -->
  <string name="user_is_not_owner">"I profili di lavoro devono essere configurati dal proprietario del dispositivo"</string>
  <!-- Error string displayed if the selected launcher doesn\'t support work profiles. -->
  <string name="managed_provisioning_not_supported_by_launcher">"Il tuo profilo di lavoro non è supportato da questa app Avvio applicazioni. Devi passare a un\'app Avvio applicazioni compatibile."</string>
  <!-- Button text for the button that cancels provisioning  -->
  <string name="cancel_provisioning">"Annulla"</string>
  <!-- Button text for the button that opens the launcher picker  -->
  <string name="pick_launcher">"OK"</string>

  <!-- Device owner provisioning flow UI. -->

  <!-- Default username for the user of the owned device. [CHAR LIMIT=45] -->
  <string name="default_owned_device_username">"Utente del dispositivo di lavoro"</string>

  <!-- Title of the work device setup screen. [CHAR LIMIT=NONE] -->
  <string name="setup_device">"Configurazione del dispositivo"</string>
  <!-- Progress text indicating that the setup data is processed. [CHAR LIMIT=45] -->
  <string name="progress_data_process">"Elaborazione dati di impostazione..."</string>
  <!-- Progress text indicating that wifi is set up. [CHAR LIMIT=45] -->
  <string name="progress_connect_to_wifi">"Connessione a rete Wi-Fi..."</string>
  <!-- Progress text indicating that the device admin package is being downloaded. [CHAR LIMIT=45] -->
  <string name="progress_download">"Download dell\'app dell\'amministratore..."</string>
  <!-- Progress text indicating that the device admin package is being installed. [CHAR LIMIT=45] -->
  <string name="progress_install">"Installazione dell\'app dell\'amministratore..."</string>
  <!-- Progress text indicating that the device admin package is set as ownder. [CHAR LIMIT=45] -->
  <string name="progress_set_owner">"Impostazione del proprietario dispositivo..."</string>

  <!-- Title of the cancel provisioning dialog. [CHAR LIMIT=45] -->
  <string name="device_owner_cancel_title">"Interrompere la configurazione?"</string>
  <!-- Message of the cancel dialog. [CHAR LIMIT=NONE] -->
  <string name="device_owner_cancel_message">"Vuoi interrompere l\'impostazione e cancellare i dati del tuo dispositivo?"</string>
  <!-- Cancel button text of the cancel dialog. [CHAR LIMIT=45] -->
  <string name="device_owner_cancel_cancel">"Annulla"</string>
  <!-- OK button text of the error dialog. [CHAR LIMIT=45] -->
  <string name="device_owner_error_ok">"OK"</string>
  <!-- Reset button text of the error dialog. [CHAR LIMIT=45] -->
  <string name="device_owner_error_reset">"Reimposta"</string>
  <!-- Message of the error dialog in case of an unspecified error. [CHAR LIMIT=NONE] -->
  <string name="device_owner_error_general">"Impossibile impostare il dispositivo. Contatta il tuo reparto IT."</string>
  <!-- Message of the error dialog when already provisioned. [CHAR LIMIT=NONE] -->
  <string name="device_owner_error_already_provisioned">"Questo dispositivo è già configurato"</string>
  <!-- Message of the error dialog when setting up wifi failed. [CHAR LIMIT=NONE] -->
  <string name="device_owner_error_wifi">"Impossibile connettersi alla rete Wi-Fi"</string>
  <!-- Message of the error dialog when data integrity check fails. [CHAR LIMIT=NONE] -->
  <string name="device_owner_error_hash_mismatch">"Impossibile utilizzare l\'app dell\'amministratore per un errore di checksum. Contatta il tuo reparto IT."</string>
  <!-- Message of the error dialog when device owner apk could not be downloaded. [CHAR LIMIT=NONE] -->
  <string name="device_owner_error_download_failed">"Impossibile scaricare l\'app di amministrazione"</string>
  <!-- Message of the error dialog when package to install is invalid. [CHAR LIMIT=NONE] -->
  <string name="device_owner_error_package_invalid">"Impossibile utilizzare l\'app dell\'amministratore per componenti mancanti o danneggiati. Contatta il tuo reparto IT."</string>
  <!-- Message of the error dialog when package could not be installed. [CHAR LIMIT=NONE] -->
  <string name="device_owner_error_installation_failed">"Impossibile installare l\'app di amministrazione"</string>
  <!-- Message of the error dialog when device admin package is not installed. [CHAR LIMIT=NONE] -->
  <string name="device_owner_error_package_not_installed">"L\'app di amministrazione non è stata installata sul tuo dispositivo"</string>
  <!-- Title of the cancel profile owner provisioning dialog. [CHAR LIMIT=45] -->
  <string name="profile_owner_cancel_title">"Interrompere la configurazione?"</string>
  <!-- Message of the cancel dialog. [CHAR LIMIT=NONE] -->
  <string name="profile_owner_cancel_message">Interrompere la configurazione?</string>
  <!-- Cancel button text of the cancel dialog. [CHAR LIMIT=45] -->
  <string name="profile_owner_cancel_cancel">"No"</string>
  <!-- OK button text of the error dialog. [CHAR LIMIT=45] -->
  <string name="profile_owner_cancel_ok">"Sì"</string>
  <!-- Message of the cancel progress dialog. [CHAR LIMIT=45] -->
  <string name="profile_owner_cancelling">Annullamento in corso...</string>
</resources>
