  <!-- General provisioning strings used for both device owner and work profile provisioning -->

  <!-- Name of the application [CHAR LIMIT=NONE] -->
  <string name="app_name">ઉપકરણ જોગવાઈકર્તા</string>
  <!-- Title of the provisioning error dialog. [CHAR LIMIT=45] -->
  <string name="provisioning_error_title">અરેરે!</string>

  <!-- Work profile provisioning flow UI. -->

  <!-- User consent -->
  <!-- Title of all work profile setup screens. [CHAR LIMIT=NONE] -->
  <string name="setup_work_space">તમારી પ્રોફાઇલ સેટ કરો</string>
  <!-- The text that asks for user consent to create a work profile [CHAR LIMIT=NONE] -->
  <string name="company_controls_workspace">તમારી સંસ્થા આ પ્રોફાઇલને નિયંત્રિત કરે છે અને તેને સુરક્ષિત રાખે છે. તમે તમારા ઉપકરણ પર બીજું બધું નિયંત્રિત કરો છો.</string>
  <!-- String shown above the icon of the app that will control the work profile after it is set up. [CHAR LIMIT=NONE]-->
  <string name="the_following_is_your_mdm">નીચેની એપ્લિકેશનને આ પ્રોફાઇલને ઍક્સેસ કરવાની જરૂર છે:</string>
  <!-- String for positive button on user consent dialog for creating a profile [CHAR LIMIT=NONE] -->
  <string name="set_up">સેટ કરો</string>
  <!-- Progress text shown when setting up work profile [CHAR LIMIT=NONE] -->
  <string name="setting_up_workspace">તમારી પ્રોફાઇલ સેટ કરી રહ્યાં છે...</string>
  <!-- Text in a pop-up that asks the user to confirm that they allow the mobile device management app to take control of the profile they are creating. [CHAR LIMIT=NONE]-->
  <string name="admin_has_ability_to_monitor_profile">તમારા વ્યવસ્થાપક પાસે નેટવર્ક પ્રવૃત્તિ અને તમારા ઉપકરણની સ્થાન માહિતી સહિત પ્રોફાઇલ સાથે સંકળાયેલ સેટિંગ્સ, કૉર્પોરેટ ઍક્સેસ, એપ્લિકેશન્સ અને ડેટાને મૉનિટર અને સંચાલિત કરવાની ક્ષમતા છે.</string>
  <!-- Text in a pop-up that asks the user to confirm that they allow the mobile device management app to take control of the device. [CHAR LIMIT=NONE]-->
  <string name="admin_has_ability_to_monitor_device">તમારા વ્યવસ્થાપક પાસે નેટવર્ક પ્રવૃત્તિ અને તમારા ઉપકરણની સ્થાન માહિતી સહિત પ્રોફાઇલ સાથે સંકળાયેલ સેટિંગ્સ, કૉર્પોરેટ ઍક્સેસ, એપ્લિકેશન્સ અને ડેટાને મૉનિટર અને સંચાલિત કરવાની ક્ષમતા છે.</string>
  <!-- String telling the user how to get more information about their work profile-->
  <string name="contact_your_admin_for_more_info">તમારી સંસ્થાની ગોપનીયતા નીતિઓ સહિત વધુ માહિતી માટે તમારા વ્યવસ્થાપકનો સંપર્ક કરો.</string>
  <!-- Text on the link that will forward the user to a website with more information about work profiles [CHAR LIMIT=30]-->
  <string name="learn_more_link">વધુ જાણો </string>
  <!-- Text on negative button in a popup shown to the user to confirm that they want to start setting up a work profile [CHAR LIMIT=15]-->
  <string name="cancel_setup">રદ કરો</string>
  <!-- Text on positive button in a popup shown to the user to confirm that they want to start setting up a work profile [CHAR LIMIT=15]-->
  <string name="ok_setup">ઓકે</string>

  <!-- Default profile name used for the creation of work profiles. [CHAR LIMIT=NONE] -->
  <string name="default_managed_profile_name">કાર્ય પ્રોફાઇલ</string>

  <!-- String shown when there is already a work profile for this user. CHAR LIMIT=NONE] -->
  <string name="managed_profile_already_present">તમારી પાસે તમારા ઉપકરણ પર પહેલાંથી એક કાર્ય પ્રોફાઇલ છે. શું તમે તેને દૂર કરવા માંગો છો?</string>
  <!-- String on the dialog that prompts the user to confirm that they really want to delete their existing work profile-->
  <string name="sure_you_want_to_delete_profile">શું તમે ખરેખર આ કાર્ય પ્રોફાઇલ દૂર કરવા માંગો છો? તેનો બધો ડેટા કાઢી નાખવામાં આવશે.</string>
  <!-- String on the button that triggers the deletion of a work profile.-->
  <string name="delete_profile">દૂર કરો</string>
  <!-- String on the button that cancels out of the deletion of a work profile.-->
  <string name="cancel_delete_profile">રદ કરો</string>

  <!-- String shown on prompt for user to encrypt and reboot the device [CHAR LIMIT=NONE]-->
  <string name="encrypt_device_text">તમારી કાર્ય પ્રોફાઇલને સેટ કરવાનું ચાલુ રાખવા માટે, તમને તમારા ઉપકરણને એન્ક્રિપ્ટ કરવું પડશે. આમાં થોડોક સમય લાગી શકે છે.</string>
  <!-- String shown on button for user to cancel setup when asked to encrypt device. [CHAR LIMIT=20]-->
  <string name="encrypt_device_cancel">રદ કરો</string>
  <!-- String shown on button for user to continue to settings to encrypt the device. [CHAR LIMIT=20]-->
  <string name="encrypt_device_launch_settings">એન્ક્રિપ્ટ કરો</string>

  <!-- Title for reminder notification to resume provisioning after encryption [CHAR LIMIT=30] -->
  <string name="continue_provisioning_notify_title">એન્ક્રિપ્શન પૂર્ણ</string>
  <!-- Body for reminder notification to resume provisioning after encryption [CHAR LIMIT=NONE] -->
  <string name="continue_provisioning_notify_text">તમારી કાર્ય પ્રોફાઇલ સેટ કરવાનું ચાલુ રાખવા માટે ટચ કરો</string>

  <!-- Explains the failure and what to do to next. [CHAR LIMIT=NONE] -->
  <string name="managed_provisioning_error_text">તમારી કાર્ય પ્રોફાઇલ સેટ કરી શકાઈ નથી. તમારા IT વિભાગનો સંપર્ક કરો અથવા પછીથી ફરી પ્રયાસ કરો.</string>
  <!-- Error string displayed if this device doesn\'t support work profiles. -->
  <string name="managed_provisioning_not_supported">તમારું ઉપકરણ કાર્ય પ્રોફાઇલ્સને સમર્થન આપતું નથી</string>
  <!-- Error string displayed if the user that initiated the provisioning is not the user owner. -->
  <string name="user_is_not_owner">ઉપકરણ માલિક દ્વારા કાર્ય પ્રોફાઇલ્સ સેટ કરવાની જરૂર છે</string>
  <!-- Error string displayed if the selected launcher doesn\'t support work profiles. -->
  <string name="managed_provisioning_not_supported_by_launcher">આ લૉન્ચર એપ્લિકેશન દ્વારા તમારા કાર્ય પ્રોફાઇલ્સ સમર્થિત નથી. તમારે સંગત હોય એવા લૉન્ચર પર સ્વિચ કરવું પડશે.</string>
  <!-- Button text for the button that cancels provisioning  -->
  <string name="cancel_provisioning">રદ કરો</string>
  <!-- Button text for the button that opens the launcher picker  -->
  <string name="pick_launcher">ઓકે</string>

