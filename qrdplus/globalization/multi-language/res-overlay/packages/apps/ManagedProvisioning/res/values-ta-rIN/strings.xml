  <string name="progress_data_process">"அமைவு தரவைச் செயல்படுத்துகிறது..."</string>
  <!-- Progress text indicating that wifi is set up. [CHAR LIMIT=45] -->
  <string name="progress_connect_to_wifi">"வைஃபை உடன் இணைக்கிறது..."</string>
