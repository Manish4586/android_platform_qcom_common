  <!-- General provisioning strings used for both device owner and work profile provisioning -->

  <!-- Name of the application [CHAR LIMIT=NONE] -->
  <string name="app_name">ডিভাইস সংস্থানকারী</string>
  <!-- Title of the provisioning error dialog. [CHAR LIMIT=45] -->
  <string name="provisioning_error_title">ওহো!</string>

  <!-- Work profile provisioning flow UI. -->

  <!-- User consent -->
  <!-- Title of all work profile setup screens. [CHAR LIMIT=NONE] -->
  <string name="setup_work_space">আপনার প্রোফাইল সেট আপ করুন</string>
  <!-- The text that asks for user consent to create a work profile [CHAR LIMIT=NONE] -->
  <string name="company_controls_workspace">আপনার সংস্থান এই প্রোফাইল নিয়ন্ত্রণ করে ও এটিকে নিরাপদ রাখে। আপনি আপনার ডিভাইসের বাকি সবকিছু নিয়ন্ত্রণ করেন।</string>
  <!-- String shown above the icon of the app that will control the work profile after it is set up. [CHAR LIMIT=NONE]-->
  <string name="the_following_is_your_mdm">নিম্নোক্ত অ্যাপটিকে এই প্রোফাইলে অ্যাক্সেস করতে হবে:</string>
  <!-- String for positive button on user consent dialog for creating a profile [CHAR LIMIT=NONE] -->
  <string name="set_up">সেট আপ</string>
  <!-- Progress text shown when setting up work profile [CHAR LIMIT=NONE] -->
  <string name="setting_up_workspace">আপনার কাজের প্রোফাইল সেট আপ হচ্ছে...</string>
  <!-- Text in a pop-up that asks the user to confirm that they allow the mobile device management app to take control of the profile they are creating. [CHAR LIMIT=NONE]-->
  <string name="admin_has_ability_to_monitor_profile">আপনার প্রশাসক নেটওয়ার্ক ক্রিয়াকলাপ ও আপনার ডিভাইসের অবস্থান তথ্য সহ সেটিংস, কর্পোরেট অ্যাক্সেস, অ্যাপগুলি ও প্রোফাইলের সাথে সম্পর্কিত ডেটা মনিটর ও পরিচলান করতে পারে।</string>
  <!-- Text in a pop-up that asks the user to confirm that they allow the mobile device management app to take control of the device. [CHAR LIMIT=NONE]-->
  <string name="admin_has_ability_to_monitor_device">আপনার প্রশাসক নেটওয়ার্ক ক্রিয়াকলাপ ও আপনার ডিভাইসের অবস্থান তথ্য সহ সেটিংস, কর্পোরেট অ্যাক্সেস, অ্যাপগুলি এবং এই ডিভাইসের সাথে সম্পর্কিত ডেটা মনিটর ও পরিচলান করতে পারে।</string>
  <!-- String telling the user how to get more information about their work profile-->
  <string name="contact_your_admin_for_more_info">আপনার সংস্থার গোপনীয়তা নীতি সহ আরও তথ্যের জন্য আপনার প্রশাসকের সাথে যোগাযোগ করুন।</string>
  <!-- Text on the link that will forward the user to a website with more information about work profiles [CHAR LIMIT=30]-->
  <string name="learn_more_link">আরও শিখুন </string>
