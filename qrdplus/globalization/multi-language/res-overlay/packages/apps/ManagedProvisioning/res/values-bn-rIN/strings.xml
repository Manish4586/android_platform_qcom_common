  <string name="cancel_setup">বাতিল</string>
  <!-- Text on positive button in a popup shown to the user to confirm that they want to start setting up a work profile [CHAR LIMIT=15]-->
  <string name="ok_setup">ঠিক আছে</string>

  <!-- Default profile name used for the creation of work profiles. [CHAR LIMIT=NONE] -->
  <string name="default_managed_profile_name">কর্মস্থানের প্রোফাইল</string>

  <!-- String shown when there is already a work profile for this user. CHAR LIMIT=NONE] -->
  <string name="managed_profile_already_present">আপনার ডিভাইসে ইতিমধ্যে একটি কাজের প্রোফাইল আছে। আপনি কি এটি অপসারণ করতে চান?</string>
  <!-- String on the dialog that prompts the user to confirm that they really want to delete their existing work profile-->
  <string name="sure_you_want_to_delete_profile">আপনি কি এই কাজের প্রোফাইলটি অপসারণ করতে চাওয়ার বিষয়ে নিশ্চিত? এর সমস্ত ডেটা মুছে যাবে।</string>
  <!-- String on the button that triggers the deletion of a work profile.-->
  <string name="delete_profile">সরান</string>
  <!-- String on the button that cancels out of the deletion of a work profile.-->
  <string name="cancel_delete_profile">বাতিল</string>

  <!-- String shown on prompt for user to encrypt and reboot the device [CHAR LIMIT=NONE]-->
  <string name="encrypt_device_text">আপনার কাজের প্রোফাইল সেট আপ করা অবিরত রাখতে, আপনাকে আপনার ডিভাইস এনক্রিপ্ট করতে হবে। এটি কিছু সময় নিতে পারে।</string>
  <!-- String shown on button for user to cancel setup when asked to encrypt device. [CHAR LIMIT=20]-->
  <string name="encrypt_device_cancel">বাতিল</string>
  <!-- String shown on button for user to continue to settings to encrypt the device. [CHAR LIMIT=20]-->
  <string name="encrypt_device_launch_settings">এনক্রিপ্ট করুন</string>

  <!-- Title for reminder notification to resume provisioning after encryption [CHAR LIMIT=30] -->
  <string name="continue_provisioning_notify_title">এনক্রিপশন সম্পূর্ণ</string>
  <!-- Body for reminder notification to resume provisioning after encryption [CHAR LIMIT=NONE] -->
  <string name="continue_provisioning_notify_text">আপনার কাজের প্রোফাইল সেট আপ করা অবিরত রাখতে স্পর্শ করুন</string>

  <!-- Explains the failure and what to do to next. [CHAR LIMIT=NONE] -->
  <string name="managed_provisioning_error_text">আপনার প্রোফাইল সেট আপ করা যায়নি। আপনার IT বিভাগে যোগাযোগ করুন বা পরে আবার চেষ্টা করুন।</string>
  <!-- Error string displayed if this device doesn\'t support work profiles. -->
  <string name="managed_provisioning_not_supported">আপনার ডিভাইস কাজের প্রোফাইলগুলিকে সমর্থন করে না</string>
  <!-- Error string displayed if the user that initiated the provisioning is not the user owner. -->
  <string name="user_is_not_owner">কাজের প্রোফাইলগুলি ডিভাইসের মালিককে সেট আপ করতে হবে</string>
  <!-- Error string displayed if the selected launcher doesn\'t support work profiles. -->
  <string name="managed_provisioning_not_supported_by_launcher">এই লঞ্চার অ্যাপ আপনার কাজের প্রোফাইলকে সমর্থন করে না। আপনাকে একটি এমন লঞ্চারে সুযিচ করতে হবে যা সুসঙ্গত।</string>
  <!-- Button text for the button that cancels provisioning  -->
  <string name="cancel_provisioning">বাতিল</string>
  <!-- Button text for the button that opens the launcher picker  -->
  <string name="pick_launcher">ঠিক আছে</string>

