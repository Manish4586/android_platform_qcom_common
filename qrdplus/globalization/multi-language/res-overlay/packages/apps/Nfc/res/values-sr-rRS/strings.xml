<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Nfc Service"</string>
    <string name="nfcUserLabel">"Nfc"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"Контакт је примљен преко NFC-а"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"Додирните да бисте додали ову особу као контакт."</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"NFC интеракција је довршена"</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"Додирните да бисте овој особи дали контакт информације."</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"NFC је омогућен."</string>
    <string name="touch">"Додирните за емитовање"</string>

    <string name="beam_progress">"Долазно пребацивање..."</string>
    <string name="beam_outgoing">"Пребацивање..."</string>
    <string name="beam_complete">"Пребацивање је довршено"</string>
    <string name="beam_failed">"Пребацивање се није завршило"</string>
    <string name="beam_canceled">"Пребацивање је отказано"</string>
    <string name="cancel">"Откажи"</string>
    <string name="beam_touch_to_view">"Додирните да бисте прегледали"</string>
    <string name="beam_handover_not_supported">"Уређај пријемника не подржава пренос великих датотека пребацивањем."</string>
    <string name="beam_try_again">"Поново приближите уређаје"</string>
    <string name="beam_busy">Веза је тренутно заузета. Покушајте поново када се заврши претходни пренос.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">Уређај</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral">Повезује се <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> је повезан</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> није повезан</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral">Прекида се веза са <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral">Веза са <xliff:g id="device_name">%1$s</xliff:g> је прекинута</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral">Упарује се са <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed">Упаривање са <xliff:g id="device_name">%1$s</xliff:g> није успело</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"Није могуће омогућити Bluetooth"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"Да ли сте сигурни да желите да упарите Bluetooth уређај <xliff:g id="DEVICE_NAME">%1$s</xliff:g>?"</string>
    <string name="pair_yes">"Да"</string>
    <string name="pair_no">"Не"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"Додирните поново да бисте платили помоћу апликације <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"Додирните поново да бисте довршили помоћу апликације <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"Ову трансакцију није могуће довршити помоћу апликације <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"Није могуће користити <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"Плаћање помоћу апликација"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"Довршавање помоћу апликација"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"Изабрана услуга за функцију Додирни и плати је уклоњена. Желите ли да изаберете другу?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"Додирните други уређај да бисте завршили"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"Повежи"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"Повезивање са мрежом није могуће"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"Повезана је"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"Повежите се са мрежом"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"Желите ли да се повежете са мрежом <xliff:g id="NETWORK_SSID">%1$s</xliff:g>?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"Android пребацивање захтева NFC да бисте могли да га омогућите. Желите ли да га омогућите?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"Android пребацивање"</string>
</resources>
