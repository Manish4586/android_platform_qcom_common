<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Storitev NFC"</string>
    <string name="nfcUserLabel">"NFC"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"Stik prejet prek NFC-ja"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"Tapnite, če želite dodati to osebo kot stik."</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"Pošiljanje prek NFC-ja končano"</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"Tapnite, če želite tej osebi poslati svoje podatke za stik."</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"NFC omogočen."</string>
    <string name="touch">"Dotaknite se, da pošljete"</string>

    <string name="beam_progress">"Dohodni prenos ..."</string>
    <string name="beam_outgoing">"Prenašanje ..."</string>
    <string name="beam_complete">"Prenos končan"</string>
    <string name="beam_failed">"Prenos se ni končal"</string>
    <string name="beam_canceled">"Prenos je preklican"</string>
    <string name="cancel">"Prekliči"</string>
    <string name="beam_touch_to_view">"Dotaknite se za prikaz"</string>
    <string name="beam_handover_not_supported">"Prejemnikova naprava ne podpira prenosa velikih datotek prek funkcije Android Beam."</string>
    <string name="beam_try_again">"Znova združi naprave"</string>
    <string name="beam_busy">Storitev Beam je trenutno zasedena. Znova poskusite po koncu predhodnega prenosa.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">naprava</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral">Povezovanje z napravo <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral">Povezava z napravo <xliff:g id="device_name">%1$s</xliff:g> vzpostavljena</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed">Povezave z napravo <xliff:g id="device_name">%1$s</xliff:g> ni bilo mogoče vzpostaviti</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral">Prekinjanje povezave z napravo <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral">Povezava z napravo <xliff:g id="device_name">%1$s</xliff:g> prekinjena</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral">Združevanje z napravo <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed">Združevanje z napravo <xliff:g id="device_name">%1$s</xliff:g> ni uspelo</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"Bluetootha ni mogoče omogočiti"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"Ste prepričani, da želite seznaniti napravo Bluetooth <xliff:g id="DEVICE_NAME">%1$s</xliff:g>?"</string>
    <string name="pair_yes">"Da"</string>
    <string name="pair_no">"Ne"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"Znova se dotaknite, da plačate z aplikacijo <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"Znova se dotaknite, da dokončate z aplikacijo <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"Transakcije ni bilo mogoče dokončati z aplikacijo <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"Aplikacije <xliff:g id="APP">%1$s</xliff:g> ni bilo mogoče uporabiti."</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"Plačaj z aplikacijo"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"Dokončaj z aplikacijo"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"Vaša prednostna storitev za plačevanje z dotikom je bila odstranjena. Želite izbrati drugo?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"Dotaknite se druge naprave, če želite dokončati"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"Vzpostavi povezavo"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"Povezave z omrežjem ni mogoče vzpostaviti"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"Povezava je vzpostavljena"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"Vzpostavitev povezave z omrežjem"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"Želite vzpostaviti povezavo z omrežjem <xliff:g id="NETWORK_SSID">%1$s</xliff:g>?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"Za Android Beam mora biti omogočen NFC. Ali ga želite omogočiti?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"Android Beam"</string>
</resources>
