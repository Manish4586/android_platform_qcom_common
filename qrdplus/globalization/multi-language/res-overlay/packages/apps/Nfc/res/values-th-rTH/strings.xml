    <string name="transaction_failure">"ไม่สามารถดำเนินการธุรกรรมนี้ให้เสร็จสมบูรณ์ด้วย <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"ไม่สามารถใช้ <xliff:g id="APP">%1$s</xliff:g>"</string>
