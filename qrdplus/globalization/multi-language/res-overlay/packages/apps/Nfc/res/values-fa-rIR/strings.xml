<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"‏سرویس Nfc"</string>
    <string name="nfcUserLabel">"Nfc"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"‏مخاطب از طریق NFC دریافت شد"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"برای افزودن این فرد به‌عنوان یک مخاطب، لمس کنید."</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"‏تعامل NFC کامل شد"</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"لمس کنید تا اطلاعات تماس خود را به این فرد بدهید."</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"‏با NFC فعال شده."</string>
    <string name="touch">"برای پخش، لمس کنید"</string>

    <string name="beam_progress">"در حال دریافت پرتو..."</string>
    <string name="beam_outgoing">"پرتو…"</string>
    <string name="beam_complete">"پرتو ارسال شد"</string>
    <string name="beam_failed">"پرتو کامل نشد"</string>
    <string name="beam_canceled">"پرتو لغو شد"</string>
    <string name="cancel">"لغو"</string>
    <string name="beam_touch_to_view">"برای مشاهده لمس کنید"</string>
    <string name="beam_handover_not_supported">"دستگاه گیرنده انتقال فایل‌های بزرگ از طریق پرتو را پشتیبانی نمی‌کند."</string>
    <string name="beam_try_again">"مرتبط‌کردن دوباره دستگاه‌ها"</string>
    <string name="beam_busy">دریافت مشغول است. وقتی انتقال قبلی تمام شد دوباره امتحان کنید.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">دستگاه</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral">در حال اتصال ‎<xliff:g id="device_name">%1$s</xliff:g>‎</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral">‎<xliff:g id="device_name">%1$s</xliff:g>‎ متصل شد</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed">اتصال ‎<xliff:g id="device_name">%1$s</xliff:g>‎ ممکن نیست</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral">در حال قطع اتصال ‎<xliff:g id="device_name">%1$s</xliff:g>‎</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral">اتصال ‎<xliff:g id="device_name">%1$s</xliff:g>‎ قطع شد</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral">در حال جفت شدن ‎<xliff:g id="device_name">%1$s</xliff:g>‎</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed">جفت شدن ‎<xliff:g id="device_name">%1$s</xliff:g>‎ ممکن نیست</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"فعال کردن بلوتوث امکانپذیر نیست"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"آیا مطمئن هستید که می‌خواهید دستگاه بلوتوث <xliff:g id="DEVICE_NAME">%1$s</xliff:g> را جفت کنید؟"</string>
    <string name="pair_yes">"بله"</string>
    <string name="pair_no">"خیر"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"برای پرداخت با <xliff:g id="APP">%1$s</xliff:g>، دوباره ضربه بزنید"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"برای تکمیل از طریق <xliff:g id="APP">%1$s</xliff:g>، دوباره ضربه بزنید"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"این تراکنش با <xliff:g id="APP">%1$s</xliff:g> تکمیل نشد."</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"نتوانست از <xliff:g id="APP">%1$s</xliff:g> استفاده کند."</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"پرداخت با"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"تکمیل با"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"سرویس ترجیحی شما برای ضربه و پرداخت حذف شده است. سرویس دیگری را انتخاب می‌کنید؟"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"برای تکمیل، روی دستگاه دیگری ضربه بزنید"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"اتصال"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"اتصال به شبکه ممکن نیست"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"مرتبط شد"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"اتصال به شبکه"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"به شبکه <xliff:g id="NETWORK_SSID">%1$s</xliff:g> متصل می‌شوید؟"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"‏پرتوی Android برای فعال شدن به NFC نیاز دارد. می‌خواهید آن را فعال کنید؟"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"‏پرتو Android"</string>
</resources>
