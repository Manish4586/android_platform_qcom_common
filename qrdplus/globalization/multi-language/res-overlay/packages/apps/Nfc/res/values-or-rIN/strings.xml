    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">ଡିଭାଇସ୍</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ସଂଯୋଗ କରୁଛି</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral"><xliff:g id="device_name">%1$s</xliff:g>ସଂଯୋଜିତ</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> କୁ ସଂଯୋଗ କରିପାରିଲା ନାହିଁ।</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ବିଛିନ୍ନ କରୁଛି</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ବିଛିନ୍ନ ହୋଇଛି</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ପେୟାରିଂ</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> କୁ ପେୟାର୍ କରିପାରିବେ ନାହିଁ।</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">ବ୍ଲୁଟୁଥ୍ ସକ୍ଷମ କରିପାରିଲା ନାହିଁ</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">ଆପଣ ବ୍ଲୁଟୁଥ୍ ଡିଭାଇସ୍ <xliff:g id="device_name">%1$s</xliff:g> ପେୟାର୍ କରିବାକୁ ଚାହୁଁଥିବା ନିଶ୍ଚିତ କି?</string>
    <string name="pair_yes">ହଁ</string>
    <string name="pair_no">ନା</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay"><xliff:g id="app">%1$s</xliff:g> ସହ ପେୟାର୍ କରିବାକୁ ପୁନଃଚେଷ୍ଟା କରନ୍ତୁ</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete"><xliff:g id="app">%1$s</xliff:g> ସହ ସମ୍ପୂର୍ଣ୍ଣ କରିବାକୁ ପୁନଃ ଟ୍ୟାପ୍ କରନ୍ତୁ</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">ଟ୍ରାଞ୍ଜେକସନ୍ <xliff:g id="app">%1$s</xliff:g> ସହ ସମ୍ପୂର୍ଣ୍ନ ହୋଇପାରିବ ନାହିଁ।</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app"><xliff:g id="app">%1$s</xliff:g> ବ୍ୟବହାର କରି ପାରିଲା ନାହିଁ।</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">ସହ ଦେୟ</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">ଏହା ସହ ସମ୍ପୂର୍ଣ୍ଣ</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">ଟ୍ୟାପ୍ &amp; ଦେୟ ପାଇଁ ପଣଙ୍କ ପସନ୍ଦିତ ସେବା ଅପସାରିତ ହେଲା। ଅନ୍ୟଟିଏ ବାଛିବେ?</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">ସମ୍ପୂର୍ଣ୍ଣ କରିବାକୁ ଅନ୍ୟ ଡିଭାଇସ୍ ଟ୍ୟାପ୍ କରନ୍ତୁ</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">ସଂଯୋଗ କରନ୍ତୁ</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">ନେଟୱାର୍କକୁ ସଂଯୋଗ କରିବାକୁ ଅସମର୍ଥ</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">ସଂଯୋଜିତ ହୋଇଛି</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">ନେଟୱାର୍କକୁ ସଂଯୋଗ କରନ୍ତୁ</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network"><xliff:g id="network_ssid">%1$s</xliff:g>କୁ ସଂଯୋଗ କରିବେ?</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">ସକ୍ଷମ କରିବାକୁ ଆଣ୍ଡ୍ରୋଏଡ୍ ବିମ୍ NFC ଆବଶ୍ୟକ କରେ। ଆପଣ ଏହାକୁ ସକ୍ଷମ କରିବାକୁ ଚାହାଁନ୍ତି କି?</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">ଆଣ୍ଡ୍ରୋଏଡ୍ ବିମ୍</string>
</resources>
