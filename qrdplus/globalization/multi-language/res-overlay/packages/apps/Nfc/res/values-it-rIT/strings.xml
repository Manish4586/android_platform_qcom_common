<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Servizio NFC"</string>
    <string name="nfcUserLabel">"NFC"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"Contatto ricevuto via NFC"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"Tocca per aggiungere questa persona come contatto."</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"Interazione NFC completa"</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"Tocca per inviare a questa persona le informazioni di contatto"</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"Comunicazione NFC abilitata."</string>
    <string name="touch">"Tocca per trasmettere"</string>

    <string name="beam_progress">"Trasmissione in arrivo..."</string>
    <string name="beam_outgoing">"Trasmissione..."</string>
    <string name="beam_complete">"Trasmissione completata"</string>
    <string name="beam_failed">"Trasmissione non completata"</string>
    <string name="beam_canceled">"Trasmissione annullata"</string>
    <string name="cancel">"Annulla"</string>
    <string name="beam_touch_to_view">"Tocca per visualizzare"</string>
    <string name="beam_handover_not_supported">"Il dispositivo del destinatario non supporta il trasferimento di file di grandi dimensioni tramite Android Beam."</string>
    <string name="beam_try_again">"Avvicina di nuovo i dispositivi"</string>
    <string name="beam_busy">Beam attualmente occupato. Riprovare una volta completato il trasferimento precedente.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">Dispositivo</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral">Connessione a <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral">Connesso a <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed">Impossibile connettersi a <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral">Disconnessione da <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral">Disconnesso <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral">Associazione <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed">Impossibile associare <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"Impossibile attivare il Bluetooth"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"Vuoi accoppiare il dispositivo Bluetooth <xliff:g id="DEVICE_NAME">%1$s</xliff:g>?"</string>
    <string name="pair_yes">"Sì"</string>
    <string name="pair_no">"No"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"Tocca ancora per pagare con <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"Tocca ancora per completare con <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"Impossibile completare la transazione con <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"Impossibile utilizzare <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"Paga con"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"Completa con"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"Il tuo servizio preferito per Touch &amp; Pay è stato rimosso. Vuoi sceglierne un altro?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"Per completare, tocca un altro dispositivo"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"Connessione"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"Impossibile connettersi alla rete"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"Collegamento avvenuto"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"Connetti a rete"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"Connettere alla rete <xliff:g id="NETWORK_SSID">%1$s</xliff:g>?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"L\'attivazione della funzione Android Beam richiede la tecnologia NFC. Vuoi attivarla?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"Android Beam"</string>
</resources>
