<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Usługa NFC"</string>
    <string name="nfcUserLabel">"NFC"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"Kontakt odebrany przez NFC"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"Dotknij, aby dodać tę osobę do kontaktów."</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"Zakończono interakcję przez NFC"</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"Dotknij, aby przekazać tej osobie swoje dane kontaktowe."</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"Komunikacja NFC włączona."</string>
    <string name="touch">"Dotknij, aby przesłać"</string>

    <string name="beam_progress">"Odbieram dane zbliżeniowo..."</string>
    <string name="beam_outgoing">"Przesyłam..."</string>
    <string name="beam_complete">"Przesyłanie zbliżeniowe zakończone"</string>
    <string name="beam_failed">"Przesyłanie zbliżeniowe nie udało się"</string>
    <string name="beam_canceled">"Przesyłanie zbliżeniowe zostało anulowane"</string>
    <string name="cancel">"Anuluj"</string>
    <string name="beam_touch_to_view">"Dotknij, by wyświetlić"</string>
    <string name="beam_handover_not_supported">"Urządzenie odbierające nie pozwala na zbliżeniowe przesyłanie dużych plików."</string>
    <string name="beam_try_again">"Zbliż urządzenia do siebie"</string>
    <string name="beam_busy">Przesyłanie w toku. Spróbuj ponownie po zakończeniu trwającego przesyłania.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">urządzenie</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral">Łączenie z <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral">Połączono z <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed">Nie można połączyć z <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral">Rozłączanie z <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral">Rozłączono z <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral">Parowanie z <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed">Nie można sparować z <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"Nie można włączyć Bluetootha"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"Czy na pewno chcesz sparować urządzenie Bluetooth <xliff:g id="DEVICE_NAME">%1$s</xliff:g>?"</string>
    <string name="pair_yes">"Tak"</string>
    <string name="pair_no">"Nie"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"Kliknij ponownie, by zapłacić, używając <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"Kliknij ponownie, by wykonać w <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"Tej transakcji nie można wykonać w <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"Nie można użyć <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"Zapłać w"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"Wykonaj w"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"Twoja preferowana usługa systemu dotknij i zapłać została usunięta. Wybrać inną?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"Dotknij innego urządzenia, by zakończyć"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"Połącz"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"Nie można połączyć z siecią"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"Połączono"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"Połącz z siecią"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"Połączyć z siecią <xliff:g id="NETWORK_SSID">%1$s</xliff:g>?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"Android Beam wymaga włączenia NFC. Włączyć?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"Android Beam"</string>
</resources>
