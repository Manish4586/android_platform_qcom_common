<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Service NFC"</string>
    <string name="nfcUserLabel">"NFC"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"Contact reçu via NFC"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"Appuyez pour ajouter cette personne à vos contacts."</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"Communication NFC terminée."</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"Appuyez pour communiquer vos coordonnées à cette personne."</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"NFC activée"</string>
    <string name="touch">"Appuyer pour partager"</string>

    <string name="beam_progress">"Partage entrant..."</string>
    <string name="beam_outgoing">"Partage en cours…"</string>
    <string name="beam_complete">"Partage terminé."</string>
    <string name="beam_failed">"Échec du partage."</string>
    <string name="beam_canceled">"Partage annulé."</string>
    <string name="cancel">"Annuler"</string>
    <string name="beam_touch_to_view">"Appuyer pour afficher"</string>
    <string name="beam_handover_not_supported">"Le récepteur n\'est pas compatible avec le transfert de fichiers volumineux via le partage."</string>
    <string name="beam_try_again">"Mettre de nouveau les appareils côte à côte"</string>
    <string name="beam_busy">Le partage est actuellement indisponible. Réessayez une fois le précédent transfert terminé.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">appareil</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral">Connexion de l\'appareil <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral">Appareil <xliff:g id="device_name">%1$s</xliff:g> connecté</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed">Impossible de connecter l\'appareil <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral">Déconnexion de l\'appareil <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral">Appareil <xliff:g id="device_name">%1$s</xliff:g> déconnecté</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral">Association de l\'appareil <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed">Impossible d\'associer l\'appareil <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"Impossible d\'activer le Bluetooth."</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"Voulez-vous vraiment associer l\'appareil Bluetooth <xliff:g id="DEVICE_NAME">%1$s</xliff:g> ?"</string>
    <string name="pair_yes">"Oui"</string>
    <string name="pair_no">"Non"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"Appuyez à nouveau pour payer avec <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"Appuyez à nouveau pour terminer la transaction avec <xliff:g id="APP">%1$s</xliff:g>"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"Impossible d\'effectuer cette transaction avec <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"Impossible d\'utiliser <xliff:g id="APP">%1$s</xliff:g>."</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"Payer avec"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"Effectuer avec"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"Le service par défaut de paiement sans contact a été supprimé. Voulez-vous en sélectionner un autre ?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"Approchez votre téléphone d\'un autre appareil pour finaliser l\'envoi."</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"Se connecter"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"Impossible de se connecter au réseau."</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"Connecté"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"Se connecter au réseau"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"Se connecter au réseau <xliff:g id="NETWORK_SSID">%1$s</xliff:g> ?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"L\'activation de la NFC est nécessaire pour utiliser Android Beam. Voulez-vous l\'activer ?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"Android Beam"</string>
</resources>
