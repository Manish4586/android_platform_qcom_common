<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"Служба NFC"</string>
    <string name="nfcUserLabel">"NFC"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"Данные контакта получены по NFC"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"Нажмите, чтобы добавить контакт."</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"Обмен данными по NFC завершен"</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"Нажмите, чтобы передать свои контактные данные."</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"Служба NFC включена"</string>
    <string name="touch">"Нажмите, чтобы передать данные"</string>

    <string name="beam_progress">"Получение данных…"</string>
    <string name="beam_outgoing">"Передача данных..."</string>
    <string name="beam_complete">"Передача данных завершена"</string>
    <string name="beam_failed">"Передача данных не завершена"</string>
    <string name="beam_canceled">"Передача данных отменена"</string>
    <string name="cancel">"Отмена"</string>
    <string name="beam_touch_to_view">"Нажмите, чтобы просмотреть"</string>
    <string name="beam_handover_not_supported">"Устройство получателя не поддерживает передачу больших объемов данных через Android Beam."</string>
    <string name="beam_try_again">"Повторить попытку"</string>
    <string name="beam_busy">В настоящий момент функция Beam занята. Повторите попытку после завершения предыдущей передачи.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">устройство</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral">Подключение к <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral">Подключено к <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed">Невозможно подключиться к <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral">Идет отключение от <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral">Отключено от <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral">Сопряжение с <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed">Не удалось выполнить сопряжение с <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"Не удалось включить Bluetooth"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"Подключить Bluetooth-устройство <xliff:g id="DEVICE_NAME">%1$s</xliff:g>?"</string>
    <string name="pair_yes">"Да"</string>
    <string name="pair_no">"Нет"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"Чтобы использовать для оплаты приложение \"<xliff:g id="APP">%1$s</xliff:g>\", снова приблизьте телефон"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"Чтобы использовать приложение \"<xliff:g id="APP">%1$s</xliff:g>\", снова приблизьте телефон"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"Для этой операции нельзя использовать приложение \"<xliff:g id="APP">%1$s</xliff:g>\""</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"Невозможно использовать приложение \"<xliff:g id="APP">%1$s</xliff:g>\""</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"Способ оплаты"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"Использовать"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"Вы удалили приложение для бесконтактной оплаты, используемое по умолчанию. Выбрать другое?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"Нажмите на экран другого устройства, чтобы передать данные"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"Подключиться"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"Не удалось подключиться к сети"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"Соединение установлено"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"Подключение к сети"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"Подключиться к сети <xliff:g id="NETWORK_SSID">%1$s</xliff:g>?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"Для работы Android Beam требуется NFC. Включить?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"Android Beam"</string>
</resources>
