    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">ကိရိယာ</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ကုိခ်ိတ္ဆက္ေနသည္</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ကုိခ်ိတ္ဆက္ၿပီးျဖစ္သည္</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> ထံ ခ်ိတ္ဆက္၍ မရႏုိင္ပါ</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ကုိခ်ိတ္ဆက္မႈမွ ျဖဳတ္ေနသည္</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ကုိခ်ိတ္ဆက္မႈမွ ျဖဳတ္ၿပီးျဖစ္သည္</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ကုိ တြဲေနသည္</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> ကုိ တြဲ၍ မရႏုိင္ပါ</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">ဘလူးတုသ္ကို မရရွိႏိုင္ပါ</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">ဘလူးတုသ္စက္<xliff:g id="DEVICE_NAME">%1$s</xliff:g>ႏွင့္ တြဲခ်ိတ္ရန္ က်ိန္းေသပါသလား?</string>
    <string name="pair_yes">မွန္ပါသည္</string>
    <string name="pair_no">ဟင့္အင္း</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay"><xliff:g id="APP">%1$s</xliff:g>ႏွင့္ ေပးေခ်ရန္ ေနာက္တစ္ခါ ေခါက္ထိပါ</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete"><xliff:g id="APP">%1$s</xliff:g>ႏွင့္ၿပီးဆံုးရန္ ေနာက္တစ္ခါ ေခါက္ထိပါ</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure"><xliff:g id="APP">%1$s</xliff:g>ႏွင့္ လုပ္ငန္းစဥ္္မၿပီးဆံုးႏိုင္ပါ</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app"><xliff:g id="APP">%1$s</xliff:g>ကို အသံုးျပဳ၍ မရပါ</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">ျဖင့္ ေငြေပးခ်ေမည္</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">ျဖင့္ၿပီးဆံုးပါၿပီ</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">သင့္ပိုမိုအနွစ္သက္ေသာ tap &amp; payစနစ္အတြက္ ဝန္ေဆာင္မႈမွာ ဖယ္ရွားထားပါသည္ ေနာက္ထပ္တစ္ခုကို ေရြးမည္လား?</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">ၿပီးဆံုးေစရန္ ေနာက္ထပ္စက္ကို တို႔ထိပါ</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">ခ်ိတ္ဆက္ရန္</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">ကြန္ရက္ကို ခ်ိတ္ဆက္ မရခဲ့</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">ခ်ိတ္ဆက္ထားၿပီး</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">ကြန္ယက္ကို ဆက္သြယ္ရန္</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">ကြန္ရက္ <xliff:g id="NETWORK_SSID">%1$s</xliff:g>ကို ခ်ိတ္ဆက္ရမလား?</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">သင္ NFC ဖြင့္ရန္ အန္းဒ႐ြိဳက္ ဘီးမ္ လိုအပ္သည္။ ၄င္းအား သင္ ဖြင့္လိုသလား?</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">အန္ဒ႐ြိဳက္ ဘီးမ္</string>
</resources>
