    <string name="beam_complete">বিম সম্পূর্ণ</string>
    <string name="beam_failed">বিম সম্পূর্ণ হয় নি</string>
    <string name="beam_canceled">বিম বাতিল করা হয়েছে</string>
    <string name="cancel">বাতিল</string>
    <string name="beam_touch_to_view">দেখার জন্য স্পর্শ করুন।</string>
    <string name="beam_handover_not_supported">গ্রহীতার উপকরণ বিমের মাধ্যমে বড় ফাইল স্থানান্তরে সহায়তা করে না।</string>
    <string name="beam_try_again">ডিভাইসগুলি আবার একত্রিত করুন</string>
