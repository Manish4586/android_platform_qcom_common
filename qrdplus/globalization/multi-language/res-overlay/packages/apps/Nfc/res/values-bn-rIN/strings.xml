    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">ডিভাইস</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> সংযোগ হচ্ছে</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> সংযুক্ত হয়েছে</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> সংযোগ করা যায়নি</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> সংযোগ বিচ্ছিন্ন হচ্ছে</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> সংযোগ বিচ্ছিন্ন হয়েছে</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral"><xliff:g id="device_name">%1$s</xliff:g> পেয়ার করা হচ্ছে</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> পেয়ার করা যায়নি</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">ব্লুটুথ সক্ষম করা যায় নি</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">আপনি কি নিশ্চিত যে আপনি <xliff:g id="device_name">%1$s</xliff:g> এর সাথে জোড়া বাঁধতে চান?</string>
    <string name="pair_yes">হ্যাঁ</string>
    <string name="pair_no">না</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay"><xliff:g id="app">%1$s</xliff:g> ব্যবহার করে প্রদান করতে পুনরায় ট্যাপ করুন</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete"><xliff:g id="app">%1$s</xliff:g>টি ব্যবহার করে শেষ করতে পুনরায় ট্যাপ করুন</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">এই লেনদেনটি <xliff:g id="app">%1$s</xliff:g> ব্যবহার করে সম্পূর্ণ করা যায়নি।</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app"><xliff:g id="app">%1$s</xliff:g> ব্যবহার করা যায়নি।</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">এটি ব্যবহার করে প্রদান করুন</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">এটি দিয়ে সম্পূর্ণ করুন</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">ট্যাপ &amp; প্রদান করার জন্য আপনার পছন্দের পরিষেবাটি অপসারিত হয়েছে। অন্য একটি চয়ন করবেন?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">সম্পূর্ণ করতে অন্য একটি ডিভাইস ট্যাপ করুন</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">সংযোগ করুন</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">নেটওয়ার্কে সংযোগ করতে অক্ষম</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">সংযুক্ত</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">নেটওয়ার্কে সংযোগ করুন</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network"><xliff:g id="network_ssid">%1$s</xliff:g> নেটওয়ার্কে সংযোগ করবেন?</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">Android Beam-এর NFC সক্ষম হওয়া প্রয়োজন। আপনি কি এটি সক্ষম করতে চান?</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">Android বিম</string>
</resources>
