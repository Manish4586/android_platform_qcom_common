    <string name="app_name">"Nfc சேவை"</string>
    <string name="nfcUserLabel">"Nfc"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"NFC வழியாக தொடர்பு பெறப்பட்டது"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"தொடர்பாக இவரைச் சேர்க்கத் தொடவும்."</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"NFC இடைவினை முழுமையடைந்தது"</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"இவருக்கு உங்கள் தொடர்புத் தகவலை வழங்கத் தொடவும்."</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"NFC இயக்கப்பட்டது."</string>
    <string name="touch">"பீம் செய்யத் தட்டவும்"</string>

    <string name="beam_progress">"உள்வரும் பீம்..."</string>
    <string name="beam_outgoing">"பரிமாற்றுகிறது..."</string>
    <string name="beam_complete">"பீம் முடிந்தது"</string>
    <string name="beam_failed">"பீம் ஐ முடிக்க முடியவில்லை"</string>
    <string name="beam_canceled">"பீம் ரத்து செய்யப்பட்டது"</string>
    <string name="cancel">"ரத்துசெய்"</string>
    <string name="beam_touch_to_view">"பார்க்கத் தொடவும்"</string>
    <string name="beam_handover_not_supported">"பெறுநரின் சாதனம் பீம் வழியான பெரிய கோப்புப் பரிமாற்றத்தை ஆதரிக்கவில்லை."</string>
    <string name="beam_try_again">"மீண்டும் சாதனங்களை ஒன்றாகக் கொண்டுவரவும்"</string>
    <string name="beam_busy">பீம் தற்போது பணிமிகுதியாக உள்ளது. முந்தைய இடமாற்றம் முடிந்ததும் மீண்டும் முயற்சிக்கவும்.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">சாதனம்</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g>-ஐ இணைத்தல்</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> இணைக்கப்பட்டது</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> உடன் இணைக்க முடியவில்லை</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g>-ஐத் துண்டித்தல்</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> துண்டிக்கப்பட்டது</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral"><xliff:g id="device_name">%1$s</xliff:g>-ஐப் பிணைத்தல்</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g>-ஐப் பிணைக்க முடியவில்லை</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"புளூடூத்தைச் செயல்படுத்த முடியவில்லை"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"புளூடூத் சாதனம் <xliff:g id="DEVICE_NAME">%1$s</xliff:g> உடன் இணைக்க வேண்டும் என்று விரும்புகிறீர்களா?"</string>
    <string name="pair_yes">"ஆம்"</string>
    <string name="pair_no">இல்ல.</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"<xliff:g id="APP">%1$s</xliff:g> உடன் கட்டணம் செலுத்த மீண்டும் தட்டவும்"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"<xliff:g id="APP">%1$s</xliff:g> உடன் நிறைவு செய்ய மீண்டும் தட்டவும்"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"இந்தப் பரிவர்த்தனையை <xliff:g id="APP">%1$s</xliff:g> உடன் முடிக்க முடியவில்லை."</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"<xliff:g id="APP">%1$s</xliff:g> ஐப் பயன்படுத்த முடியவில்லை."</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"இதனுடன் செலுத்து"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"இதனுடன் நிறைவு செய்"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"தட்டி கட்டண செலுத்தலுக்கான உங்களின் முன்னுரிமை சேவை அகற்றப்பட்டது. மற்றொன்றைத் தேர்ந்தெடுக்கவா?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"முடிக்க மற்றொரு சாதனத்தைத் தட்டவும்"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"இணை"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"நெட்வொர்க்குடன் இணைக்க முடியவில்லை"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"இணைக்கப்பட்டது"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"நெட்வொர்க்குடன் இணை"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"<xliff:g id="NETWORK_SSID">%1$s</xliff:g> நெட்வொர்க்குடன் இணைக்கவா?"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"Android பீம்மிற்கு NFC இயக்கப்பட்டிருக்க வேண்டும். இயக்கவா?"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"Android பீம்"</string>
