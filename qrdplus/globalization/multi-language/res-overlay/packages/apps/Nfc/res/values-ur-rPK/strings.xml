<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">"‏Nfc سروس"</string>
    <string name="nfcUserLabel">"Nfc"</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">"‏NFC پر موصولہ رابطہ"</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">"اس شخص کو ایک رابطہ کے بطور شامل کرنے کیلئے ٹچ کریں۔"</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">"‏NFC تعامل مکمل ہو گیا"</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">"اس شخص کو اپنے رابطہ کی معلومات دینے کیلئے ٹچ کریں۔"</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">"‏NFC فعال ہے۔"</string>
    <string name="touch">"بیم کیلئے ٹچ کریں"</string>

    <string name="beam_progress">"بیم موصول ہو رہا ہے…"</string>
    <string name="beam_outgoing">"بیم کی جا رہی ہے…"</string>
    <string name="beam_complete">"بیم مکمل ہو گیا"</string>
    <string name="beam_failed">"بیم مکمل نہیں ہوا"</string>
    <string name="beam_canceled">"بیم منسوخ ہو گیا"</string>
    <string name="cancel">"منسوخ کریں"</string>
    <string name="beam_touch_to_view">"دیکھنے کیلئے ٹچ کریں"</string>
    <string name="beam_handover_not_supported">"موصول کرنے والے کا آلہ بیم کے ذریعے بڑی فائل کی منتقلی کا تعاون نہیں کرتا ہے۔"</string>
    <string name="beam_try_again">"آلات کو دوبارہ سے ساتھ لائیں"</string>
    <string name="beam_busy">بیم فی الحال مصروف ہے۔ دوبارہ کوشش کریں جب سابقہ منتقلی مکمل ہو۔</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">آلہ</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> سے مربوط ہو رہا ہے</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> سے مربوط ہے</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> سے مربوط نہ ہو سکا</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> سے منقطع کر رہا ہے</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> سے منقطع ہو گیا ہے</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral"><xliff:g id="device_name">%1$s</xliff:g> سے جوڑا بنا رہا ہے</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> سے جوڑا نہ بنا سکا</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">"بلوٹوتھ فعال نہیں ہو سکا"</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">"کیا آپ واقعی بلوٹوتھ آلہ <xliff:g id="DEVICE_NAME">%1$s</xliff:g> کا جوڑا بنانا چاہتے ہیں؟"</string>
    <string name="pair_yes">"ہاں"</string>
    <string name="pair_no">"نہیں"</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay">"<xliff:g id="APP">%1$s</xliff:g> کے ساتھ ادائیگی کرنے کیلئے دوبارہ تھپتھپائیں"</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete">"<xliff:g id="APP">%1$s</xliff:g> کے ساتھ مکمل کرنے کیلئے دوبارہ تھپتھپائیں"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">"یہ لین دین <xliff:g id="APP">%1$s</xliff:g> کے ساتھ مکمل نہیں ہو سکا۔"</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app">"<xliff:g id="APP">%1$s</xliff:g> کا استعمال نہیں کر سکا۔"</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">"اس کے ساتھ ادائیگی کریں"</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">"اس کے ساتھ مکمل کریں"</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">"تھپتھپائیں اور ادائیگی کریں کیلئے آپ کی پسندیدہ سروس کو ہٹا دیا گیا۔ کوئی دوسری سروس منتخب کریں؟"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">"مکمل کرنے کیلئے کوئی دوسرا آلہ تھپتھپائیں"</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">"مربوط کریں"</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">"نیٹ ورک سے رابطہ قائم کرنے سے قاصر"</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">"مربوط"</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">"نیٹ ورک سے مربوط ہوں"</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">"نیٹ ورک <xliff:g id="NETWORK_SSID">%1$s</xliff:g> سے مربوط کریں؟"</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">"‏Android Beam کو فعال کیے جانے کیلئے NFC درکار ہے۔ کیا آپ اسے فعال کرنا چاہتے ہیں؟"</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">"Android Beam"</string>
</resources>
