<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="app_name">Nfc સેવા</string>
    <string name="nfcUserLabel">Nfc</string>

    <!-- A notification description string informing the user that contact details were received over NFC [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_title">NFC પર સંપર્ક પ્રાપ્ત થયો</string>

    <!-- A notification action string prompting the user to add contact details to the People application [CHAR-LIMIT=64] -->
    <string name="inbound_me_profile_text">આ વ્યક્તિને સંપર્ક તરીકે ઉમેરવા માટે ટચ કરો.</string>

    <!-- A notification description string asking the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_title">NFC ક્રિયાપ્રતિક્રિયા પૂર્ણ</string>

    <!-- A notification action string prompting the user to share their contact details over NFC [CHAR-LIMIT=64] -->
    <string name="outbound_me_profile_text">આ વ્યક્તિને તમારી સંપર્ક માહિતી આપવા માટે ટચ કરો.</string>

    <!-- Content description of the NFC enabled notification icon for accessibility (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_nfc_enabled">NFC સક્ષમ છે.</string>
    <string name="touch">બીમ કરવા માટે ટચ કરો</string>

    <string name="beam_progress">ઇનકમિંગ બીમ...</string>
    <string name="beam_outgoing">બીમ થઈ રહ્યું છે
...</string>
    <string name="beam_complete">બીમ પૂર્ણ</string>
    <string name="beam_failed">બીમ પૂર્ણ થયું નથી</string>
    <string name="beam_canceled">બીમ રદ કર્યું</string>
    <string name="cancel">રદ કરો</string>
    <string name="beam_touch_to_view">જોવા માટે ટચ કરો</string>
    <string name="beam_handover_not_supported">રીસીવરનું ઉપકરણ મોટા બીમ દ્વારા મોટા ફાઇલ ટ્રાન્સફરને સમર્થન આપતું નથી.</string>
    <string name="beam_try_again">ઉપકરણોને ફરી એકસાથે લાવો</string>
    <string name="beam_busy">બીમ હાલમાં વ્યસ્ત છે. પાછલું સ્થાનાંતર પૂર્ણ થાય ત્યારે ફરી પ્રયાસ કરો.</string>

    <!-- String referring to the device being connected when there is no name for the device -->
    <string name="device">ઉપકરણ</string>
    <!-- Toast string informing the user that the device is connecting [CHAR_LIMIT=35] -->
    <string name="connecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> થી જોડાઈ રહ્યું છે</string>
    <!-- Toast string informing the user that the device is connected [CHAR_LIMIT=35] -->
    <string name="connected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> થી જોડાયું</string>
    <!-- Toast string informing the user that the device failed to connect [CHAR_LIMIT=35] -->
    <string name="connect_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> થી જોડાઈ શક્યું નથી</string>
    <!-- Toast string informing the user that the device is disconnecting [CHAR_LIMIT=35] -->
    <string name="disconnecting_peripheral"><xliff:g id="device_name">%1$s</xliff:g> થી જોડાણ દૂર કરી રહ્યું છે</string>
    <!-- Toast string informing the user that the device is disconnected [CHAR_LIMIT=35] -->
    <string name="disconnected_peripheral"><xliff:g id="device_name">%1$s</xliff:g> થી જોડાણ દૂર કર્યુ</string>
    <!-- Toast string informing the user that the device is pairing [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral"><xliff:g id="device_name">%1$s</xliff:g> ની જોડી બનાવી રહ્યું છે</string>
    <!-- Toast string informing the user that the device failed to pair [CHAR_LIMIT=35] -->
    <string name="pairing_peripheral_failed"><xliff:g id="device_name">%1$s</xliff:g> ની જોડી બનાવી શકાઈ નથી</string>
    <!-- Toast string informing the user that BT could not be enabled [CHAR_LIMIT=35] -->
    <string name="failed_to_enable_bt">બ્લુટૂથ અક્ષમ કરી શકાયું નથી</string>
    <!-- Dialog string informing the user that BT could not be enabled [CHAR_LIMIT=75] -->
    <string name="confirm_pairing">શું તમે ખરેખર બ્લુટૂથ ઉપકરણ <xliff:g id="device_name">%1$s</xliff:g> ની જોડી બનાવવા માગો છો?</string>
    <string name="pair_yes">હા</string>
    <string name="pair_no">નહીં</string>

    <!-- String that is prompting the user to tap his NFC phone against a payment reader again, to
         pay with the specified app -->
    <string name="tap_again_to_pay"><xliff:g id="app">%1$s</xliff:g> સાથે ચલાવવા માટે ફરીથી ટેપ કરો</string>
    <!-- String that is prompting the user to tap his NFC phone against a generic NFC reader again, to
         pay with the specified app -->
    <string name="tap_again_to_complete"><xliff:g id="app">%1$s</xliff:g> સાથે પૂર્ણ કરવા માટે ફરીથી ટેપ કરો</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="transaction_failure">આ લેવડદેવડને <xliff:g id="app">%1$s</xliff:g> સાથે પૂર્ણ કરી શકાઈ નથી.</string>
    <!-- String that is telling the user that his NFC transaction couldn\'t be completed with the
         indicated app -->
    <string name="could_not_use_app"><xliff:g id="app">%1$s</xliff:g> નો ઉપયોગ કરી શકાયો નથી.</string>
    <!-- Label for a dialog showing a list of applications can be paid with -->
    <string name="pay_with">આનાથી ચૂકવો</string>
    <!-- Label for a dialog showing a list of applications that can be used to complete the NFC action -->
    <string name="complete_with">આની સાથે પૂર્ણ કરો</string>
    <!-- String that is telling the user that his default app for tap and pay has been removed,
         and asks him to pick another app as default.-->
    <string name="default_pay_app_removed">ટેપ કરો અને ચૂકવો માટે તમારી પસંદીદા સેવાને દૂર કરવામાં આવી હતી. બીજી પસંદ કરીએ?"</string>
    <!-- Label that is shown when the user must tap another NFC enabled device to send data -->
    <string name="ask_nfc_tap">પૂર્ણ કરવા માટે બીજું ઉપકરણ પસંદ કરો</string>

    <!-- Strings for connecting to a network from WiFi NFC tags -->
    <!-- Dialog button text for confirming the user wants to connect to the network on the tag-->
    <string name="wifi_connect">જોડો</string>
    <!-- Text informing the user that an error ocurred during the process of connecting to the network-->
    <string name="status_unable_to_connect">નેટવર્કથી જોડવામાં અસમર્થ</string>
    <!-- Text informing the user that the network was connected to successfully -->
    <string name="status_wifi_connected">જોડેલ</string>
    <!-- Title for dialog where user confirms that they want to connect to the network on the tag they tapped-->
    <string name="title_connect_to_network">નેટવર્કથી જોડાઓ</string>
    <!-- Message prompt asking the user if they wish to connect to the given network. Contains the network name (ssid).-->
    <string name="prompt_connect_to_network">નેટવર્ક <xliff:g id="network_ssid">%1$s</xliff:g> થી જોડીએ?</string>

    <!-- Message shown in a dialog when user wants to use Android Beam, but NFC is not enabled -->
    <string name="beam_requires_nfc_enabled">એન્ડ્રોઇડ બીમ માટે NFC સક્ષમ હોવું જરૂરી છે. શું તમે તેને સક્ષમ કરવા માગો છો?</string>

    <!-- Used to refer to in the Share Menu Android Beam . This phrase is a trademark. [CHAR LIMIT=32] -->
    <string name="android_beam">એન્ડ્રોઇડ બીમ</string>
</resources>
