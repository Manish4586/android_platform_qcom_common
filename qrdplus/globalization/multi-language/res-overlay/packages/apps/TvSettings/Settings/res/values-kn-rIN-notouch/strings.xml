    <!-- Daydream description for configuring the time before the device goes to sleep [CHAR LIMIT=NONE] -->
    <string name="device_daydreams_sleep_description">"ಈ ನಿಷ್ಕ್ರಿಯ ಅವಧಿಯ ಬಳಿಕ ಡೇಡ್ರೀಮಿಂಗ್ ಪ್ರಾರಂಭವಾಗುತ್ತದೆ. ಯಾವುದೇ ಡೇಡ್ರೀಮ್ ಆಯ್ಕೆ ಮಾಡಿಲ್ಲದಿದ್ದರೆ, ಪ್ರದರ್ಶನ ಆಫ್ ಆಗುತ್ತದೆ."</string>
    <!-- Daydream summary for displaying the time before the device goes to sleep -->
    <string name="device_daydreams_sleep_summary">"ನಿಷ್ಕ್ರಿಯತೆಯ <xliff:g id="SLEEP_DESCRIPTION">%1$s</xliff:g> ರ ಬಳಿಕ"</string>
    <!-- Daydream choice for configuring the time before the device\'s screen goes off after dreaming [CHAR LIMIT=40] -->
