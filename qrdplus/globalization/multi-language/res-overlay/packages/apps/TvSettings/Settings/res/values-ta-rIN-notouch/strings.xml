    <string name="device_daydreams_sleep">"முழுநேர இயக்கத்தில் இருக்கும்போது"</string>
    <!-- Daydream description for configuring the time before the device goes to sleep [CHAR LIMIT=NONE] -->
    <string name="device_daydreams_sleep_description">"செயல்படா நேரத்திற்குப் பின், முழுநேர இயக்கம் தொடங்கும். முழுநேர இயக்கம் எதுவும் தேர்ந்தெடுக்கப்படவில்லை எனில், திரை முடக்கப்படும்."</string>
    <!-- Daydream summary for displaying the time before the device goes to sleep -->
    <string name="device_daydreams_sleep_summary">"செயல்படாத <xliff:g id="SLEEP_DESCRIPTION">%1$s</xliff:g> க்குப் பின்"</string>
    <!-- Daydream choice for configuring the time before the device\'s screen goes off after dreaming [CHAR LIMIT=40] -->
    <string name="device_daydreams_screen_off">"உறக்கத்திற்குச் செல்லவேண்டிய நேரம்"</string>
