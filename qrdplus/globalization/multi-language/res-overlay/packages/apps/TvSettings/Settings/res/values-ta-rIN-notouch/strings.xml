    <!-- Label for list item that indicates no notifications exist [CHAR LIMIT=40] -->
    <string name="no_notifications">ஏதும.</string>
    <!-- Label for list item for toggling the "Do Not Disturb" setting ON/OFF [CHAR LIMIT=40] -->
