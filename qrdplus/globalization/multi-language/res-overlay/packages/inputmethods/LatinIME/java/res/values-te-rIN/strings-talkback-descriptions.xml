<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Spoken description to let the user know that when typing in a password, they can plug in a headset in to hear spoken descriptions of the keys they type. [CHAR LIMIT=NONE] -->
    <string name="spoken_use_headphones">"బిగ్గరగా చదివి వినిపించే పాస్‌వర్డ్ కీలను వినడానికి హెడ్‌సెట్‌ను ప్లగిన్ చేయండి."</string>

    <!-- Spoken description for the currently entered text -->
    <string name="spoken_current_text_is">"ప్రస్తుత వచనం %s"</string>
    <!-- Spoken description when there is no text entered -->
    <string name="spoken_no_text_entered">"వచనం ఏదీ నమోదు చేయబడలేదు"</string>

    <!-- Spoken description to let the user know what auto-correction will be performed when a key is pressed. An auto-correction replaces a single word with one or more words. -->
    <string name="spoken_auto_correct">"<xliff:g id="KEY_NAME">%1$s</xliff:g> <xliff:g id="ORIGINAL_WORD">%2$s</xliff:g>ని <xliff:g id="CORRECTED_WORD">%3$s</xliff:g>గా సరి చేస్తుంది"</string>
    <!-- Spoken description used during obscured (e.g. password) entry to let the user know that auto-correction will be performed when a key is pressed. -->
    <string name="spoken_auto_correct_obscured">"<xliff:g id="KEY_NAME">%1$s</xliff:g> స్వీయ-సవరణను అమలు చేస్తుంది"</string>

    <!-- Spoken description for unknown keyboard keys. -->
    <string name="spoken_description_unknown">"తెలియని అక్షరం"</string>
    <!-- Spoken description for the "Shift" keyboard key when "Shift" is off. -->
    <string name="spoken_description_shift">"షిప్ట్"</string>
    <!-- Spoken description for the "Shift" keyboard key in symbols mode. -->
    <string name="spoken_description_symbols_shift">"మరిన్ని గుర్తులు"</string>
    <!-- Spoken description for the "Shift" keyboard key when "Shift" is on. -->
    <string name="spoken_description_shift_shifted">"షిప్ట్"</string>
    <!-- Spoken description for the "Shift" keyboard key in 2nd symbols (a.k.a. symbols shift) mode. -->
    <string name="spoken_description_symbols_shift_shifted">"గుర్తులు"</string>
    <!-- Spoken description for the "Shift" keyboard key when "Caps lock" is on. -->
    <string name="spoken_description_caps_lock">"షిప్ట్"</string>
    <!-- Spoken description for the "Delete" keyboard key. -->
    <string name="spoken_description_delete">"తొలగించు"</string>
    <!-- Spoken description for the "To Symbol" keyboard key. -->
    <string name="spoken_description_to_symbol">"గుర్తులు"</string>
    <!-- Spoken description for the "To Alpha" keyboard key. -->
    <string name="spoken_description_to_alpha">"అక్షరాలు"</string>
    <!-- Spoken description for the "To Numbers" keyboard key. -->
    <string name="spoken_description_to_numeric">"సంఖ్యలు"</string>
    <!-- Spoken description for the "Settings" keyboard key. -->
    <string name="spoken_description_settings">"సెట్టింగ్‌లు"</string>
    <!-- Spoken description for the "Tab" keyboard key. -->
    <string name="spoken_description_tab">"ట్యాబ్"</string>
    <!-- Spoken description for the "Space" keyboard key. -->
    <string name="spoken_description_space">"స్పేస్"</string>
    <!-- Spoken description for the "Mic" keyboard key. -->
    <string name="spoken_description_mic">"వాయిస్ ఇన్‌పుట్"</string>
    <!-- Spoken description for the "Emoji" keyboard key. -->
    <string name="spoken_description_emoji">"ఎమోజి"</string>
    <!-- Spoken description for the "Return" keyboard key. -->
    <string name="spoken_description_return">"తిరిగి వెళ్లు"</string>
    <!-- Spoken description for the "Search" keyboard key. -->
    <string name="spoken_description_search">"శోధించు"</string>
    <!-- Spoken description for the "U+2022" (BULLET) keyboard key. -->
    <string name="spoken_description_dot">"చుక్క"</string>
    <!-- Spoken description for the "Switch language" keyboard key. -->
    <string name="spoken_description_language_switch">"భాషను మార్చండి"</string>
    <!-- Spoken description for the "Next" action keyboard key. -->
    <string name="spoken_description_action_next">"తదుపరి"</string>
    <!-- Spoken description for the "Previous" action keyboard key. -->
    <string name="spoken_description_action_previous">"మునుపటి"</string>

    <!-- Spoken feedback after turning "Shift" mode on. -->
    <string name="spoken_description_shiftmode_on">"షిఫ్ట్ ప్రారంభించబడింది"</string>
    <!-- Spoken feedback after turning "Caps lock" mode on. -->
    <string name="spoken_description_shiftmode_locked">"Caps lock ప్రారంభించబడింది"</string>
    <!-- Spoken feedback after changing to the symbols keyboard. -->
    <string name="spoken_description_mode_symbol">"గుర్తుల మోడ్"</string>
    <!-- Spoken feedback after changing to the 2nd symbols (a.k.a. symbols shift) keyboard. -->
    <string name="spoken_description_mode_symbol_shift">"మరిన్ని గుర్తుల మోడ్"</string>
    <!-- Spoken feedback after changing to the alphanumeric keyboard. -->
    <string name="spoken_description_mode_alpha">"అక్షరాల మోడ్"</string>
    <!-- Spoken feedback after changing to the phone dialer keyboard. -->
    <string name="spoken_description_mode_phone">"ఫోన్ మోడ్"</string>
    <!-- Spoken feedback after changing to the shifted phone dialer (symbols) keyboard. -->
    <string name="spoken_description_mode_phone_shift">"ఫోన్ గుర్తుల మోడ్"</string>

    <!-- Spoken feedback when the keyboard is hidden. -->
    <string name="announce_keyboard_hidden">"కీబోర్డ్ దాచబడింది"</string>
    <!-- Spoken feedback when the keyboard mode changes. -->
    <string name="announce_keyboard_mode">"<xliff:g id="KEYBOARD_MODE">%s</xliff:g> కీబోర్డ్‌ను చూపుతోంది"</string>
    <!-- Description of the keyboard mode for entering dates. -->
    <string name="keyboard_mode_date">"తేదీ"</string>
    <!-- Description of the keyboard mode for entering dates and times. -->
    <string name="keyboard_mode_date_time">"తేదీ మరియు సమయం"</string>
    <!-- Description of the keyboard mode for entering email addresses. -->
    <string name="keyboard_mode_email">"ఇమెయిల్"</string>
    <!-- Description of the keyboard mode for entering text messages. -->
    <string name="keyboard_mode_im">"సందేశం"</string>
    <!-- Description of the keyboard mode for entering numbers. -->
    <string name="keyboard_mode_number">"సంఖ్య"</string>
    <!-- Description of the keyboard mode for entering phone numbers. -->
    <string name="keyboard_mode_phone">"ఫోన్"</string>
    <!-- Description of the keyboard mode for entering generic text. -->
    <string name="keyboard_mode_text">"వచనం"</string>
    <!-- Description of the keyboard mode for entering times. -->
    <string name="keyboard_mode_time">"సమయం"</string>
    <!-- Description of the keyboard mode for entering URLs. -->
    <string name="keyboard_mode_url">"URL"</string>

    <!-- Description of the emoji category icon of Recents. -->
    <string name="spoken_descrption_emoji_category_recents">"ఇటీవలివి"</string>
    <!-- Description of the emoji category icon of People. -->
    <string name="spoken_descrption_emoji_category_people">"వ్యక్తులు"</string>
    <!-- Description of the emoji category icon of Objects. -->
    <string name="spoken_descrption_emoji_category_objects">"ఆబ్జెక్ట్‌లు"</string>
    <!-- Description of the emoji category icon of Nature. -->
    <string name="spoken_descrption_emoji_category_nature">"ప్రకృతి"</string>
    <!-- Description of the emoji category icon of Places. -->
    <string name="spoken_descrption_emoji_category_places">"స్థలాలు"</string>
    <!-- Description of the emoji category icon of Symbols. -->
    <string name="spoken_descrption_emoji_category_symbols">"గుర్తులు"</string>
    <!-- Description of the emoji category icon of Emoticons. -->
    <string name="spoken_descrption_emoji_category_emoticons">"ఎమోటికాన్‌లు"</string>

    <!-- Description of an upper case letter of LOWER_LETTER. -->
    <string name="spoken_description_upper_case">"క్యాపిటల్ <xliff:g id="LOWER_LETTER">%s</xliff:g>"</string>
    <!-- Spoken description for Unicode code point U+0049: "I" LATIN CAPITAL LETTER I
         Note that depending on locale, the lower-case of this letter is U+0069 or U+0131. -->
    <string name="spoken_letter_0049">"క్యాపిటల్ I"</string>
    <!-- Spoken description for Unicode code point U+0130: "İ" LATIN CAPITAL LETTER I WITH DOT ABOVE
         Note that depending on locale, the lower-case of this letter is U+0069 or U+0131. -->
    <string name="spoken_letter_0130">"క్యాపిటల్ I, ఎగువన చుక్క"</string>
    <!-- Spoken description for unknown symbol code point. -->
    <string name="spoken_symbol_unknown">"తెలియని చిహ్నం"</string>
    <!-- Spoken description for unknown emoji code point. -->
    <string name="spoken_emoji_unknown">"తెలియని ఎమోజీ"</string>

    <!-- Spoken descriptions when opening a more keys keyboard that has alternative characters. -->
    <string name="spoken_open_more_keys_keyboard">"ప్రత్యామ్నాయ అక్షరాలు అందుబాటులో ఉన్నాయి"</string>
    <!-- Spoken descriptions when closing a more keys keyboard that has alternative characters. -->
    <string name="spoken_close_more_keys_keyboard">"ప్రత్యామ్నాయ అక్షరాలు తీసివేయబడ్డాయి"</string>

    <!-- Spoken descriptions when opening a more suggestions panel that has alternative suggested words. -->
    <string name="spoken_open_more_suggestions">"ప్రత్యామ్నాయ సూచనలు అందుబాటులో ఉన్నాయి"</string>
    <!-- Spoken descriptions when closing a more suggestions panel that has alternative suggested words. -->
    <string name="spoken_close_more_suggestions">"ప్రత్యామ్నాయ సూచనలు తీసివేయబడ్డాయి"</string>
</resources>
