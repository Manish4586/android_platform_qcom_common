    <string name="spoken_description_action_next">"ถัดไป"</string>
    <!-- Spoken description for the "Previous" action keyboard key. -->
    <string name="spoken_description_action_previous">"ก่อนหน้า"</string>

    <!-- Spoken feedback after turning "Shift" mode on. -->
    <string name="spoken_description_shiftmode_on">"เปิดใช้งาน Shift แล้ว"</string>
    <!-- Spoken feedback after turning "Caps lock" mode on. -->
    <string name="spoken_description_shiftmode_locked">"เปิดใช้งาน Caps Lock แล้ว"</string>
    <!-- Spoken feedback after changing to the symbols keyboard. -->
    <string name="spoken_description_mode_symbol">"โหมดสัญลักษณ์"</string>
