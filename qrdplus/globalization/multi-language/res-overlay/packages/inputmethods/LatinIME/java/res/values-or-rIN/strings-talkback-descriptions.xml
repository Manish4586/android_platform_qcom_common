<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Spoken description to let the user know that when typing in a password, they can plug in a headset in to hear spoken descriptions of the keys they type. [CHAR LIMIT=NONE] -->
    <string name="spoken_use_headphones">ପାସୱାର୍ଡ୍ କୀ ଉଚ୍ଚସ୍ଵରରେ କହିବା ପାଇଁ ହେଡସେଟରେ ଶୁଣିବା ପାଇଁ ପ୍ଲଗ୍ କରନ୍ତୁ।</string>

    <!-- Spoken description for the currently entered text -->
    <string name="spoken_current_text_is">ସମ୍ପ୍ରତି ଟେକ୍ସଟ୍ "%s"</string>
    <!-- Spoken description when there is no text entered -->
    <string name="spoken_no_text_entered">କୌଣସି ଟେକ୍ସଟ୍ ପ୍ରବେଶ ହୋଇନାହିଁ</string>

    <!-- Spoken description to let the user know what auto-correction will be performed when a key is pressed. An auto-correction replaces a single word with one or more words. -->
    <string name="spoken_auto_correct"><xliff:g id="KEY_NAME" example="Space">%1$s</xliff:g> <xliff:g id="ORIGINAL_WORD">%2$s</xliff:g>କୁ <xliff:g id="CORRECTED_WORD">%3$s</xliff:g>ରେ ସଂଶୋଧନ କରନ୍ତୁ</string>
    <!-- Spoken description used during obscured (e.g. password) entry to let the user know that auto-correction will be performed when a key is pressed. -->
    <string name="spoken_auto_correct_obscured"><xliff:g id="KEY_NAME" example="Space">%1$s</xliff:g> ସ୍ଵତଃ-ସଂଶୋଧନ ପ୍ରଦର୍ଶିନ</string>

    <!-- Spoken description for unknown keyboard keys. -->
    <string name="spoken_description_unknown">ଅଜ୍ଞାତ ବର୍ଣ୍ଣ</string>
    <!-- Spoken description for the "Shift" keyboard key when "Shift" is off. -->
    <string name="spoken_description_shift">ସିଫ୍ଟ</string>
    <!-- Spoken description for the "Shift" keyboard key in symbols mode. -->
    <string name="spoken_description_symbols_shift">ଅଧିକ ସଙ୍କେତଗୁଡିକ</string>
    <!-- Spoken description for the "Shift" keyboard key when "Shift" is on. -->
    <string name="spoken_description_shift_shifted">ସିଫ୍ଟ</string>
    <!-- Spoken description for the "Shift" keyboard key in 2nd symbols (a.k.a. symbols shift) mode. -->
    <string name="spoken_description_symbols_shift_shifted">ସଙ୍କେତଗୁଡିକ</string>
    <!-- Spoken description for the "Shift" keyboard key when "Caps lock" is on. -->
    <string name="spoken_description_caps_lock">ସିଫ୍ଟ</string>
    <!-- Spoken description for the "Delete" keyboard key. -->
    <string name="spoken_description_delete">ବିଲୋପ କରନ୍ତୁ</string>
    <!-- Spoken description for the "To Symbol" keyboard key. -->
    <string name="spoken_description_to_symbol">ସଙ୍କେତଗୁଡିକ</string>
    <!-- Spoken description for the "To Alpha" keyboard key. -->
    <string name="spoken_description_to_alpha">ଅକ୍ଷରଗୁଡିକ</string>
    <!-- Spoken description for the "To Numbers" keyboard key. -->
    <string name="spoken_description_to_numeric">ନମ୍ୱରଗୁଡିକ</string>
    <!-- Spoken description for the "Settings" keyboard key. -->
    <string name="spoken_description_settings">ସେଟିଂସ୍‌</string>
    <!-- Spoken description for the "Tab" keyboard key. -->
    <string name="spoken_description_tab">ଟ୍ୟାବ୍</string>
    <!-- Spoken description for the "Space" keyboard key. -->
    <string name="spoken_description_space">ସ୍ପେସ୍</string>
    <!-- Spoken description for the "Mic" keyboard key. -->
    <string name="spoken_description_mic">ଭଏସ୍ ଇନପୁଟ୍</string>
    <!-- Spoken description for the "Emoji" keyboard key. -->
    <string name="spoken_description_emoji">ଇମୋଜି</string>
    <!-- Spoken description for the "Return" keyboard key. -->
    <string name="spoken_description_return">ଫେରନ୍ତୁ</string>
    <!-- Spoken description for the "Search" keyboard key. -->
    <string name="spoken_description_search">ସନ୍ଧାନ</string>
    <!-- Spoken description for the "U+2022" (BULLET) keyboard key. -->
    <string name="spoken_description_dot">ଡଟ୍</string>
    <!-- Spoken description for the "Switch language" keyboard key. -->
    <string name="spoken_description_language_switch">ଭାଷାକୁ ସ୍ୱୁଇଚ୍‌ କରନ୍ତୁ</string>
    <!-- Spoken description for the "Next" action keyboard key. -->
    <string name="spoken_description_action_next">ପରବର୍ତ୍ତୀ</string>
    <!-- Spoken description for the "Previous" action keyboard key. -->
    <string name="spoken_description_action_previous">ପୂର୍ବବର୍ତ୍ତୀ</string>

    <!-- Spoken feedback after turning "Shift" mode on. -->
    <string name="spoken_description_shiftmode_on">ସିଫ୍ଟ୍ ସକ୍ଷମ ହୋଇଛି</string>
    <!-- Spoken feedback after turning "Caps lock" mode on. -->
    <string name="spoken_description_shiftmode_locked">କ୍ୟାପସ୍ ଲକ୍ ସକ୍ଷମ ହୋଇଛି</string>
    <!-- Spoken feedback after changing to the symbols keyboard. -->
    <string name="spoken_description_mode_symbol">ମୋଡ୍ ସଙ୍କେତଗୁଡିକ</string>
    <!-- Spoken feedback after changing to the 2nd symbols (a.k.a. symbols shift) keyboard. -->
    <string name="spoken_description_mode_symbol_shift">ଅଧିକ ସଙ୍କେତ ମୋଡ୍</string>
    <!-- Spoken feedback after changing to the alphanumeric keyboard. -->
    <string name="spoken_description_mode_alpha">ଅକ୍ଷରଗୁଡିକ ମୋଡ୍</string>
    <!-- Spoken feedback after changing to the phone dialer keyboard. -->
    <string name="spoken_description_mode_phone">ଫୋନ୍ ମୋଡ୍</string>
    <!-- Spoken feedback after changing to the shifted phone dialer (symbols) keyboard. -->
    <string name="spoken_description_mode_phone_shift">ଫୋନ୍ ସଙ୍କେତ ମୋଡ୍</string>

    <!-- Spoken feedback when the keyboard is hidden. -->
    <string name="announce_keyboard_hidden">କୀବୋର୍ଡ ଲୁକ୍କାୟିତ</string>
    <!-- Spoken feedback when the keyboard mode changes. -->
    <string name="announce_keyboard_mode"><xliff:g id="KEYBOARD_MODE" example="email">%s</xliff:g> କୀବୋର୍ଡ ଦେଖାଉଛି</string>
    <!-- Description of the keyboard mode for entering dates. -->
    <string name="keyboard_mode_date">ତାରିଖ</string>
    <!-- Description of the keyboard mode for entering dates and times. -->
    <string name="keyboard_mode_date_time">ତାରିଖ ଏବଂ ସମୟ</string>
    <!-- Description of the keyboard mode for entering email addresses. -->
    <string name="keyboard_mode_email">ଇମେଲ୍</string>
    <!-- Description of the keyboard mode for entering text messages. -->
    <string name="keyboard_mode_im">ମେସେଜିଂ</string>
    <!-- Description of the keyboard mode for entering numbers. -->
    <string name="keyboard_mode_number">ନମ୍ଵର୍</string>
    <!-- Description of the keyboard mode for entering phone numbers. -->
    <string name="keyboard_mode_phone">ଫୋନ୍</string>
    <!-- Description of the keyboard mode for entering generic text. -->
    <string name="keyboard_mode_text">ଟେକ୍ସଟ୍</string>
    <!-- Description of the keyboard mode for entering times. -->
    <string name="keyboard_mode_time">ସମୟ</string>
    <!-- Description of the keyboard mode for entering URLs. -->
    <string name="keyboard_mode_url">URL</string>

    <!-- Description of the emoji category icon of Recents. -->
    <string name="spoken_descrption_emoji_category_recents">ସମ୍ପର୍ତିଗୁଡିକ</string>
    <!-- Description of the emoji category icon of People. -->
    <string name="spoken_descrption_emoji_category_people">ଲୋକମାନେ</string>
    <!-- Description of the emoji category icon of Objects. -->
    <string name="spoken_descrption_emoji_category_objects">ଅବଜେକ୍ଟଗୁଡିକ</string>
    <!-- Description of the emoji category icon of Nature. -->
    <string name="spoken_descrption_emoji_category_nature">ପ୍ରକୃତି</string>
    <!-- Description of the emoji category icon of Places. -->
    <string name="spoken_descrption_emoji_category_places">ସ୍ଥାନଗୁଡିକ</string>
    <!-- Description of the emoji category icon of Symbols. -->
    <string name="spoken_descrption_emoji_category_symbols">ସଙ୍କେତଗୁଡିକ</string>
    <!-- Description of the emoji category icon of Emoticons. -->
    <string name="spoken_descrption_emoji_category_emoticons">ଇମୋଟିକୋନ୍‌ଗୁଡିକ</string>

    <!-- Description of an upper case letter of LOWER_LETTER. -->
    <string name="spoken_description_upper_case">ବଡ <xliff:g id="LOWER_LETTER" example="A, E, ligature">%s</xliff:g></string>
    <!-- Spoken description for Unicode code point U+0049: "I" LATIN CAPITAL LETTER I
         Note that depending on locale, the lower-case of this letter is U+0069 or U+0131. -->
    <string name="spoken_letter_0049">ବଡ I</string>
    <!-- Spoken description for Unicode code point U+0130: "İ" LATIN CAPITAL LETTER I WITH DOT ABOVE
         Note that depending on locale, the lower-case of this letter is U+0069 or U+0131. -->
    <string name="spoken_letter_0130">ବଡ I,ଉପର ଡଟ୍</string>
    <!-- Spoken description for unknown symbol code point. -->
    <string name="spoken_symbol_unknown">ଅଜ୍ଞାତ ସଙ୍କେତ</string>
    <!-- Spoken description for unknown emoji code point. -->
    <string name="spoken_emoji_unknown">ଅଜ୍ଞାତ ଏମୋଜି</string>

    <!-- Spoken descriptions when opening a more keys keyboard that has alternative characters. -->
    <string name="spoken_open_more_keys_keyboard">ବୈକଳ୍ପିକ ବର୍ଣ୍ଣଗୁଡିକ ଉପଲବ୍ଧ ହୋଇଛି</string>
    <!-- Spoken descriptions when closing a more keys keyboard that has alternative characters. -->
    <string name="spoken_close_more_keys_keyboard">ବୈକଳ୍ପିକ ବର୍ଣ୍ଣଗୁଡିକ ଅଗ୍ରାହ୍ୟ ହୋଇଛି</string>

    <!-- Spoken descriptions when opening a more suggestions panel that has alternative suggested words. -->
    <string name="spoken_open_more_suggestions">ବୈକଳ୍ପିକ ପରାମର୍ଶଗୁଡିକ ଉପଲବ୍ଧ ହୋଇଛି</string>
    <!-- Spoken descriptions when closing a more suggestions panel that has alternative suggested words. -->
    <string name="spoken_close_more_suggestions">ବୈକଳ୍ପିକ ପରାମର୍ଶଗୁଡିକ ଅଗ୍ରାହ୍ୟ ହୋଇଛି</string>
</resources>
