    <string name="announce_keyboard_mode">"<xliff:g id="KEYBOARD_MODE">%s</xliff:g> کی بورڈ دکھائی دے رہا ہے"</string>
    <!-- Description of the keyboard mode for entering dates. -->
    <string name="keyboard_mode_date">"تاریخ"</string>
