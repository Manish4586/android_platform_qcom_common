    <string name="spoken_description_to_alpha">"حروف"</string>
    <!-- Spoken description for the "To Numbers" keyboard key. -->
    <string name="spoken_description_to_numeric">"نمبرز"</string>
    <!-- Spoken description for the "Settings" keyboard key. -->
    <string name="spoken_description_settings">"ترتیبات"</string>
