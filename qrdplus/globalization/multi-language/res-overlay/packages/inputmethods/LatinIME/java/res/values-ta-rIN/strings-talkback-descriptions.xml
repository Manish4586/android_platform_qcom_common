<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Spoken description to let the user know that when typing in a password, they can plug in a headset in to hear spoken descriptions of the keys they type. [CHAR LIMIT=NONE] -->
    <string name="spoken_use_headphones">"சத்தமாகக் கூறப்படும் கடவுச்சொல் விசைகளைக் கேட்பதற்கு ஹெட்செட்டைச் செருகவும்."</string>

    <!-- Spoken description for the currently entered text -->
    <string name="spoken_current_text_is">"நடப்பு உரை %s"</string>
    <!-- Spoken description when there is no text entered -->
    <string name="spoken_no_text_entered">"உரை எதுவும் உள்ளிடப்படவில்லை"</string>

    <!-- Spoken description to let the user know what auto-correction will be performed when a key is pressed. An auto-correction replaces a single word with one or more words. -->
    <string name="spoken_auto_correct">"<xliff:g id="ORIGINAL_WORD">%2$s</xliff:g> இலிருந்து <xliff:g id="CORRECTED_WORD">%3$s</xliff:g> ஆக <xliff:g id="KEY_NAME">%1$s</xliff:g> திருத்துகிறது"</string>
    <!-- Spoken description used during obscured (e.g. password) entry to let the user know that auto-correction will be performed when a key is pressed. -->
    <string name="spoken_auto_correct_obscured">"<xliff:g id="KEY_NAME">%1$s</xliff:g> தன்னியக்க திருத்தத்தைச் செயல்படுத்துகிறது"</string>

    <!-- Spoken description for unknown keyboard keys. -->
    <string name="spoken_description_unknown">"அறியப்படாத எழுத்துக்குறி"</string>
    <!-- Spoken description for the "Shift" keyboard key when "Shift" is off. -->
    <string name="spoken_description_shift">"ஷிஃப்டு"</string>
    <!-- Spoken description for the "Shift" keyboard key in symbols mode. -->
    <string name="spoken_description_symbols_shift">"கூடுதல் குறியீடுகள்"</string>
    <!-- Spoken description for the "Shift" keyboard key when "Shift" is on. -->
    <string name="spoken_description_shift_shifted">"ஷிஃப்டு"</string>
    <!-- Spoken description for the "Shift" keyboard key in 2nd symbols (a.k.a. symbols shift) mode. -->
    <string name="spoken_description_symbols_shift_shifted">"குறியீடுகள்"</string>
    <!-- Spoken description for the "Shift" keyboard key when "Caps lock" is on. -->
    <string name="spoken_description_caps_lock">"ஷிஃப்டு"</string>
    <!-- Spoken description for the "Delete" keyboard key. -->
    <string name="spoken_description_delete">"நீக்கு"</string>
    <!-- Spoken description for the "To Symbol" keyboard key. -->
    <string name="spoken_description_to_symbol">"சின்னங்கள்"</string>
    <!-- Spoken description for the "To Alpha" keyboard key. -->
    <string name="spoken_description_to_alpha">"எழுத்துகள்"</string>
    <!-- Spoken description for the "To Numbers" keyboard key. -->
    <string name="spoken_description_to_numeric">"எண்கள்"</string>
    <!-- Spoken description for the "Settings" keyboard key. -->
    <string name="spoken_description_settings">"அமைப்பு"</string>
    <!-- Spoken description for the "Tab" keyboard key. -->
    <string name="spoken_description_tab">"டேப்"</string>
    <!-- Spoken description for the "Space" keyboard key. -->
    <string name="spoken_description_space">"ஸ்பேஸ்"</string>
    <!-- Spoken description for the "Mic" keyboard key. -->
    <string name="spoken_description_mic">"குரல் உள்ளீடு"</string>
    <!-- Spoken description for the "Emoji" keyboard key. -->
    <string name="spoken_description_emoji">"ஈமோஜி"</string>
    <!-- Spoken description for the "Return" keyboard key. -->
    <string name="spoken_description_return">"திரும்பு"</string>
    <!-- Spoken description for the "Search" keyboard key. -->
    <string name="spoken_description_search">"தேடல்"</string>
    <!-- Spoken description for the "U+2022" (BULLET) keyboard key. -->
    <string name="spoken_description_dot">"புள்ளி"</string>
    <!-- Spoken description for the "Switch language" keyboard key. -->
    <string name="spoken_description_language_switch">"மொழியை மாற்று"</string>
    <!-- Spoken description for the "Next" action keyboard key. -->
    <string name="spoken_description_action_next">"அடுத்து"</string>
    <!-- Spoken description for the "Previous" action keyboard key. -->
    <string name="spoken_description_action_previous">"முந்தையது"</string>

    <!-- Spoken feedback after turning "Shift" mode on. -->
    <string name="spoken_description_shiftmode_on">"ஷிப்டு இயக்கப்பட்டது"</string>
    <!-- Spoken feedback after turning "Caps lock" mode on. -->
    <string name="spoken_description_shiftmode_locked">"பேரெழுத்தாக்கம் இயக்கப்பட்டது"</string>
    <!-- Spoken feedback after changing to the symbols keyboard. -->
    <string name="spoken_description_mode_symbol">"குறியீடுகள் பயன்முறை"</string>
    <!-- Spoken feedback after changing to the 2nd symbols (a.k.a. symbols shift) keyboard. -->
    <string name="spoken_description_mode_symbol_shift">"கூடுதல் குறியீடுகள் முறை"</string>
    <!-- Spoken feedback after changing to the alphanumeric keyboard. -->
    <string name="spoken_description_mode_alpha">"எழுத்துகள் பயன்முறை"</string>
    <!-- Spoken feedback after changing to the phone dialer keyboard. -->
    <string name="spoken_description_mode_phone">"ஃபோன் பயன்முறை"</string>
    <!-- Spoken feedback after changing to the shifted phone dialer (symbols) keyboard. -->
    <string name="spoken_description_mode_phone_shift">"ஃபோன் குறியீடுகள் பயன்முறை"</string>

    <!-- Spoken feedback when the keyboard is hidden. -->
    <string name="announce_keyboard_hidden">"விசைப்பலகை மறைக்கப்பட்டது"</string>
    <!-- Spoken feedback when the keyboard mode changes. -->
    <string name="announce_keyboard_mode">"<xliff:g id="KEYBOARD_MODE">%s</xliff:g> விசைப்பலகையைக் காட்டுகிறது"</string>
    <!-- Description of the keyboard mode for entering dates. -->
    <string name="keyboard_mode_date">"தேதி"</string>
    <!-- Description of the keyboard mode for entering dates and times. -->
    <string name="keyboard_mode_date_time">"தேதி மற்றும் நேரம்"</string>
    <!-- Description of the keyboard mode for entering email addresses. -->
    <string name="keyboard_mode_email">"மின்னஞ்சல்"</string>
    <!-- Description of the keyboard mode for entering text messages. -->
    <string name="keyboard_mode_im">"மெசேஜ்"</string>
    <!-- Description of the keyboard mode for entering numbers. -->
    <string name="keyboard_mode_number">"எண்"</string>
    <!-- Description of the keyboard mode for entering phone numbers. -->
    <string name="keyboard_mode_phone">"ஃபோன்"</string>
    <!-- Description of the keyboard mode for entering generic text. -->
    <string name="keyboard_mode_text">"உரை"</string>
    <!-- Description of the keyboard mode for entering times. -->
    <string name="keyboard_mode_time">"நேரம்"</string>
    <!-- Description of the keyboard mode for entering URLs. -->
    <string name="keyboard_mode_url">"URL"</string>

    <!-- Description of the emoji category icon of Recents. -->
    <string name="spoken_descrption_emoji_category_recents">"சமீபத்தியவை"</string>
    <!-- Description of the emoji category icon of People. -->
    <string name="spoken_descrption_emoji_category_people">"நபர்கள்"</string>
    <!-- Description of the emoji category icon of Objects. -->
    <string name="spoken_descrption_emoji_category_objects">"பொருட்கள்"</string>
    <!-- Description of the emoji category icon of Nature. -->
    <string name="spoken_descrption_emoji_category_nature">"இயற்கை"</string>
    <!-- Description of the emoji category icon of Places. -->
    <string name="spoken_descrption_emoji_category_places">"இடங்கள்"</string>
    <!-- Description of the emoji category icon of Symbols. -->
    <string name="spoken_descrption_emoji_category_symbols">"சின்னங்கள்"</string>
    <!-- Description of the emoji category icon of Emoticons. -->
    <string name="spoken_descrption_emoji_category_emoticons">"எமோடிகான்ஸ்"</string>

    <!-- Description of an upper case letter of LOWER_LETTER. -->
    <string name="spoken_description_upper_case">"பேரெழுத்து <xliff:g id="LOWER_LETTER">%s</xliff:g>"</string>
    <!-- Spoken description for Unicode code point U+0049: "I" LATIN CAPITAL LETTER I
         Note that depending on locale, the lower-case of this letter is U+0069 or U+0131. -->
    <string name="spoken_letter_0049">"பேரெழுத்து I"</string>
    <!-- Spoken description for Unicode code point U+0130: "İ" LATIN CAPITAL LETTER I WITH DOT ABOVE
         Note that depending on locale, the lower-case of this letter is U+0069 or U+0131. -->
    <string name="spoken_letter_0130">"பேரெழுத்து I, மேல் புள்ளி"</string>
    <!-- Spoken description for unknown symbol code point. -->
    <string name="spoken_symbol_unknown">"அறியப்படாத குறியீடு"</string>
    <!-- Spoken description for unknown emoji code point. -->
    <string name="spoken_emoji_unknown">"அறியப்படாத ஈமோஜி"</string>

    <!-- Spoken descriptions when opening a more keys keyboard that has alternative characters. -->
    <string name="spoken_open_more_keys_keyboard">"மாற்று எழுத்துக்குறிகள் உள்ளன"</string>
    <!-- Spoken descriptions when closing a more keys keyboard that has alternative characters. -->
    <string name="spoken_close_more_keys_keyboard">"மாற்று எழுத்துக்குறிகள் நிராகரிக்கப்பட்டன"</string>

    <!-- Spoken descriptions when opening a more suggestions panel that has alternative suggested words. -->
    <string name="spoken_open_more_suggestions">"மாற்று பரிந்துரைகள் உள்ளன"</string>
    <!-- Spoken descriptions when closing a more suggestions panel that has alternative suggested words. -->
    <string name="spoken_close_more_suggestions">"மாற்று பரிந்துரைகள் நிராகரிக்கப்பட்டன"</string>
</resources>
