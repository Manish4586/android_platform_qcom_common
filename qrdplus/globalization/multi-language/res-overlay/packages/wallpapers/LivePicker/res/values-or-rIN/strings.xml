<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- General -->
    <skip/>
    <!-- Application name -->
    <string name="application_name">ଲାଇଭ୍ ୱାଲପେପର୍ ପିକର୍</string>
    <!-- Title for the screen that lets the user choose a live wallpaper to use
         for the system. -->
    <string name="live_wallpaper_picker_title">ଲାଇଭ୍ ୱାଲପେପର୍ ଚୟନ କରନ୍ତୁ</string>
    <!-- Label for the live wallpaper picker [CHAR_LIMIT=40]-->
    <string name="live_wallpaper_picker_label">ଲାଇଭ୍ ୱାଲପେପରଗୁଡିକ</string>
    <!-- Title for the screen that lets the user preview a live wallpaper. -->
    <string name="live_wallpaper_preview_title">ଲାଇଭ୍ ୱାଲପେପର୍ ପୂର୍ବାବଲୋକନ</string>
    <!-- List item for configuring the current wallpaper -->
    <string name="configure_wallpaper">ସେଟିଂସ୍…</string>
    <!-- Button label on Wallpaper Gallery screen; user selects this button to set a specific wallpaper -->
    <string name="wallpaper_instructions">ୱାଲପେପର୍ ସେଟ୍ କରନ୍ତୁ</string>
    <!-- Warning message, no live wallpapers available. -->
    <string name="live_wallpaper_empty">କୌଣସି ଲାଇଭ୍‌ ୱାଲପେପର୍‌ ନାହିଁ।</string>
    <!-- Button label, action, sets the currently selected wallpaper. -->
    <string name="set_live_wallpaper">ୱାଲପେପର୍ ସେଟ୍ କରନ୍ତୁ</string>
    <!-- Label, title and author of the live wallpaper -->
    <string name="wallpaper_title_and_author"><xliff:g id="author" example="Google">%2$s</xliff:g> ଦ୍ଵାରା <xliff:g id="title" example="Galaxy">%1$s</xliff:g></string>
    <!-- Message, tells the user the selected live wallpaper is loading. -->
    <string name="live_wallpaper_loading">ଲାଇଭ୍‌ ୱାଲପେପର୍‌ ଲୋଡ୍‍ କରୁଛି...</string>
    <!-- Button label, action, cancels the wallpaper picker dialog [CHAR_LIMIT=40]-->
    <string name="wallpaper_cancel">ବାତିଲ୍</string>

</resources>
