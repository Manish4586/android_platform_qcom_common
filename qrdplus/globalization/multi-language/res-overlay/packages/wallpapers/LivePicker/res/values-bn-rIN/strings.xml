<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- General -->
    <skip/>
    <!-- Application name -->
    <string name="application_name">জীবন্ত ওয়ালপেপার বেছে নেওয়ার</string>
    <!-- Title for the screen that lets the user choose a live wallpaper to use
         for the system. -->
    <string name="live_wallpaper_picker_title">জীবন্ত ওয়ালপেপার নির্বাচন করুন</string>
    <!-- Label for the live wallpaper picker [CHAR_LIMIT=40]-->
    <string name="live_wallpaper_picker_label">জীবন্ত ওয়ালপেপারগুলি</string>
    <!-- Title for the screen that lets the user preview a live wallpaper. -->
    <string name="live_wallpaper_preview_title">জীবন্ত ওয়ালপেপার প্রাকদর্শন</string>
    <!-- List item for configuring the current wallpaper -->
    <string name="configure_wallpaper">সেটিংগুলি…</string>
    <!-- Button label on Wallpaper Gallery screen; user selects this button to set a specific wallpaper -->
    <string name="wallpaper_instructions">ওয়ালপেপার সেট করা</string>
    <!-- Warning message, no live wallpapers available. -->
    <string name="live_wallpaper_empty">কোন জীবন্ত ওয়ালপেপার নেই।</string>
    <!-- Button label, action, sets the currently selected wallpaper. -->
    <string name="set_live_wallpaper">ওয়ালপেপার সেট করা</string>
    <!-- Label, title and author of the live wallpaper -->
    <string name="wallpaper_title_and_author"><xliff:g id="title" example="Galaxy">%1$s</xliff:g> দ্বারা <xliff:g id="author" example="Google">%2$s</xliff:g></string>
    <!-- Message, tells the user the selected live wallpaper is loading. -->
    <string name="live_wallpaper_loading">জীবন্ত ওয়ালপেপার লোড করছি…</string>
    <!-- Button label, action, cancels the wallpaper picker dialog [CHAR_LIMIT=40]-->
    <string name="wallpaper_cancel">বাতিল</string>

</resources>
