<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- General -->
    <skip/>
    <!-- Application name -->
    <string name="application_name">"Live Wallpaper Picker"</string>
    <!-- Title for the screen that lets the user choose a live wallpaper to use
         for the system. -->
    <string name="live_wallpaper_picker_title">"एनीमेशन वाला वॉलपेपर चुनें"</string>
    <!-- Label for the live wallpaper picker [CHAR_LIMIT=40]-->
    <string name="live_wallpaper_picker_label">"लाइव वॉलपेपर"</string>
    <!-- Title for the screen that lets the user preview a live wallpaper. -->
    <string name="live_wallpaper_preview_title">"एनीमेशन वाला वॉलपेपर पूर्वावलोकन"</string>
    <!-- List item for configuring the current wallpaper -->
    <string name="configure_wallpaper">"सेटिंग…"</string>
    <!-- Button label on Wallpaper Gallery screen; user selects this button to set a specific wallpaper -->
    <string name="wallpaper_instructions">"वॉलपेपर सेट करें"</string>
    <!-- Warning message, no live wallpapers available. -->
    <string name="live_wallpaper_empty">"कोई एनीमेशन वाला वॉलपेपर नहीं."</string>
    <!-- Button label, action, sets the currently selected wallpaper. -->
    <string name="set_live_wallpaper">"वॉलपेपर सेट करें"</string>
    <!-- Label, title and author of the live wallpaper -->
    <string name="wallpaper_title_and_author">"<xliff:g id="TITLE">%1$s</xliff:g> द्वारा <xliff:g id="AUTHOR">%2$s</xliff:g>"</string>
    <!-- Message, tells the user the selected live wallpaper is loading. -->
    <string name="live_wallpaper_loading">"एनीमेशन वाला वॉलपेपर लोड हो रहा है…"</string>
    <!-- Button label, action, cancels the wallpaper picker dialog [CHAR_LIMIT=40]-->
    <string name="wallpaper_cancel">"रहने दें"</string>

</resources>
