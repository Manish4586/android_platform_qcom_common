    <!-- Play Store title text [CHAR-LIMIT=30] -->
    <string name="webviewgoogle_play_store_title">"WebView za sistem Android"</string>

    <!-- Play Store tagline text. [CHAR-LIMIT=80] -->
    <string name="webviewgoogle_play_store_tagline">"Spletna vsebina za aplikacije, ki uporabljajo Chrome"</string>

    <!-- Play Store description text. [CHAR-LIMIT=4000] -->
    <string name="webviewgoogle_play_store_description">"Android Webview je sistemska komponenta, ki uporablja Chrome in aplikacijam za sistem Android omogoča prikaz spletne vsebine. Ta komponenta je v vaši napravi vnaprej nameščena in jo morate sproti posodabljati, če si želite zagotoviti najnovejše varnostne posodobitve in druge popravke napak."</string>

