<resources>
    <!-- Name for the default system route prior to Jellybean. [CHAR LIMIT=30] -->
    <string name="mr_system_route_name">"సిస్టమ్"</string>

    <!-- Name for the user route category created when publishing routes to the system in Jellybean and above. [CHAR LIMIT=30] -->
    <string name="mr_user_route_category_name">"పరికరాలు"</string>

    <!-- Content description of a MediaRouteButton for accessibility support.
        Cast is the standard android verb for sending content to a remote device. [CHAR LIMIT=50] -->
    <string name="mr_media_route_button_content_description">"ప్రసారం చేయండి"</string>

    <!-- Title of the media route chooser dialog. [CHAR LIMIT=30] -->
    <string name="mr_media_route_chooser_title">"పరికరానికి కనెక్ట్ చేయండి"</string>

    <!-- Placeholder text to show when no devices have been found. [CHAR LIMIT=50] -->
    <string name="mr_media_route_chooser_searching">"పరికరాల కోసం శోధిస్తోంది…"</string>

    <!-- Button to disconnect from a media route.  [CHAR LIMIT=30] -->
    <string name="mr_media_route_controller_disconnect">"డిస్‌కనెక్ట్ చేయి"</string>

    <!-- Button to stop playback and disconnect from a media route.  [CHAR LIMIT=30] -->
    <string name="mr_media_route_controller_stop">ప్రసారాన్ని ఆపివేయి</string>

    <!-- Description for a button that takes you to settings for the active route -->
    <string name="mr_media_route_controller_settings_description">రూట్ సెట్టింగ్‌లు</string>

    <!-- Accessibility description for the play button -->
    <string name="mr_media_route_controller_play">"ప్లే చేయి"</string>

    <!-- Accessibility description for the pause button -->
    <string name="mr_media_route_controller_pause">"పాజ్ చేయి"</string>
</resources>
