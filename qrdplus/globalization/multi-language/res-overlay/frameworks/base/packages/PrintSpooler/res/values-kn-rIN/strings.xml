    <string name="reason_unknown">"ಅಜ್ಞಾತ"</string>

    <!-- Label for a printer that is not available. [CHAR LIMIT=25] -->
    <string name="printer_unavailable">"<xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> – ಲಭ್ಯವಿಲ್ಲ"</string>

    <!-- Arrays -->

    <!-- Color mode labels. -->
    <string-array name="color_mode_labels">
