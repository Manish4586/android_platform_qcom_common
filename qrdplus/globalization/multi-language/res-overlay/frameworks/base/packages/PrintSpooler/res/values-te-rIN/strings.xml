    <string name="label_pages">"పేజీలు"</string>

    <!-- Template for the all pages option in the page selection widget. [CHAR LIMIT=20] -->
    <string name="template_all_pages">"మొత్తం <xliff:g id="PAGE_COUNT">%1$s</xliff:g>"</string>

    <!-- Template for the page range option in the page selection widget. [CHAR LIMIT=20] -->
    <string name="template_page_range">"<xliff:g id="PAGE_COUNT">%1$s</xliff:g> పరిధి"</string>

    <!-- Page range exmple used as a hint of how to specify such. [CHAR LIMIT=20] -->
    <string name="pages_range_example">"ఉదా. 1—5,8,11—13"</string>

    <!-- Title for the pring preview button .[CHAR LIMIT=30] -->
    <string name="print_preview">"ముద్రణ పరిదృశ్యం"</string>

    <!-- Title for the pring preview button if there is no PDF viewer isntalled. [CHAR LIMIT=50] -->
    <string name="install_for_print_preview">"పరిదృశ్యం చేయడానికి PDF వ్యూయర్‌ను ఇన్‌స్టాల్ చేయండి"</string>

    <!-- Title of the message that the printing application crashed. [CHAR LIMIT=50] -->
    <string name="printing_app_crashed">"ముద్రణ అనువర్తనం క్రాష్ అయ్యింది"</string>

    <!-- Title for the temporary dialog show while an app is generating a print job. [CHAR LIMIT=30] -->
    <string name="generating_print_job">"ముద్రణ జాబ్‌ను ఉత్పన్నం చేస్తోంది"</string>

