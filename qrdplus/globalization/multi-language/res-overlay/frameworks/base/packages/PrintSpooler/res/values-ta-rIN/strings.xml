    <!-- Template for the notification label for a cancelling print job. [CHAR LIMIT=25] -->
    <string name="cancelling_notification_title_template">"<xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> ஐ ரத்துசெய்கிறது"</string>

