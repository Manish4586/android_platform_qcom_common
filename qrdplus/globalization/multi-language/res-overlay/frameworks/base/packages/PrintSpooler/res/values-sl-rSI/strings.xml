<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Title of the PrintSpooler application. [CHAR LIMIT=50] -->
    <string name="app_label">"Tisk. v ozadju"</string>

    <!-- Label of the print dialog\'s button for more print options. [CHAR LIMIT=25] -->
    <string name="more_options_button">"Več možnosti"</string>

    <!-- Label of the destination widget. [CHAR LIMIT=20] -->
    <string name="label_destination">"Cilj"</string>

    <!-- Label of the copies count widget. [CHAR LIMIT=20] -->
    <string name="label_copies">"Št. kopij"</string>

    <!-- Label of the copies count for the print options summary. [CHAR LIMIT=20] -->
    <string name="label_copies_summary">"Št. kopij:"</string>

    <!-- Label of the paper size widget. [CHAR LIMIT=20] -->
    <string name="label_paper_size">"Velikost papirja"</string>

    <!-- Label of the paper size for the print options summary. [CHAR LIMIT=20] -->
    <string name="label_paper_size_summary">"Velikost papirja:"</string>

    <!-- Label of the color mode widget. [CHAR LIMIT=20] -->
    <string name="label_color">"Barvno"</string>

    <!-- Label of the orientation widget. [CHAR LIMIT=20] -->
    <string name="label_orientation">"Postavitev"</string>

    <!-- Label of the page selection widget. [CHAR LIMIT=20] -->
    <string name="label_pages">"Strani"</string>

    <!-- Template for the all pages option in the page selection widget. [CHAR LIMIT=20] -->
    <string name="template_all_pages">"Vse (<xliff:g id="PAGE_COUNT">%1$s</xliff:g>)"</string>

    <!-- Template for the page range option in the page selection widget. [CHAR LIMIT=20] -->
    <string name="template_page_range">"Obseg strani: <xliff:g id="PAGE_COUNT">%1$s</xliff:g>"</string>

    <!-- Page range exmple used as a hint of how to specify such. [CHAR LIMIT=20] -->
    <string name="pages_range_example">"npr. 1–5, 8, 11–13"</string>

    <!-- Title for the pring preview button .[CHAR LIMIT=30] -->
    <string name="print_preview">"Predogled tiskanja"</string>

    <!-- Title for the pring preview button if there is no PDF viewer isntalled. [CHAR LIMIT=50] -->
    <string name="install_for_print_preview">"Za predogled namestite pregledovalnik za PDF-je"</string>

    <!-- Title of the message that the printing application crashed. [CHAR LIMIT=50] -->
    <string name="printing_app_crashed">"Aplikacija za tiskanje se je zrušila"</string>

    <!-- Title for the temporary dialog show while an app is generating a print job. [CHAR LIMIT=30] -->
    <string name="generating_print_job">"Ustvarjanje zahteve za tisk"</string>

    <!-- Title for the save as PDF option in the printer list. [CHAR LIMIT=30] -->
    <string name="save_as_pdf">"Shrani kot PDF"</string>

    <!-- Title for the open all printers UI option in the printer list. [CHAR LIMIT=30] -->
    <string name="all_printers">"Vsi tiskalniki …"</string>

    <!-- Title for the print dialog announced to the user for accessibility. Not shown in the UI. [CHAR LIMIT=none] -->
    <string name="print_dialog">"Pogovorno okno za tiskanje"</string>

    <!-- Template for the message that shows the current page out of the total number of pages [CHAR LIMIT=none] -->
    <string name="current_page_template">"<xliff:g id="CURRENT_PAGE">%1$d</xliff:g>/<xliff:g id="PAGE_COUNT">%2$d</xliff:g>"</string>

    <!-- Description of the current page - spoken to the user [CHAR LIMIT=none] -->
    <string name="page_description_template">"Stran <xliff:g id="CURRENT_PAGE">%1$d</xliff:g> od <xliff:g id="PAGE_COUNT">%2$d</xliff:g>"</string>

    <!-- Template for the message to announce the print options summary - spoken to the user. [CHAR LIMIT=none] -->
    <string name="summary_template">"Povzetek, št. kopij <xliff:g id="COPIES">%1$s</xliff:g>, velikost papirja <xliff:g id="PAPER_SIZE">%2$s</xliff:g>"</string>

    <!-- Description for the handle to expand all print options - spoken to the user. [CHAR LIMIT=none] -->
    <string name="expand_handle">"Ročica za razširitev"</string>

    <!-- Description for the handle to collapse all print options - spoken to the user. [CHAR LIMIT=none] -->
    <string name="collapse_handle">"Ročica za strnitev"</string>

    <!-- Description for the print button - spoken to the user. [CHAR LIMIT=none] -->
    <string name="print_button">"Natisni"</string>

    <!-- Description for the save to PDF button - spoken to the user. [CHAR LIMIT=none] -->
    <string name="savetopdf_button">"Shrani v PDF"</string>

    <!-- Message to announce print options are expanded - spoken to the user. [CHAR LIMIT=none] -->
    <string name="print_options_expanded">"Možnosti tiskanja razširjene"</string>

    <!-- Message to announce print options are collapsed - spoken to the user. [CHAR LIMIT=none] -->
    <string name="print_options_collapsed">"Možnosti tiskanja strnjene"</string>

    <!-- Select printer activity -->

    <!-- Title for the share action bar menu item. [CHAR LIMIT=20] -->
    <string name="search">"Iskanje"</string>

    <!-- Title for the select printer activity. [CHAR LIMIT=30] -->
    <string name="all_printers_label">"Vsi tiskalniki"</string>

    <!-- Title of the button to install a print service. [CHAR LIMIT=25] -->
    <string name="add_print_service_label">"Dodaj storitev"</string>

    <!-- Utterance to announce that the search box is shown. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <string name="print_search_box_shown_utterance">"Iskalno polje je prikazano"</string>

    <!-- Utterance to announce that the search box is hidden. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <string name="print_search_box_hidden_utterance">"Iskalno polje je skrito"</string>

    <!-- Title of the action bar button to got to add a printer. [CHAR LIMIT=25] -->
    <string name="print_add_printer">"Dodajanje tiskalnika"</string>

    <!-- Title of the menu item to select a printer. [CHAR LIMIT=25] -->
    <string name="print_select_printer">"Izbira tiskalnika"</string>

    <!-- Title of the menu item to forget a printer. [CHAR LIMIT=25] -->
    <string name="print_forget_printer">"Odstranitev tiskalnika"</string>

    <!-- Utterance to announce a change in the number of matches during a search. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <plurals name="print_search_result_count_utterance">
        <item quantity="one">"Najden <xliff:g id="COUNT">%1$s</xliff:g> tiskalnik"</item>
        <item quantity="other">"Število najdenih tiskalnikov: <xliff:g id="COUNT">%1$s</xliff:g>"</item>
    </plurals>

    <!-- Add printer dialog  -->

    <!-- Title for the alert dialog for selecting a print service. [CHAR LIMIT=50] -->
    <string name="choose_print_service">"Izberite tiskalno storitev"</string>

    <!-- Title for the prompt shown as a placeholder if no printers are found while not searching. [CHAR LIMIT=50] -->
    <string name="print_searching_for_printers">"Iskanje tiskalnikov"</string>

    <!-- Title for the prompt shown as a placeholder if there are no printers while searching. [CHAR LIMIT=50] -->
    <string name="print_no_printers">"Tiskalnikov ni mogoče najti"</string>

    <!-- Notifications -->

    <!-- Template for the notification label for a printing print job. [CHAR LIMIT=25] -->
    <string name="printing_notification_title_template">"Tiskanje: <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Template for the notification label for a cancelling print job. [CHAR LIMIT=25] -->
    <string name="cancelling_notification_title_template">"Preklic: <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Template for the notification label for a failed print job. [CHAR LIMIT=25] -->
    <string name="failed_notification_title_template">"Napaka tiskalnika: <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Template for the notification label for a blocked print job. [CHAR LIMIT=25] -->
    <string name="blocked_notification_title_template">"Tiskalnik je blokiral <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Template for the notification label for a composite (multiple items) print jobs notification. [CHAR LIMIT=25] -->
    <plurals name="composite_notification_title_template">
        <item quantity="one">"Tiskalno opravilo: <xliff:g id="PRINT_JOB_NAME">%1$d</xliff:g>"</item>
        <item quantity="other">"Tiskalna opravila: <xliff:g id="PRINT_JOB_NAME">%1$d</xliff:g>"</item>
    </plurals>

    <!-- Label for the notification button for cancelling a print job. [CHAR LIMIT=25] -->
    <string name="cancel">"Prekliči"</string>

    <!-- Label for the notification button for restarting a filed print job. [CHAR LIMIT=25] -->
    <string name="restart">"Začni znova"</string>

    <!-- Message that there is no connection to a printer. [CHAR LIMIT=40] -->
    <string name="no_connection_to_printer">"Ni povezave s tiskalnikom"</string>

    <!-- Label for an unknown reason for failed or blocked print job. [CHAR LIMIT=25] -->
    <string name="reason_unknown">"neznano"</string>

    <!-- Label for a printer that is not available. [CHAR LIMIT=25] -->
    <string name="printer_unavailable">"<xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> – ni na voljo"</string>

    <!-- Arrays -->

    <!-- Color mode labels. -->
    <string-array name="color_mode_labels">
        <!-- Color modelabel: Monochrome color scheme, e.g. one color is used. [CHAR LIMIT=20] -->
        <item>"Črno-belo"</item>
        <!-- Color mode label: Color color scheme, e.g. many colors are used. [CHAR LIMIT=20] -->
        <item>"Barvno"</item>
    </string-array>

    <!-- Orientation labels. -->
    <string-array name="orientation_labels">
        <!-- Orientation label: Portrait page orientation. [CHAR LIMIT=30] -->
        <item>"Pokončno"</item>
        <!-- Orientation label: Landscape page orientation [CHAR LIMIT=30] -->
        <item>"Ležeče"</item>
    </string-array>

    <!-- Permissions -->

    <!-- Title of an application permission, listed so the user can choose whether they want
         to allow the application to do this. -->
    <string name="permlab_accessAllPrintJobs" translatable="false">access all print jobs</string>
    <!-- Description of an application permission, listed so the user can choose whether
         they want to allow the application to do this. -->
    <string name="permdesc_accessAllPrintJobs" translatable="false">Allows the holder to access
        print jobs created by another app. Should never be needed for normal apps.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want
         to allow the application to do this. -->
    <string name="permlab_startPrintServiceConfigActivity" translatable="false">start print
        service configuration activities</string>
    <!-- Description of an application permission, listed so the user can choose whether they
         want to allow the application to do this. -->
    <string name="permdesc_startPrintServiceConfigActivity" translatable="false">Allows the
        holder to start the configuration activities of a print service. Should never be needed
        for normal apps.</string>

    <!-- Error messages -->

    <!-- Message for an error when trying to print to a PDF file. [CHAR LIMIT=50] -->
    <string name="print_write_error_message">"V datoteko ni bilo mogoče zapisovati"</string>

    <!-- Default message for an error while generating a print job. [CHAR LIMIT=50] -->
    <string name="print_error_default_message">"To žal ni delovalo. Poskusite znova."</string>

    <!-- Label for the retry button in the error message. [CHAR LIMIT=50] -->
    <string name="print_error_retry">"Poskusi znova"</string>

    <!-- Message for the currently selected printer being unavailable. [CHAR LIMIT=100] -->
    <string name="print_error_printer_unavailable">"Ta tiskalnik trenutno ni na voljo."</string>

    <!-- Long running operations -->

    <!-- Message long running operation when preparing print preview. [CHAR LIMIT=50] -->
    <string name="print_preparing_preview">"Priprava predogleda …"</string>

</resources>
