    <!-- Message to announce print options are collapsed - spoken to the user. [CHAR LIMIT=none] -->
    <string name="print_options_collapsed">"Yazdırma seçenekleri daraltıldı"</string>

    <!-- Select printer activity -->

    <!-- Title for the share action bar menu item. [CHAR LIMIT=20] -->
    <string name="search">"Ara"</string>

    <!-- Title for the select printer activity. [CHAR LIMIT=30] -->
    <string name="all_printers_label">"Tüm yazıcılar"</string>

    <!-- Title of the button to install a print service. [CHAR LIMIT=25] -->
    <string name="add_print_service_label">"Hizmet ekle"</string>

    <!-- Utterance to announce that the search box is shown. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <string name="print_search_box_shown_utterance">"Arama kutusu gösteriliyor"</string>

    <!-- Utterance to announce that the search box is hidden. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <string name="print_search_box_hidden_utterance">"Arama kutusu gizli"</string>

    <!-- Title of the action bar button to got to add a printer. [CHAR LIMIT=25] -->
    <string name="print_add_printer">"Yazıcı ekle"</string>

    <!-- Title of the menu item to select a printer. [CHAR LIMIT=25] -->
    <string name="print_select_printer">"Yazıcı seç"</string>

    <!-- Title of the menu item to forget a printer. [CHAR LIMIT=25] -->
    <string name="print_forget_printer">"Yazıcıyı unut"</string>

    <!-- Utterance to announce a change in the number of matches during a search. This is spoken to a blind user. [CHAR LIMIT=none] -->
    <plurals name="print_search_result_count_utterance">
        <item quantity="one">"<xliff:g id="COUNT">%1$s</xliff:g> yazıcı bulundu"</item>
        <item quantity="other">"<xliff:g id="COUNT">%1$s</xliff:g> yazıcı bulundu"</item>
    </plurals>

    <!-- Add printer dialog  -->

    <!-- Title for the alert dialog for selecting a print service. [CHAR LIMIT=50] -->
    <string name="choose_print_service">"Yazdırma hizmetini seçin"</string>

    <!-- Title for the prompt shown as a placeholder if no printers are found while not searching. [CHAR LIMIT=50] -->
    <string name="print_searching_for_printers">"Yazıcılar aranıyor"</string>

    <!-- Title for the prompt shown as a placeholder if there are no printers while searching. [CHAR LIMIT=50] -->
    <string name="print_no_printers">"Yazıcı bulunamadı"</string>

    <!-- Notifications -->

    <!-- Template for the notification label for a printing print job. [CHAR LIMIT=25] -->
    <string name="printing_notification_title_template">"<xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> yazdırılıyor"</string>

    <!-- Template for the notification label for a cancelling print job. [CHAR LIMIT=25] -->
    <string name="cancelling_notification_title_template">"<xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> iptal ediliyor"</string>

    <!-- Template for the notification label for a failed print job. [CHAR LIMIT=25] -->
    <string name="failed_notification_title_template">"Yazıcı hatası: <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

    <!-- Template for the notification label for a blocked print job. [CHAR LIMIT=25] -->
    <string name="blocked_notification_title_template">"Yazıcı <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> işini engelledi"</string>

    <!-- Template for the notification label for a composite (multiple items) print jobs notification. [CHAR LIMIT=25] -->
    <plurals name="composite_notification_title_template">
        <item quantity="one">"<xliff:g id="PRINT_JOB_NAME">%1$d</xliff:g> yazdırma işi"</item>
        <item quantity="other">"<xliff:g id="PRINT_JOB_NAME">%1$d</xliff:g> yazdırma işleri"</item>
    </plurals>

    <!-- Label for the notification button for cancelling a print job. [CHAR LIMIT=25] -->
    <string name="cancel">"İptal"</string>

    <!-- Label for the notification button for restarting a filed print job. [CHAR LIMIT=25] -->
    <string name="restart">"Yeniden başlat"</string>

    <!-- Message that there is no connection to a printer. [CHAR LIMIT=40] -->
    <string name="no_connection_to_printer">"Yazıcı bağlantısı yok"</string>

    <!-- Label for an unknown reason for failed or blocked print job. [CHAR LIMIT=25] -->
    <string name="reason_unknown">"bilinmiyor"</string>

    <!-- Label for a printer that is not available. [CHAR LIMIT=25] -->
    <string name="printer_unavailable">"<xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> – kullanılamıyor"</string>

    <!-- Arrays -->

    <!-- Color mode labels. -->
    <string-array name="color_mode_labels">
        <!-- Color modelabel: Monochrome color scheme, e.g. one color is used. [CHAR LIMIT=20] -->
        <item>"Siyah Beyaz"</item>
        <!-- Color mode label: Color color scheme, e.g. many colors are used. [CHAR LIMIT=20] -->
        <item>"Renkli"</item>
    </string-array>

    <!-- Orientation labels. -->
    <string-array name="orientation_labels">
        <!-- Orientation label: Portrait page orientation. [CHAR LIMIT=30] -->
        <item>"Dikey"</item>
        <!-- Orientation label: Landscape page orientation [CHAR LIMIT=30] -->
        <item>"Yatay"</item>
    </string-array>

    <!-- Permissions -->

    <!-- Title of an application permission, listed so the user can choose whether they want
         to allow the application to do this. -->
    <string name="permlab_accessAllPrintJobs" translatable="false">access all print jobs</string>
    <!-- Description of an application permission, listed so the user can choose whether
         they want to allow the application to do this. -->
    <string name="permdesc_accessAllPrintJobs" translatable="false">Allows the holder to access
        print jobs created by another app. Should never be needed for normal apps.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want
         to allow the application to do this. -->
    <string name="permlab_startPrintServiceConfigActivity" translatable="false">start print
        service configuration activities</string>
    <!-- Description of an application permission, listed so the user can choose whether they
         want to allow the application to do this. -->
    <string name="permdesc_startPrintServiceConfigActivity" translatable="false">Allows the
        holder to start the configuration activities of a print service. Should never be needed
        for normal apps.</string>

    <!-- Error messages -->

    <!-- Message for an error when trying to print to a PDF file. [CHAR LIMIT=50] -->
    <string name="print_write_error_message">"Dosyaya yazılamadı"</string>

    <!-- Default message for an error while generating a print job. [CHAR LIMIT=50] -->
    <string name="print_error_default_message">"Maalesef bu işe yaramadı. Tekrar deneyin."</string>

    <!-- Label for the retry button in the error message. [CHAR LIMIT=50] -->
    <string name="print_error_retry">"Yeniden dene"</string>

    <!-- Message for the currently selected printer being unavailable. [CHAR LIMIT=100] -->
    <string name="print_error_printer_unavailable">"Bu yazı şu anda kullanılamıyor."</string>

    <!-- Long running operations -->

    <!-- Message long running operation when preparing print preview. [CHAR LIMIT=50] -->
    <string name="print_preparing_preview">"Önizleme hazırlanıyor…"</string>

</resources>
