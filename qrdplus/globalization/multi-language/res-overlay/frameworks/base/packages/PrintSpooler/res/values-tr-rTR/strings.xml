    <string name="summary_template">"Özet. Kopya sayısı: <xliff:g id="COPIES">%1$s</xliff:g>, kağıt boyutu: <xliff:g id="PAPER_SIZE">%2$s</xliff:g>"</string>

    <!-- Description for the handle to expand all print options - spoken to the user. [CHAR LIMIT=none] -->
    <string name="expand_handle">"Genişletme tutacağı"</string>

