    <plurals name="composite_notification_title_template">
        <item quantity="one">"<xliff:g id="PRINT_JOB_NAME">%1$d</xliff:g> मुद्रण कार्य"</item>
        <item quantity="other">"<xliff:g id="PRINT_JOB_NAME">%1$d</xliff:g> मुद्रण कार्ये"</item>
    </plurals>

    <!-- Label for the notification button for cancelling a print job. [CHAR LIMIT=25] -->
    <string name="cancel">रद्द करा</string>

    <!-- Label for the notification button for restarting a filed print job. [CHAR LIMIT=25] -->
    <string name="restart">"रीस्टार्ट करा"</string>

