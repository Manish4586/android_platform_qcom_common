    <!-- Template for the notification label for a printing print job. [CHAR LIMIT=25] -->
    <string name="printing_notification_title_template">"<xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> मुद्रण करीत आहे"</string>

    <!-- Template for the notification label for a cancelling print job. [CHAR LIMIT=25] -->
    <string name="cancelling_notification_title_template">"<xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g> रद्द करीत आहे"</string>

    <!-- Template for the notification label for a failed print job. [CHAR LIMIT=25] -->
    <string name="failed_notification_title_template">"प्रिंटर त्रुटी <xliff:g id="PRINT_JOB_NAME">%1$s</xliff:g>"</string>

