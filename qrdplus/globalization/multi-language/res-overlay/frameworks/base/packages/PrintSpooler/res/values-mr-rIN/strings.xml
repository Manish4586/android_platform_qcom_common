    <string name="print_search_box_hidden_utterance">"शोध बॉक्स लपविला"</string>

    <!-- Title of the action bar button to got to add a printer. [CHAR LIMIT=25] -->
    <string name="print_add_printer">"प्रिंटर जोडा"</string>

    <!-- Title of the menu item to select a printer. [CHAR LIMIT=25] -->
    <string name="print_select_printer">"प्रिंटर निवडा"</string>

    <!-- Title of the menu item to forget a printer. [CHAR LIMIT=25] -->
    <string name="print_forget_printer">"प्रिंटर विसरा"</string>

