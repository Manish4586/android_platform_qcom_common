<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Dialog title to identify the request from a VPN application. [CHAR LIMIT=60] -->
    <string name="prompt">"សំណើ​សុំ​ការ​តភ្ជាប់"</string>

    <!-- Dialog message to warn about the risk of using a VPN application. [CHAR LIMIT=NONE] -->
    <string name="warning">"<xliff:g id="APP">%s</xliff:g> ចង់​បង្កើត​ការ​តភ្ជាប់ VPN ដែល​អនុញ្ញាត​ឲ្យ​វា​ត្រួតពិនិត្យ​ចរាចរ​បណ្ដាញ។ ព្រម​ទទួល ប្រសិនបើ​អ្នក​ទុកចិត្ត​លើ​ប្រភព​តែប៉ុណ្ណោះ។ &lt;br /&gt; &lt;br /&gt; &lt;img src=vpn_icon /&gt; នឹង​លេចឡើង​នៅ​ផ្នែក​ខាងលើ​នៃ​អេក្រង់​របស់​អ្នក ពេល VPN សកម្ម។"</string>

    <!-- Dialog title for built-in VPN. [CHAR LIMIT=40]  -->
    <string name="legacy_title">"បា​ន​ភ្ជាប់ VPN"</string>
    <!-- Button label to configure the current VPN session. [CHAR LIMIT=20] -->
    <string name="configure">"កំណត់​រចនាសម្ព័ន្ធ"</string>
    <!-- Button label to disconnect the current VPN session. [CHAR LIMIT=20] -->
    <string name="disconnect">"ផ្ដាច់"</string>

    <!-- Label for the name of the current VPN session. [CHAR LIMIT=20] -->
    <string name="session">"សម័យ៖"</string>
    <!-- Label for the duration of the current VPN session. [CHAR LIMIT=20] -->
    <string name="duration">"ថិរវេលា៖"</string>
    <!-- Label for the network usage of data transmitted over VPN. [CHAR LIMIT=20] -->
    <string name="data_transmitted">"បាន​ផ្ញើ៖"</string>
    <!-- Label for the network usage of data received over VPN. [CHAR LIMIT=20] -->
    <string name="data_received">"បាន​ទទួល៖"</string>

    <!-- Formatted string for the network usage over VPN. [CHAR LIMIT=40] -->
    <string name="data_value_format">"<xliff:g id="NUMBER_0">%1$s</xliff:g> បៃ / <xliff:g id="NUMBER_1">%2$s</xliff:g> កញ្ចប់​ព័ត៌មាន"</string>
</resources>
