<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Dialog title to identify the request from a VPN application. [CHAR LIMIT=60] -->
    <string name="prompt">"कनेक्शन अनुरोध"</string>

    <!-- Dialog message to warn about the risk of using a VPN application. [CHAR LIMIT=NONE] -->
    <string name="warning">"<xliff:g id="APP">%s</xliff:g> VPN कनेक्‍शन सेट करना चाहता है जो उसे नेटवर्क ट्रैफ़िक मॉनीटर करने देता है. केवल तभी स्‍वीकार करें, जबकि आप स्रोत पर विश्वास करते हों. VPN सक्रिय होने पर आपकी स्‍क्रीन पर ऊपर &lt;br /&gt; &lt;br /&gt; &lt;img src=vpn_icon /&gt; दिखाई देता है."</string>

    <!-- Dialog title for built-in VPN. [CHAR LIMIT=40]  -->
    <string name="legacy_title">"VPN कनेक्‍ट है"</string>
    <!-- Button label to configure the current VPN session. [CHAR LIMIT=20] -->
    <string name="configure">"कॉन्फ़िगर करें"</string>
    <!-- Button label to disconnect the current VPN session. [CHAR LIMIT=20] -->
    <string name="disconnect">"डिस्‍कनेक्‍ट करें"</string>

    <!-- Label for the name of the current VPN session. [CHAR LIMIT=20] -->
    <string name="session">"सत्र:"</string>
    <!-- Label for the duration of the current VPN session. [CHAR LIMIT=20] -->
    <string name="duration">"अवधि:"</string>
    <!-- Label for the network usage of data transmitted over VPN. [CHAR LIMIT=20] -->
    <string name="data_transmitted">"भेजे गए:"</string>
    <!-- Label for the network usage of data received over VPN. [CHAR LIMIT=20] -->
    <string name="data_received">"प्राप्त:"</string>

    <!-- Formatted string for the network usage over VPN. [CHAR LIMIT=40] -->
    <string name="data_value_format">"<xliff:g id="NUMBER_0">%1$s</xliff:g> बाइट / <xliff:g id="NUMBER_1">%2$s</xliff:g> पैकेट"</string>
</resources>
