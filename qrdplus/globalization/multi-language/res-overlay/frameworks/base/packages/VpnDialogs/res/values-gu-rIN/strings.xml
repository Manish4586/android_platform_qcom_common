<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Dialog title to identify the request from a VPN application. [CHAR LIMIT=60] -->
    <string name="prompt">જોડાણની વિનંતી</string>

    <!-- Dialog message to warn about the risk of using a VPN application. [CHAR LIMIT=NONE] -->
    <string name="warning"><xliff:g id="app">%s</xliff:g> એક VPN જોડાણ સેટ કરવા માંગે છે
જેને નેટવર્કની દેખરેખ કરવાની અનુમતિ આપતું હોય. તમે તેના સ્રોત પર વિશ્વાસ કરતા હોય, તો જ સ્વીકારો.

જ્યારે VPN સક્રિય હોય ત્યારે &lt;br /&gt;
&lt;br /&gt;
&lt;img src="vpn_icon" /&gt;
તમારી સ્ક્રીનની શીર્ષ પર પ્રદર્શિત થાય છે.
    </string>

    <!-- Dialog title for built-in VPN. [CHAR LIMIT=40]  -->
    <string name="legacy_title">VPN જોડાયેલ નથી</string>
    <!-- Button label to configure the current VPN session. [CHAR LIMIT=20] -->
    <string name="configure">કન્ફિગર કરો</string>
    <!-- Button label to disconnect the current VPN session. [CHAR LIMIT=20] -->
    <string name="disconnect">જોડાણ દૂર કરો</string>

    <!-- Label for the name of the current VPN session. [CHAR LIMIT=20] -->
    <string name="session">સત્ર:</string>
    <!-- Label for the duration of the current VPN session. [CHAR LIMIT=20] -->
    <string name="duration">સમયગાળો:</string>
    <!-- Label for the network usage of data transmitted over VPN. [CHAR LIMIT=20] -->
    <string name="data_transmitted">મોકલેલ:</string>
    <!-- Label for the network usage of data received over VPN. [CHAR LIMIT=20] -->
    <string name="data_received">પ્રાપ્ત થયેલ:</string>

    <!-- Formatted string for the network usage over VPN. [CHAR LIMIT=40] -->
    <string name="data_value_format">
        <xliff:g id="number">%1$s</xliff:g> બાઇટ્સ /
<xliff:g id="number">%2$s</xliff:g> પેકેટ્સ
    </string>
</resources>
