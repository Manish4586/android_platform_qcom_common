<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Dialog title to identify the request from a VPN application. [CHAR LIMIT=60] -->
    <string name="prompt">"درخواست اتصال"</string>

    <!-- Dialog message to warn about the risk of using a VPN application. [CHAR LIMIT=NONE] -->
    <string name="warning">"‏<xliff:g id="APP">%s</xliff:g> می‌خواهد یک اتصال VPN راه‌اندازی کند که به آن امکان نظارت بر ترافیک شبکه را می‌دهد. فقط در صورتی بپذیرید که به منبع آن اطمینان دارید. هنگامی که VPN فعال شد، &lt;br /&gt; &lt;br /&gt; &lt;img src=vpn_icon /&gt; در بالای صفحه نمایش شما نشان داده می‌شود."</string>

    <!-- Dialog title for built-in VPN. [CHAR LIMIT=40]  -->
    <string name="legacy_title">"‏VPN متصل است"</string>
    <!-- Button label to configure the current VPN session. [CHAR LIMIT=20] -->
    <string name="configure">"پیکربندی"</string>
    <!-- Button label to disconnect the current VPN session. [CHAR LIMIT=20] -->
    <string name="disconnect">"قطع اتصال"</string>

    <!-- Label for the name of the current VPN session. [CHAR LIMIT=20] -->
    <string name="session">"جلسه:"</string>
    <!-- Label for the duration of the current VPN session. [CHAR LIMIT=20] -->
    <string name="duration">"مدت زمان:"</string>
    <!-- Label for the network usage of data transmitted over VPN. [CHAR LIMIT=20] -->
    <string name="data_transmitted">"ارسال شده:"</string>
    <!-- Label for the network usage of data received over VPN. [CHAR LIMIT=20] -->
    <string name="data_received">"دریافتی:"</string>

    <!-- Formatted string for the network usage over VPN. [CHAR LIMIT=40] -->
    <string name="data_value_format">"<xliff:g id="NUMBER_0">%1$s</xliff:g> بایت / <xliff:g id="NUMBER_1">%2$s</xliff:g> بسته"</string>
</resources>
