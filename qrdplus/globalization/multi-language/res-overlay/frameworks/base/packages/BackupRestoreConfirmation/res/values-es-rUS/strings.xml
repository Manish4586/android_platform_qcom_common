<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of the activity when a full backup has been requested and must be confirmed -->
    <string name="backup_confirm_title">"Realización de la copia de seguridad completa"</string>
    <!-- Title of the activity when a full restore has been requested and must be confirmed -->
    <string name="restore_confirm_title">"Restauración completa"</string>

    <!-- Text for message to user that a full backup has been requested, and must be confirmed. -->
    <string name="backup_confirm_text">"Se ha solicitado una copia de seguridad completa de todos los datos en una computadora de escritorio conectada. ¿Deseas permitirla?\n\nSi tú no has solicitado la copia de seguridad, no permitas que se realice la operación."</string>
    <!-- Button to allow a requested full backup to occur -->
    <string name="allow_backup_button_label">"Copia de seguridad de mis datos"</string>
    <!-- Button to refuse to allow the requested full backup -->
    <string name="deny_backup_button_label">"No realizar una copia de seguridad"</string>

    <!-- Text for message to user that a full restore has been requested, and must be confirmed. -->
    <string name="restore_confirm_text">"Se ha solicitado una restauración completa de todos los datos de una computadora de escritorio conectada. ¿Deseas permitirla?\n\nSi tú no has solicitado la restauración, no permitas que se realice la operación. Esto reemplazaría los datos actuales del dispositivo."</string>
    <!-- Button to allow a requested full restore to occur -->
    <string name="allow_restore_button_label">"Restaurar mis datos"</string>
    <!-- Button to refuse to allow the requested full restore -->
    <string name="deny_restore_button_label">"No restaurar"</string>

    <!-- Text for message to user that they must enter their predefined backup password in order to perform this operation. -->
    <string name="current_password_text">"Introduce tu contraseña actual de copia de seguridad a continuación:"</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this restore operation. -->
    <string name="device_encryption_restore_text">"Introduce tu contraseña de encriptación del dispositivo a continuación:"</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this backup operation. -->
    <string name="device_encryption_backup_text">"Introduce tu contraseña de encriptación del dispositivo a continuación. Esta contraseña también se utilizará para encriptar la copia de seguridad."</string>

    <!-- Text for message to user that they must enter an encryption password to use for the full backup operation. -->
    <string name="backup_enc_password_text">"Introduce una contraseña para encriptar los datos de la copia de seguridad completa. Si dejas este campo en blanco, se utilizará tu contraseña actual de copia de seguridad:"</string>
    <!-- Text for message to user that they may optionally supply an encryption password to use for a full backup operation. -->
    <string name="backup_enc_password_optional">"Si deseas encriptar los datos de la copia de seguridad completa, introduce una contraseña a continuación:"</string>
    <!-- Text for message to user that they must supply an encryption password to use for a full backup operation because their phone is locked. -->
    <string name="backup_enc_password_required">"Dado que el dispositivo está encriptado, es necesario que encriptes la copia de seguridad. Ingresa una contraseña a continuación:"</string>

    <!-- Text for message to user when performing a full restore operation, explaining that they must enter the password originally used to encrypt the full backup data. -->
    <string name="restore_enc_password_text">"Si los datos de recuperación están encriptados, vuelve a introducir la contraseña a continuación:"</string>

    <!-- Text of a toast telling the user that a full backup operation has begun -->
    <string name="toast_backup_started">"Iniciando copia de seguridad..."</string>
    <!-- Text of a toast telling the user that a full backup operation has ended -->
    <string name="toast_backup_ended">"La realización de la copia de seguridad finalizó."</string>
    <!-- Text of a toast telling the user that a full restore operation has begun -->
    <string name="toast_restore_started">"Iniciando restauración..."</string>
    <!-- Text of a toast telling the user that a full restore operation has ended -->
    <string name="toast_restore_ended">"La restauración finalizó."</string>
    <!-- Text of a toast telling the user that the operation has timed out -->
    <string name="toast_timeout">"Se agotó el tiempo de espera para la operación."</string>
</resources>
