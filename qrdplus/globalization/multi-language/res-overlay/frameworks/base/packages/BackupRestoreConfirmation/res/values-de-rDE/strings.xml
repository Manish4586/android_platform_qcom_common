<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of the activity when a full backup has been requested and must be confirmed -->
    <string name="backup_confirm_title">"Vollständige Sicherung"</string>
    <!-- Title of the activity when a full restore has been requested and must be confirmed -->
    <string name="restore_confirm_title">"Vollständige Wiederherstellung"</string>

    <!-- Text for message to user that a full backup has been requested, and must be confirmed. -->
    <string name="backup_confirm_text">"Es wurde eine vollständige Sicherung sämtlicher Daten auf einen verbundenen Desktop-Computer angefordert. Möchten Sie dies zulassen?\n\nWenn Sie die Sicherung nicht selbst angefordert haben, sollten Sie dem Vorgang nicht zustimmen."</string>
    <!-- Button to allow a requested full backup to occur -->
    <string name="allow_backup_button_label">"Meine Daten sichern"</string>
    <!-- Button to refuse to allow the requested full backup -->
    <string name="deny_backup_button_label">"Nicht sichern"</string>

    <!-- Text for message to user that a full restore has been requested, and must be confirmed. -->
    <string name="restore_confirm_text">"Es wurde eine vollständige Wiederherstellung aller Daten von einem verbundenen Desktop-Computer angefordert. Möchten Sie dies zulassen?\n\nWenn Sie die Wiederherstellung nicht selbst angefordert haben, sollten Sie dem Vorgang nicht zustimmen. Dadurch werden alle zurzeit auf dem Gerät befindlichen Daten ersetzt!"</string>
    <!-- Button to allow a requested full restore to occur -->
    <string name="allow_restore_button_label">"Meine Daten wiederherstellen"</string>
    <!-- Button to refuse to allow the requested full restore -->
    <string name="deny_restore_button_label">"Nicht wiederherstellen"</string>

    <!-- Text for message to user that they must enter their predefined backup password in order to perform this operation. -->
    <string name="current_password_text">"Geben Sie Ihr aktuelles Sicherungspasswort unten ein:"</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this restore operation. -->
    <string name="device_encryption_restore_text">"Geben Sie Ihr Passwort zur Geräteverschlüsselung unten ein."</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this backup operation. -->
    <string name="device_encryption_backup_text">"Geben Sie Ihr Passwort zur Geräteverschlüsselung unten ein. Damit wird auch das Sicherungsarchiv verschlüsselt."</string>

    <!-- Text for message to user that they must enter an encryption password to use for the full backup operation. -->
    <string name="backup_enc_password_text">"Geben Sie ein Passwort für die Verschlüsselung der vollständigen Sicherungsdaten ein. Wenn Sie dieses Feld leer lassen, wird Ihr aktuelles Sicherungspasswort verwendet:"</string>
    <!-- Text for message to user that they may optionally supply an encryption password to use for a full backup operation. -->
    <string name="backup_enc_password_optional">"Wenn Sie die gesamten Sicherungsdaten verschlüsseln möchten, geben Sie unten ein Passwort ein:"</string>
    <!-- Text for message to user that they must supply an encryption password to use for a full backup operation because their phone is locked. -->
    <string name="backup_enc_password_required">"Da Ihr Gerät verschlüsselt ist, muss auch die Sicherung verschlüsselt werden. Geben Sie unten ein Passwort ein:"</string>

    <!-- Text for message to user when performing a full restore operation, explaining that they must enter the password originally used to encrypt the full backup data. -->
    <string name="restore_enc_password_text">"Geben Sie das Passwort unten ein, wenn die Daten für die Wiederherstellung verschlüsselt sind:"</string>

    <!-- Text of a toast telling the user that a full backup operation has begun -->
    <string name="toast_backup_started">"Sicherung wird gestartet..."</string>
    <!-- Text of a toast telling the user that a full backup operation has ended -->
    <string name="toast_backup_ended">"Sicherung abgeschlossen"</string>
    <!-- Text of a toast telling the user that a full restore operation has begun -->
    <string name="toast_restore_started">"Wiederherstellung wird gestartet..."</string>
    <!-- Text of a toast telling the user that a full restore operation has ended -->
    <string name="toast_restore_ended">"Wiederherstellung beendet"</string>
    <!-- Text of a toast telling the user that the operation has timed out -->
    <string name="toast_timeout">"Zeitüberschreitung bei Vorgang"</string>
</resources>
