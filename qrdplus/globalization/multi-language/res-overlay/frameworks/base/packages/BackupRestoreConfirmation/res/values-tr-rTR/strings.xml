<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of the activity when a full backup has been requested and must be confirmed -->
    <string name="backup_confirm_title">"Tam yedekleme"</string>
    <!-- Title of the activity when a full restore has been requested and must be confirmed -->
    <string name="restore_confirm_title">"Tam geri yükleme"</string>

    <!-- Text for message to user that a full backup has been requested, and must be confirmed. -->
    <string name="backup_confirm_text">"Tüm verilerin bağlı bir masaüstü bilgisayara tam olarak yedeklenmesi için istekte bulunuldu?\n\nYedekleme isteğinde siz bulunmadıysanız, işlemin devam etmesine izin vermeyin."</string>
    <!-- Button to allow a requested full backup to occur -->
    <string name="allow_backup_button_label">"Verilerimi yedekle"</string>
    <!-- Button to refuse to allow the requested full backup -->
    <string name="deny_backup_button_label">"Yedekleme"</string>

    <!-- Text for message to user that a full restore has been requested, and must be confirmed. -->
    <string name="restore_confirm_text">"Tüm verilerin, bağlı bir masaüstü bilgisayardan tam olarak geri yüklenmesi için istekte bulunuldu. Bu işleme izin vermek istiyor musunuz?\n\nGeri yükleme isteğinde siz bulunmadıysanız, işlemin ilerlemesine izin vermeyin. Bu işlem, cihazınızdaki tüm verileri silip üzerine yazar!"</string>
    <!-- Button to allow a requested full restore to occur -->
    <string name="allow_restore_button_label">"Verilerimi geri yükle"</string>
    <!-- Button to refuse to allow the requested full restore -->
    <string name="deny_restore_button_label">"Geri yükleme"</string>

    <!-- Text for message to user that they must enter their predefined backup password in order to perform this operation. -->
    <string name="current_password_text">"Lütfen mevcut yedekleme şifrenizi aşağıya girin:"</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this restore operation. -->
    <string name="device_encryption_restore_text">"Lütfen cihazı şifrelemek için kullandığınız şifreyi aşağıya girin."</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this backup operation. -->
    <string name="device_encryption_backup_text">"Lütfen cihazınızı şifrelemek için kullandığınız şifreyi aşağıya girin. Bu şifre aynı zamanda yedekleme arşivini şifrelemek için de kullanılacaktır."</string>

    <!-- Text for message to user that they must enter an encryption password to use for the full backup operation. -->
    <string name="backup_enc_password_text">"Tam yedekleme verilerini şifrelemek için lütfen bir şifre girin. Boş bırakılırsa, mevcut yedekleme şifreniz kullanılır:"</string>
    <!-- Text for message to user that they may optionally supply an encryption password to use for a full backup operation. -->
    <string name="backup_enc_password_optional">"Yedeklenen tüm verileri şifrelemek isterseniz, aşağıya bir şifre girin:"</string>
    <!-- Text for message to user that they must supply an encryption password to use for a full backup operation because their phone is locked. -->
    <string name="backup_enc_password_required">"Cihazınız şifrelenmiş olduğundan yedeklemenizi şifrelemeniz gerekmektedir. Lütfen şifreyi aşağıya girin:"</string>

    <!-- Text for message to user when performing a full restore operation, explaining that they must enter the password originally used to encrypt the full backup data. -->
    <string name="restore_enc_password_text">"Geri yükleme verileri şifreliyse, lütfen şifreyi aşağıya girin:"</string>

    <!-- Text of a toast telling the user that a full backup operation has begun -->
    <string name="toast_backup_started">"Yedekleme başlıyor..."</string>
    <!-- Text of a toast telling the user that a full backup operation has ended -->
    <string name="toast_backup_ended">"Yedekleme işlemi sona erdi"</string>
    <!-- Text of a toast telling the user that a full restore operation has begun -->
    <string name="toast_restore_started">"Geri yükleme başlıyor..."</string>
    <!-- Text of a toast telling the user that a full restore operation has ended -->
    <string name="toast_restore_ended">"Geri yükleme işlemi sona erdi"</string>
    <!-- Text of a toast telling the user that the operation has timed out -->
    <string name="toast_timeout">"İşlem zaman aşımına uğradı"</string>
</resources>
