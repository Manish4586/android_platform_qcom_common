<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of the activity when a full backup has been requested and must be confirmed -->
    <string name="backup_confirm_title">পুরো ব্যাক আপ</string>
    <!-- Title of the activity when a full restore has been requested and must be confirmed -->
    <string name="restore_confirm_title">পুরো পুনঃসঞ্চয়</string>

    <!-- Text for message to user that a full backup has been requested, and must be confirmed. -->
    <string name="backup_confirm_text">একটি সংযুক্ত ডেস্কটপ কম্পিউটারের সমস্ত ডেটার একটি সম্পূর্ণ ব্যাক আপের অনুরোধ জানানো হয়েছে।  আপনি কি এটি হতে দিতে চান\?\n\nআপনি নিজে ব্যাক আপের অনুমতি জানিয়ে না থাকলে, কাজটি হওয়ার অনুমতি দেবেন না।</string>
    <!-- Button to allow a requested full backup to occur -->
    <string name="allow_backup_button_label">আমার ডেটার ব্যাক আপ নিন</string>
    <!-- Button to refuse to allow the requested full backup -->
    <string name="deny_backup_button_label">ব্যাক আপ নেবেন না</string>

    <!-- Text for message to user that a full restore has been requested, and must be confirmed. -->
    <string name="restore_confirm_text">একটি সংযুক্ত ডেস্কটপ কম্পিউটার থেকে সমস্ত ডেটার পুরো পুনঃসঞ্চয়ের অনুরোধ জানানো হয়েছে।  আপনি কি এটি হতে দিতে চান\?\n\nআপনি নিজে পুনঃসঞ্চয়ের অনুমতি জানিয়ে না থাকলে, কাজটি হওয়ার অনুমতি দেবেন না।  এটি আপনার ডিভাইসে বর্তমানে থাকা কোন ডেটা প্রতিস্থাপিত করবে!</string>
    <!-- Button to allow a requested full restore to occur -->
    <string name="allow_restore_button_label">আমার ডেটা পুনঃসঞ্চয় করুন</string>
    <!-- Button to refuse to allow the requested full restore -->
    <string name="deny_restore_button_label">পুনঃসঞ্চয় করবেন না</string>

    <!-- Text for message to user that they must enter their predefined backup password in order to perform this operation. -->
    <string name="current_password_text">অনুগ্রহ করে নীচে আপনার বর্তমান ব্যাক আপ পাসওয়ার্ড দিন:</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this restore operation. -->
    <string name="device_encryption_restore_text">অনুগ্রহ করে নীচে আপনার ডিভাইস এনক্রিপ্টশন পাসওয়ার্ড দিন।</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this backup operation. -->
    <string name="device_encryption_backup_text">অনুগ্রহ করে নীচে আপনার ডিভাইস এনক্রিপ্টশন পাসওয়ার্ড দিন। ব্যাক আপ সংরক্ষণাগার এনক্রিপ্ট করতে এটিকেও ব্যবহার করা হবে।</string>

    <!-- Text for message to user that they must enter an encryption password to use for the full backup operation. -->
    <string name="backup_enc_password_text">অনুগ্রহ করে পুরো ব্যাক আপ ডেটা এনক্রিপ্ট করতে একটি পাসওয়ার্ড দিন। এটি খালি রাখা হলে, আপনার বর্তমান ব্যাক আপ পাসওয়ার্ড ব্যবহার করা হবে:</string>
    <!-- Text for message to user that they may optionally supply an encryption password to use for a full backup operation. -->
    <string name="backup_enc_password_optional">আপনি পুরো ব্যাক আপ ডেটা এনক্রিপ্ট করতে চাইলে, নীচে একটি পাসওয়ার্ড দিন:</string>
    <!-- Text for message to user that they must supply an encryption password to use for a full backup operation because their phone is locked. -->
    <string name="backup_enc_password_required">আপনার ডিভাইসটি এনক্রিপ্ট থাকার কারণে আপনাকে আপনার ব্যাকআপ এনক্রিপ্ট করতে হবে। অনুগ্রহ করে নীচে একটি পাসওয়ার্ড লিখুন:</string>

    <!-- Text for message to user when performing a full restore operation, explaining that they must enter the password originally used to encrypt the full backup data. -->
    <string name="restore_enc_password_text">পুনঃসঞ্চয় ডেটা এনক্রিপ্ট করা হলে, অনুগ্রহ করে নীচে পাসওয়ার্ড দিন:</string>

    <!-- Text of a toast telling the user that a full backup operation has begun -->
    <string name="toast_backup_started">ব্যাক আপ নেওয়া শুরু হচ্ছে...</string>
    <!-- Text of a toast telling the user that a full backup operation has ended -->
    <string name="toast_backup_ended">ব্যাক আপ নেওয়া শেষ হয়েছে</string>
    <!-- Text of a toast telling the user that a full restore operation has begun -->
    <string name="toast_restore_started">পুনঃসঞ্চয় করা শুরু হচ্ছে...</string>
    <!-- Text of a toast telling the user that a full restore operation has ended -->
    <string name="toast_restore_ended">পুনঃসঞ্চয় করা শেষ হয়েছে</string>
    <!-- Text of a toast telling the user that the operation has timed out -->
    <string name="toast_timeout">কাজটির সময় শেষ হয়ে গেছে</string>
</resources>
