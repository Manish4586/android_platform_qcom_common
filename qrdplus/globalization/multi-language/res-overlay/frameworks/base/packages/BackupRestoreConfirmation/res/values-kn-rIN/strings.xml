<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of the activity when a full backup has been requested and must be confirmed -->
    <string name="backup_confirm_title">"ಸಂಪೂರ್ಣ ಬ್ಯಾಕಪ್"</string>
    <!-- Title of the activity when a full restore has been requested and must be confirmed -->
    <string name="restore_confirm_title">"ಸಂಪೂರ್ಣ ಮರುಸ್ಥಾಪನೆ"</string>

    <!-- Text for message to user that a full backup has been requested, and must be confirmed. -->
    <string name="backup_confirm_text">"ಸಂಪರ್ಕಗೊಂಡಿರುವ ಡೆಸ್ಕ್‌ಟಾಪ್ ಕಂಪ್ಯೂಟರ್‌ಗೆ ಎಲ್ಲಾ ಡೇಟಾದ ಸಂಪೂರ್ಣ ಬ್ಯಾಕಪ್ ಮಾಡಲು ವಿನಂತಿಸಲಾಗಿದೆ. ಇದಕ್ಕೆ ನೀವು ಅವಕಾಶ ಮಾಡಿಕೊಡುವಿರಾ? \n\nನೀವಾಗಿಯೇ ಬ್ಯಾಕಪ್ ಮಾಡಿಕೊಳ್ಳಲು ವಿನಂತಿಸಿಲ್ಲವಾದರೆ, ಕಾರ್ಯಾಚರಣೆ ಮುಂದುವರಿಯಲು ಅನುಮತಿಸಬೇಡಿ."</string>
    <!-- Button to allow a requested full backup to occur -->
    <string name="allow_backup_button_label">"ನನ್ನ ಡೇಟಾವನ್ನು ಬ್ಯಾಕಪ್ ಮಾಡು"</string>
    <!-- Button to refuse to allow the requested full backup -->
    <string name="deny_backup_button_label">"ಬ್ಯಾಕ್ ಅಪ್ ಮಾಡದಿರು"</string>

    <!-- Text for message to user that a full restore has been requested, and must be confirmed. -->
    <string name="restore_confirm_text">"ಸಂಪರ್ಕಗೊಂಡಿರುವ ಡೆಸ್ಕ್‌ಟಾಪ್ ಕಂಪ್ಯೂಟರ್‌ನಿಂದ ಎಲ್ಲಾ ಡೇಟಾದ ಸಂಪೂರ್ಣ ಮರುಸ್ಥಾಪನೆಗೆ ವಿನಂತಿಸಿಕೊಳ್ಳಲಾಗಿದೆ. ಇದಕ್ಕೆ ನೀವು ಅವಕಾಶ ಮಾಡಿಕೊಡುವಿರಾ? \n\nನೀವಾಗಿಯೇ ಮರುಸ್ಥಾಪಿಸಿಕೊಳ್ಳಲು ನೀವು ವಿನಂತಿಸಿಲ್ಲವಾದರೆ, ಕಾರ್ಯಾಚರಣೆ ಮುಂದುವರಿಯಲು ಅನುಮತಿಸಬೇಡಿ. ಇದು ಪ್ರಸ್ತುತ ಸಾಧನದಲ್ಲಿರುವ ಯಾವುದೇ ಡೇಟಾವನ್ನು ಬದಲಾಯಿಸಬಲ್ಲದು!"</string>
    <!-- Button to allow a requested full restore to occur -->
    <string name="allow_restore_button_label">"ನನ್ನ ಡೇಟಾವನ್ನು ಮರುಸಂಗ್ರಹಿಸು"</string>
    <!-- Button to refuse to allow the requested full restore -->
    <string name="deny_restore_button_label">"ಮರುಸಂಗ್ರಹಿಸದಿರು"</string>

    <!-- Text for message to user that they must enter their predefined backup password in order to perform this operation. -->
    <string name="current_password_text">"ದಯವಿಟ್ಟು ನಿಮ್ಮ ಪ್ರಸ್ತುತ ಬ್ಯಾಕಪ್ ಪಾಸ್‌ವರ್ಡ್ ಅನ್ನು ಕೆಳಗೆ ನಮೂದಿಸಿ:"</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this restore operation. -->
    <string name="device_encryption_restore_text">"ದಯವಿಟ್ಟು ನಿಮ್ಮ ಸಾಧನದ ಎನ್‌ಕ್ರಿಪ್ಷನ್ ಪಾಸ್‌ವರ್ಡ್ ಅನ್ನು ಕೆಳಗೆ ನಮೂದಿಸಿ."</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this backup operation. -->
    <string name="device_encryption_backup_text">"ದಯವಿಟ್ಟು ಕೆಳಗೆ ನಿಮ್ಮ ಸಾಧನದ ಎನ್‌ಕ್ರಿಪ್ಶನ್ ಪಾಸ್‌ವರ್ಡ್ ನಮೂದಿಸಿ. ಇದನ್ನು ಬ್ಯಾಕಪ್ ಆರ್ಕೈವ್ ಅನ್ನು ಎನ್‌ಕ್ರಿಪ್ಟ್ ಮಾಡಲು ಸಹ ಬಳಸಲಾಗುತ್ತದೆ."</string>

    <!-- Text for message to user that they must enter an encryption password to use for the full backup operation. -->
    <string name="backup_enc_password_text">"ಸಂಪೂರ್ಣ ಬ್ಯಾಕಪ್ ಡೇಟಾವನ್ನು ಎನ್‌ಕ್ರಿಪ್ಟ್ ಮಾಡಲು ಪಾಸ್‌ವರ್ಡ್ ನಮೂದಿಸಿ. ಇದನ್ನು ಖಾಲಿಯಾಗಿಯೇ ಬಿಟ್ಟರೆ, ನಿಮ್ಮ ಪ್ರಸ್ತುತ ಬ್ಯಾಕಪ್ ಪಾಸ್‌ವರ್ಡ್ ಅನ್ನು ಬಳಸಲಾಗುತ್ತದೆ:"</string>
    <!-- Text for message to user that they may optionally supply an encryption password to use for a full backup operation. -->
    <string name="backup_enc_password_optional">"ನೀವು ಸಂಪೂರ್ಣ ಬ್ಯಾಕಪ್ ಡೇಟಾವನ್ನು ಎನ್‌ಕ್ರಿಪ್ಟ್ ಮಾಡಲು ಬಯಸಿದರೆ, ಕಳಗೆ ಪಾಸ್‌ವರ್ಡ್ ನಮೂದಿಸಿ:"</string>
    <!-- Text for message to user that they must supply an encryption password to use for a full backup operation because their phone is locked. -->
    <string name="backup_enc_password_required">"ನಿಮ್ಮ ಸಾಧನವನ್ನು ಎನ್‌ಕ್ರಿಪ್ಟ್ ಮಾಡಿರುವ ಕಾರಣ, ನಿಮ್ಮ ಬ್ಯಾಕಪ್ ಅನ್ನು ನಿವು ಎನ್‌ಕ್ರಿಪ್ಟ್ ಮಾಡುವ ಅಗತ್ಯವಿದೆ. ದಯವಿಟ್ಟು ಕೆಳಗೆ ಪಾಸ್‌ವರ್ಡ್ ನಮೂದಿಸಿ:"</string>

    <!-- Text for message to user when performing a full restore operation, explaining that they must enter the password originally used to encrypt the full backup data. -->
    <string name="restore_enc_password_text">"ಮರುಸ್ಥಾಪಿಸಲಾದ ಡೇಟಾವನ್ನು ಎನ್‌ಕ್ರಿಪ್ಟ್ ಮಾಡಲಾಗಿದ್ದರೆ, ದಯವಿಟ್ಟು ಪಾಸ್‌ವರ್ಡ್ ಕೆಳಗೆ ನಮೂದಿಸಿ:"</string>

    <!-- Text of a toast telling the user that a full backup operation has begun -->
    <string name="toast_backup_started">"ಬ್ಯಾಕಪ್ ಪ್ರಾರಂಭವಾಗುತ್ತಿದೆ..."</string>
    <!-- Text of a toast telling the user that a full backup operation has ended -->
    <string name="toast_backup_ended">"ಬ್ಯಾಕಪ್ ಪೂರ್ಣಗೊಂಡಿದೆ"</string>
    <!-- Text of a toast telling the user that a full restore operation has begun -->
    <string name="toast_restore_started">"ಮರುಸ್ಥಾಪನೆ ಪ್ರಾರಂಭಗೊಂಡಿದೆ..."</string>
    <!-- Text of a toast telling the user that a full restore operation has ended -->
    <string name="toast_restore_ended">"ಮರುಸ್ಥಾಪನೆ ಅಂತ್ಯಗೊಂಡಿದೆ"</string>
    <!-- Text of a toast telling the user that the operation has timed out -->
    <string name="toast_timeout">"ಕಾರ್ಯಾಚರಣೆ ಸಮಯ ಮೀರಿದೆ"</string>
</resources>
