<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of the activity when a full backup has been requested and must be confirmed -->
    <string name="backup_confirm_title">"Pełna kopia zapasowa"</string>
    <!-- Title of the activity when a full restore has been requested and must be confirmed -->
    <string name="restore_confirm_title">"Pełne przywracanie"</string>

    <!-- Text for message to user that a full backup has been requested, and must be confirmed. -->
    <string name="backup_confirm_text">"Zażądano wykonania pełnej kopii zapasowej wszystkich danych na podłączonym komputerze stacjonarnym. Czy chcesz na to zezwolić?\n\nJeśli żądanie utworzenia kopii zapasowej nie pochodzi od Ciebie, nie zezwalaj na kontynuowanie tej operacji."</string>
    <!-- Button to allow a requested full backup to occur -->
    <string name="allow_backup_button_label">"Utwórz kopię zapasową danych"</string>
    <!-- Button to refuse to allow the requested full backup -->
    <string name="deny_backup_button_label">"Nie twórz kopii zapasowej"</string>

    <!-- Text for message to user that a full restore has been requested, and must be confirmed. -->
    <string name="restore_confirm_text">"Zażądano pełnego przywrócenia wszystkich danych z połączonego komputera stacjonarnego. Czy chcesz na to zezwolić?\n\nJeśli żądanie przywrócenia nie pochodzi od Ciebie, nie zezwalaj na kontynuowanie tej operacji. Spowoduje to zastąpienie wszelkich danych znajdujących się aktualnie w urządzeniu."</string>
    <!-- Button to allow a requested full restore to occur -->
    <string name="allow_restore_button_label">"Przywróć moje dane"</string>
    <!-- Button to refuse to allow the requested full restore -->
    <string name="deny_restore_button_label">"Nie przywracaj"</string>

    <!-- Text for message to user that they must enter their predefined backup password in order to perform this operation. -->
    <string name="current_password_text">"Wpisz poniżej aktualne hasło kopii zapasowej:"</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this restore operation. -->
    <string name="device_encryption_restore_text">"Wpisz poniżej hasło szyfrowania urządzenia."</string>
    <!-- Text for message to user that they must enter their device encryption password in order to perform this backup operation. -->
    <string name="device_encryption_backup_text">"Wpisz poniżej hasło szyfrowania urządzenia. Służy ono również do szyfrowania archiwum kopii zapasowych."</string>

    <!-- Text for message to user that they must enter an encryption password to use for the full backup operation. -->
    <string name="backup_enc_password_text">"Wpisz hasło do zaszyfrowania pełnej kopii zapasowej. Jeśli pozostawisz puste pole, zostanie użyte aktualne hasło kopii zapasowej:"</string>
    <!-- Text for message to user that they may optionally supply an encryption password to use for a full backup operation. -->
    <string name="backup_enc_password_optional">"Jeśli chcesz zaszyfrować pełną kopię zapasową, wprowadź poniżej hasło:"</string>
    <!-- Text for message to user that they must supply an encryption password to use for a full backup operation because their phone is locked. -->
    <string name="backup_enc_password_required">"Ponieważ Twoje urządzenie jest szyfrowane, musisz też zaszyfrować jego kopię zapasową. Wpisz hasło poniżej:"</string>

    <!-- Text for message to user when performing a full restore operation, explaining that they must enter the password originally used to encrypt the full backup data. -->
    <string name="restore_enc_password_text">"Jeśli przywracane dane są zaszyfrowane, wpisz poniżej hasło:"</string>

    <!-- Text of a toast telling the user that a full backup operation has begun -->
    <string name="toast_backup_started">"Tworzenie kopii zapasowej..."</string>
    <!-- Text of a toast telling the user that a full backup operation has ended -->
    <string name="toast_backup_ended">"Utworzono kopię zapasową"</string>
    <!-- Text of a toast telling the user that a full restore operation has begun -->
    <string name="toast_restore_started">"Przywracanie..."</string>
    <!-- Text of a toast telling the user that a full restore operation has ended -->
    <string name="toast_restore_ended">"Przywracanie zakończone"</string>
    <!-- Text of a toast telling the user that the operation has timed out -->
    <string name="toast_timeout">"Operacja przekroczyła czas oczekiwania"</string>
</resources>
