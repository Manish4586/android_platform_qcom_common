    <string name="menu_advanced_hide" product="default">"SD കാർഡ് മറയ്‌ക്കുക"</string>

    <!-- Menu item that reveals the sizes of displayed files [CHAR LIMIT=24] -->
    <string name="menu_file_size_show">"ഫയൽ വലുപ്പം കാണിക്കുക"</string>
    <!-- Menu item that hides the sizes of displayed files [CHAR LIMIT=24] -->
    <string name="menu_file_size_hide">"ഫയൽ വലുപ്പം മറയ്‌ക്കുക"</string>

    <!-- Action mode title summarizing the number of documents selected [CHAR LIMIT=32] -->
    <string name="mode_selected_count">"<xliff:g id="COUNT">%1$d</xliff:g> തിരഞ്ഞെടുത്തു"</string>

