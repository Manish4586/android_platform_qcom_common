    <string name="save_error">"បាន​បរាជ័យ​ក្នុង​ការ​រក្សា​ទុក​ឯកសារ"</string>
    <!-- Toast shown when creating a folder failed with an error [CHAR LIMIT=48] -->
    <string name="create_error">"បាន​បរាជ័យ​ក្នុង​ការ​បង្កើត​ថត"</string>
    <!-- Error message shown when querying for a list of documents failed [CHAR LIMIT=48] -->
    <string name="query_error">"បាន​បរាជ័យ​ក្នុង​ការ​​ច្រោះ​ឯកសារ"</string>

    <!-- Title of storage root location that contains recently modified or used documents [CHAR LIMIT=24] -->
    <string name="root_recent">"ថ្មីៗ"</string>
    <!-- Subtitle of storage root indicating the total free space available, in bytes [CHAR LIMIT=24] -->
    <string name="root_available_bytes">"ទំនេរ <xliff:g id="SIZE">%1$s</xliff:g>"</string>

    <!-- Header title for list of storage roots that contains cloud services [CHAR LIMIT=24] -->
    <string name="root_type_service">"សេវាកម្ម​ផ្ដល់​ឧបករណ៍​ផ្ទុក"</string>
    <!-- Header title for list of storage roots that contains shortcuts to documents that may be available elsewhere [CHAR LIMIT=24] -->
    <string name="root_type_shortcut">"ផ្លូវកាត់"</string>
    <!-- Header title for list of storage roots that contains physical devices [CHAR LIMIT=24] -->
    <string name="root_type_device">"ឧបករណ៍"</string>
    <!-- Header title for list of additional apps that can provide documents [CHAR LIMIT=24] -->
    <string name="root_type_apps">"កម្ម​វិធី​​ច្រើន​ទៀត"</string>

    <!-- Text shown when a directory of documents is empty [CHAR LIMIT=24] -->
    <string name="empty">"គ្មានធាតុ​"</string>

    <!-- Toast shown when no app can be found to open the selected document [CHAR LIMIT=48] -->
    <string name="toast_no_application">"មិន​អាច​បើក​ឯកសារ"</string>
    <!-- Toast shown when some of the selected documents failed to be deleted [CHAR LIMIT=48] -->
    <string name="toast_failed_delete">"មិន​អាច​លុប​ឯកសារ​មួយ​ចំនួន"</string>

    <!-- Title of dialog when prompting user to select an app to share documents with [CHAR LIMIT=32] -->
    <string name="share_via">"ចែករំលែក​តាម"</string>

    <string name="no_permission_for_drm">"សូម​ទោស, មិន​អាច​ចែករំលែក​ឯកសារ DRM"</string>
    <string name="drm_license_info">ព័ត៌មាន​អាជ្ញាប័ណ្ណ Drm</string>
    <string name="action_consumes_rights">សិទ្ធិ​នឹង​ត្រូវ​បាន​ប្រើប្រាស់​សម្រាប់​ចាក់​មេឌៀ​នេះ</string>
    <string name="rights_expired">សិទ្ធិ​បាន​ផុត​កំណត់​សម្រាប់​បទ​ចម្រៀង</string>

    <string name="toast_not_show_thumbnail">មិន​អាច​បង្ហាញ​រូបភាព​លើ​ឧបករណ៍​ដែល​មាន​អង្គចងចាំ​តូច។</string>
</resources>
