    <string name="menu_advanced_show" product="nosdcard">"បង្ហាញឧបករណ៍ផ្ទុកខាងក្នុង"</string>
    <!-- Menu item that reveals SD cards built into the device [CHAR LIMIT=24] -->
    <string name="menu_advanced_show" product="default">"បង្ហាញកាតអេសឌី"</string>
    <!-- Menu item that hides internal storage built into the device [CHAR LIMIT=24] -->
    <string name="menu_advanced_hide" product="nosdcard">"លាក់​ឧបករណ៍​​ផ្ទុក​ខាងក្នុង"</string>
