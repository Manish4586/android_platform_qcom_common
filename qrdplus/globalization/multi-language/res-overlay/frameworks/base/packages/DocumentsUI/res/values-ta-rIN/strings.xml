    <string name="menu_advanced_show" product="default">"SD கார்டைக் காட்டு"</string>
    <!-- Menu item that hides internal storage built into the device [CHAR LIMIT=24] -->
    <string name="menu_advanced_hide" product="nosdcard">"அகச் சேமிப்பகத்தை மறை"</string>
