<resources>

    <!--
    customize the default value to play selection, default value is false,
    -->
    <bool name="def_music_play_selection_enabled">true</bool>

    <!--
    customize the default value to add menuitem, default value is false,
    true is for MicroMax
    -->
    <bool name="def_music_add_more_video_enabled">true</bool>

     <!--
    customize the default value to open music, default value is null,
    overlay key is for MicroMax like http://Music.mimicromax.com
    -->
    <string name="def_music_add_more_music" translatable="false">http://Music.mimicromax.com</string>

     <!--
    customize the default value to open music, default value is null,
    overlay key is for MicroMax like http://Videos.mimicromax.com
    -->
    <string name="def_music_add_more_video" translatable="false">http://Videos.mimicromax.com</string>

</resources>
