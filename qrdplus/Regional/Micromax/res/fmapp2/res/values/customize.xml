<resources>

    <!--
        customize the the country for fm,
        default value is false,true is for INDIA
    -->
    <bool name="def_fm_country_location_enabled">true</bool>

    <!--
        customize the the special carrier for fm,
        default value is false,true is for Micromax
    -->
    <bool name="def_fm_special_carrier_enabled">true</bool>

    <!--set the country for fm,default value is null,"INDIA" is for INDIA-->
    <string name="def_fm_country_location" translatable="false">INDIA</string>

    <!--customize the output list,true is stereo only,
        false is stereo and mono,default value is false,true is for India common-->
    <bool name="def_only_stereo_enabled">true</bool>

    <!-- set the name of audio type-->
    <string name="audio_type_mono">Speaker</string>

</resources>
