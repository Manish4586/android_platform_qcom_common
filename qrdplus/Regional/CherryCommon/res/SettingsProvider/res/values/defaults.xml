<resources>

  <!--  Alarm sounds play or not when low battery, 1 : play, other value will not paly, default value is 1 -->

  <integer name="def_power_sounds_enabled">1</integer>

  <!-- path of alarm sounds,default value is "/system/media/audio/ui/LowBattery.ogg" -->

  <string name="def_low_battery_sound" translatable="false">/system/media/audio/ui/LowBattery.ogg</string>

  <!-- NITZ (network time), use network provided time from NITZ or not, default value is true, other value is false -->

  <bool name="def_auto_time">true</bool>

  <!-- NITZ (network time), use network provided time zone from NITZ or not, default value is true, other value is false -->

  <bool name="def_auto_time_zone">false</bool>

  <!-- Screen Brightness, Default for brightness automatic, true will open the automatic mode, false will close the automatic mode, default value is false -->

  <bool name="def_screen_brightness_automatic_mode">true</bool>

  <!-- Keypad tone, Default for UI touch sounds enabled, it indicates whether phone has sound when user touch screen, default value is true -->

  <bool name="def_sound_effects_enabled">true</bool>

  <!-- default location allowed,the value may be "","gps","network","gps,network",default value is gps-->

  <string name="def_location_providers_allowed" translatable="false">gps,network</string>

  <!-- Auto rotation, true will auto rotation, false will close auto rotation, default value is true.-->

  <bool name="def_accelerometer_rotation">true</bool>

  <!-- Keep Wi-Fi on during sleep,0 == never, 1 == only when plugged in, 2 == always, default value is 2. -->

  <integer name="def_wifi_sleep_policy">2</integer>
</resources>
