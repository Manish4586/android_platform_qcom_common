<resources>
<!-- Time format, default vlaue is 24,other value is 12 format -->
<string name="def_time_format" translatable="false">24</string>

  <!-- Date format, default value is MM-dd-yyyy, yyyy-MM-dd: 2013/07/30; MM-dd-yyyy:07/30/2013; dd-MM-yyyy:30/07/2013 -->
  <string name="def_date_format" translatable="false">dd-MM-yyyy</string>
  <!-- Default Input Method, its value is from inputmethod's package name and main class name. Default value is "com.android.inputmethod.latin/.LatinIME"-->
  <string name="def_input_method" translatable="false">com.android.inputmethod.latin/.LatinIME</string>

  <!--custom system volume or not -->
  <bool name="def_custom_sys_volume">true</bool>

  <!-- Ringtone Volume, default value is 5, value is from 0 to 10 -->

  <integer name="def_ringtone_volume" translatable="false">5</integer>

  <!-- Default for Settings.Global.DATA_ROAMING -->

  <bool name="def_enable_data_roaming">true</bool>

  <!-- Default for Settings.Global.MOBILE_DATA -->

  <bool name="def_enable_mobile_data">true</bool>

  <!-- Default Enable InputMethods, its value is from inputmethod's package name and main class name
              such as "com.android.inputmethod.pinyin/.PinyinIME" ;This is a string
         containing the IDs of all enabled input methods, each ID separated by ':'.-->

  <string name="def_enable_input_methods" translatable="false">
com.android.inputmethod.latin/.LatinIME;-1337596075:com.google.android.inputmethod.latin/com.android.inputmethod.latin.LatinIME;-1337596075</string>

  <!-- Music Volume,its value is from 0 to 15,default value is 11 -->
<integer translatable="false" name="def_music_volume">5</integer>

  <!--enable accessiblity or not,1:enable;0:disable -->

  <integer name="def_enable_accessiblity" translatable="false">0</integer>

  <!--for enable accessiblity services,split by ":" ,example "com.google.android.marvin.talkback/com.google.android.marvin.talkback.TalkBackService"-->

  <string name="def_enable_accessiblity_services" translatable="false"/>
</resources>
