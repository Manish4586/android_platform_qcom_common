<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- the type of saved recordings files.The value is 0-amr , 1-3gpp , 2-wav.Default is 0-amr.-->
    <integer name="def_save_type">1</integer>

    <!-- the mimetype of saved recordings files.The value is "audio/amr","audio/3gpp","audio/wave_2ch_lpcm".Default is "audio/amr".-->
    <string name="def_save_mimetype">audio/3gpp</string>

    <!-- the prefix of recordings files name.File name like this "Rec-yyyy-MM-dd-HH-mm-ss.3gpp".If empty,use "FMRecording" as default prefix. -->
    <string name="def_save_name_prefix">Rec</string>

    <!-- the name format of recordings files-->
    <string name="def_save_name_format">yyyy-MM-dd-HH-mm-ss</string>
</resources>
