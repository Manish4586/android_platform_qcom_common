#
# ADDITIONAL_FEATURE_PROPERTIES FOR LATAM_OPEN_MAX SPEC
#
####################################################
#Set the current spec as LatamOpenAMX#
persist.env.spec=LatamOpenAMX

#set cellbroadcast enable or disable#
persist.env.cb.enable=true

#set system language#
persist.sys.language=es

#set country#
persist.sys.country=US

#for emergence call list(split by ',')
ril.ecclist=060,112,911

#set usb debugging mode, "1" means enable, "0" means disable.#
persist.service.adb.enable=1
