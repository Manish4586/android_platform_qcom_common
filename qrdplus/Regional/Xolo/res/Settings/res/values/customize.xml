<resources>
  <!-- set bluetooth device name -->
  <string name="def_custome_bt_defname" translatable="false">8909-Xolo</string>

  <!-- show country in language -->
  <bool name="def_language_country_enable">true</bool>

  <!-- Setting carrier ota enable -->
  <bool name="def_carrier_ota_enable">true</bool>

  <!-- Setting carrier ota update address -->
  <string name="def_carrier_ota_address">com.android.softwareupdateclient.LAVA_OTA_UPDATE</string>

  <!-- show OTA/System update in deviceinfo -->
  <bool name="def_device_info_ota_update_enable">true</bool>

  <!-- show regulatory info -->
  <bool name="def_regulatory_enable">true</bool>

  <!-- customize default sar value -->
  <string name="def_custome_sar_defvalue">SAR &lt; 1.6W/kg</string>

  <!-- Show regulatory info (from settings item or dialing "*#07#")-->
  <bool name="config_show_regulatory_info">true</bool>
</resources>
