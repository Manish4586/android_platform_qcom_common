#
# ADDITIONAL_FEATURE_PROPERTIES FOR LATAMBRAZIL SPEC
#
####################################################
#Set the current spec as LatamBrazil#
persist.env.spec=LatamBrazil

#set cellbroadcast enable or disable#
persist.env.cb.enable=true

#set system language#
persist.sys.language=pt

#set country#
persist.sys.country=BR

#for emergence call list(split by ',')
ril.ecclist=190,112,911

#set usb debugging mode, "1" means enable, "0" means disable.#
persist.service.adb.enable=1
