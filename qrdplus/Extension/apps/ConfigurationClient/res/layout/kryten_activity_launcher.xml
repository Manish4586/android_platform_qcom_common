<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:gravity="center"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/test_banner"/>
    <Button android:id="@+id/send_pdu_to_notifier_user_pin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/test_userpin"
        android:clickable="true"/>
    <Button android:id="@+id/straight_to_confirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/test_confirm"
        android:clickable="true"/>
    <Button android:id="@+id/straight_to_error"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/test_error"
        android:clickable="true"/>
</LinearLayout>
