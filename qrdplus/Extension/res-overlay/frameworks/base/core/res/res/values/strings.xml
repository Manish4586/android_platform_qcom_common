<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="permlab_accessWifiState">view WLAN connections</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessWifiState">Allows the app to view information
      about WLAN networking, such as whether WLAN is enabled and name of
      connected WLAN devices.</string>

    <string name="permlab_changeWifiState">connect and disconnect from WLAN</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changeWifiState">Allows the app to connect to and
      disconnect from WLAN access points and to make changes to device
      configuration for WLAN networks.</string>
    <string name="permlab_changeWifiMulticastState">allow WLAN Multicast reception</string>

    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changeWifiMulticastState" product="tablet">Allows the app to receive
      packets sent to all devices on a WLAN network using multicast addresses,
      not just your tablet.  It uses more power than the non-multicast mode.</string>
    <string name="permdesc_changeWifiMulticastState" product="default">Allows the app to receive
      packets sent to all devices on a WLAN network using multicast addresses,
      not just your phone.  It uses more power than the non-multicast mode.</string>

    <string name="wifi_available_sign_in">Sign into WLAN network</string>
    <string name="wifi_watchdog_network_disabled">Couldn\'t connect to WLAN</string>
    <string name="wifi_p2p_dialog_title">WLAN Direct</string>
    <string name="wifi_p2p_turnon_message">Start WLAN Direct. This will turn off WLAN client/hotspot.</string>
    <string name="wifi_p2p_failed_message">Couldn\'t start WLAN Direct.</string>
    <string name="wifi_p2p_enabled_notification_title">WLAN Direct is on</string>
    <string name="wifi_p2p_frequency_conflict_message" product="tablet">The tablet will temporarily disconnect from WLAN while it\'s connected to <xliff:g id="device_name">%1$s</xliff:g></string>
    <string name="wifi_p2p_frequency_conflict_message" product="default">The phone will temporarily disconnect from WLAN while it\'s connected to <xliff:g id="device_name">%1$s</xliff:g></string>
    <!-- Notification title when WLAN data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
    <string name="data_usage_wifi_limit_title">WLAN data is off</string>
    <!-- Notification title when WLAN data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <string name="data_usage_wifi_limit_snoozed_title">WLAN data limit exceeded</string>

    <string name="permlab_configureWifiDisplay">configure WLAN displays</string>
    <string name="permdesc_configureWifiDisplay">Allows the app to configure and connect to WLAN displays.</string>
    <string name="permlab_controlWifiDisplay">control WLAN displays</string>
    <string name="permdesc_controlWifiDisplay">Allows the app to control low-level features of WLAN displays.</string>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's title. -->
    <plurals name="wifi_available">
        <item quantity="one">WLAN network available</item>
        <item quantity="other">WLAN networks available</item>
    </plurals>

    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's message. -->
    <plurals name="wifi_available_detailed">
        <item quantity="one">Open WLAN network available</item>
        <item quantity="other">Open WLAN networks available</item>
    </plurals>
</resources>
