<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView01" android:layout_width="fill_parent"
    android:layout_height="fill_parent" android:scrollbars="vertical">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">


        <!-- Settings for VT -->
         <TextView
      android:text="@string/vt_setting"
      android:textSize="25sp"
      android:textColor="#ffffff"
      android:background="#717171"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"/>

<!--  Sub Section: VT Enable/Disable-->
  <LinearLayout
      android:id="@+id/checkbox_layout"
      android:orientation="vertical"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">

      <CheckBox  android:id="@+id/vt_checkbox"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:textSize="15dip"
          android:text="@string/vt_settings_text" />

      <CheckBox  android:id="@+id/ft_checkbox"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:textSize="15dip"
          android:text="@string/ft_settings_text" />

      <CheckBox  android:id="@+id/chat_checkbox"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:textSize="15dip"
          android:text="@string/chat_settings_text" />

      <CheckBox  android:id="@+id/presence_checkbox"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:textSize="15dip"
          android:text="@string/app_name" />

  </LinearLayout>

  <TextView
      android:id="@+id/data_textview_title"
      android:text="@string/data_status"
      android:textSize="25sp"
      android:textColor="#ffffff"
      android:background="#717171"
      android:layout_width="fill_parent"
      android:layout_height="wrap_content"
      android:layout_weight="1"/>

        <TextView
          android:id="@+id/data_textview"
          android:text="@string/data_on_off_status"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          android:paddingTop="10dip"
          android:layout_weight="1"/>


<!--  Section Heading: Notify Settings-->
  <TextView
      android:text="@string/notify_settings_text"
      android:textSize="25sp"
      android:textColor="#ffffff"
      android:background="#717171"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"/>

<!--  Sub Section: Notify format-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/set_notify_fmt_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:paddingTop="10dip"
          android:layout_weight="1"/>
      <Spinner
          android:id="@+id/notify_fmt_Spinner"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"
          android:entries="@array/notify_fmt_list"/>
  </LinearLayout>

  <!--  Buttons layout for Notify Format -->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <Button
          android:id="@+id/set_notify_fmt_button"
          android:text="@string/set_text"
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          android:layout_weight="1"/>
      <Button
          android:id="@+id/get_notify_current_fmt_button"
          android:text="@string/get_text"
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          android:layout_weight="1"/>
  </LinearLayout>

<!--  Section Heading: Event Reporting Settings-->
