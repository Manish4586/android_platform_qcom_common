<!--  Section Heading: Timer Settings-->
  <TextView
      android:text="@string/timer_settings_text"
      android:textSize="25sp"
      android:textColor="#ffffff"
      android:background="#717171"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"/>

<!--  Sub Section: AVAILABILITY_CACHE_EXPIRATION-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/availability_cache_exp_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"/>
      <EditText
          android:id="@+id/availability_cache_exp_value"
          android:layout_height="fill_parent"
          android:layout_width="fill_parent"
          android:inputType="numberDecimal"
          android:layout_weight="1" android:lines="1"/>

  </LinearLayout>

  <!--  Sub Section: CAPABILITIES_CACHE_EXPIRATION-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/capability_cache_exp_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"/>
      <EditText
          android:id="@+id/capability_cache_exp_value"
          android:layout_height="fill_parent"
          android:layout_width="fill_parent"
          android:inputType="numberDecimal"
          android:layout_weight="1" android:lines="1"/>

  </LinearLayout>

  <!--  Sub Section: CAPABILITY_POLL_INTERVAL-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/capability_poll_interval_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"/>
      <EditText
          android:id="@+id/capability_poll_interval_value"
          android:layout_height="fill_parent"
          android:layout_width="fill_parent"
          android:inputType="numberDecimal"
          android:layout_weight="1" android:lines="1"/>

  </LinearLayout>

  <!--  Sub Section: CAPABILITY_POLL_LIST_SUBSCRIPTION_EXPIRATION-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/capability_poll_list_exp_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"/>
      <EditText
          android:id="@+id/capability_poll_list_exp_value"
          android:layout_height="fill_parent"
          android:layout_width="fill_parent"
          android:inputType="numberDecimal"
          android:layout_weight="1" android:lines="1"/>

  </LinearLayout>

  <!--  Sub Section: SOURCE_THROTTLE_PUBLISH-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/source_throttle_publish_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"/>
      <EditText
          android:id="@+id/source_throttle_publish_value"
          android:layout_height="fill_parent"
          android:layout_width="fill_parent"
          android:inputType="numberDecimal"
          android:layout_weight="1" android:lines="1"/>
  </LinearLayout>

<!--  Section Heading: Retry Settings-->
  <TextView
      android:text="@string/retry_info_text"
      android:textSize="25sp"
      android:textColor="#ffffff"
      android:background="#717171"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"/>

  <!--  Sub Section: Subscribe retry-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/subscribe_retry_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"/>
      <EditText
          android:id="@+id/subscribe_retry_value"
          android:layout_height="fill_parent"
          android:layout_width="fill_parent"
          android:inputType="numberDecimal"
          android:layout_weight="1" android:lines="1"/>
  </LinearLayout>

  <!--  Sub Section: Publish retry-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/publish_retry_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"/>
      <EditText
          android:id="@+id/publish_retry_value"
          android:layout_height="fill_parent"
          android:layout_width="fill_parent"
          android:inputType="numberDecimal"
          android:layout_weight="1" android:lines="1"/>
  </LinearLayout>

<!--  Section Heading: List subscription settings-->
  <TextView
      android:text="@string/list_subscription_settings_text"
      android:textSize="25sp"
      android:textColor="#ffffff"
      android:background="#717171"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1"/>


  <!--  Sub Section: Max entry in list subscription-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <TextView
          android:text="@string/max_list_entries_text"
          android:textSize="15dip"
          android:layout_width="fill_parent"
          android:layout_height="fill_parent"
          android:layout_weight="1"/>
      <EditText
          android:id="@+id/max_list_entries_value"
          android:layout_height="fill_parent"
          android:layout_width="fill_parent"
          android:inputType="numberDecimal"
          android:layout_weight="1" android:lines="1"/>
  </LinearLayout>

  <!--  Buttons layout-->
  <LinearLayout
      android:orientation="horizontal"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent"
      android:layout_weight="1">
      <Button
          android:id="@+id/ok"
          android:text="@string/oktext"
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          android:layout_weight="1"/>
      <Button
          android:id="@+id/cancel"
          android:text="@string/canceltext"
          android:layout_width="fill_parent"
          android:layout_height="wrap_content"
          android:layout_weight="1"/>
  </LinearLayout>
</LinearLayout>
</ScrollView>
